        private void btn_FinalizarCompra_Click(object sender, EventArgs e)
        {
            if (txt_NomeFantasia.Text != "")
            {
                if (txt_CNPJ.Text != "")
                {
                    if (txt_CodigodoProduto.Text != "")
                    {
                        if (txt_NomedoProduto.Text != "")
                        {
                            if (txt_PrecoSomente.Text != "")
                            {
                                if (txt_Massa.Text != "")
                                {
                                    if (Decimal.Parse(txt_ValordaCompra.Text) > (Decimal.Parse(txt_ValorRecebido.Text)))
                                    {
                                        MessageBox.Show("Valor recebido insuficiente.");
                                        return;
                                    }

                                    if (cbb_FormadePagamento.SelectedIndex == -1)
                                    {
                                        MessageBox.Show("Escolha uma forma de pagamento.");
                                        return;

                                    }
                                    else
                                    {
                                        for (int i = 0; i < dgv_CaixaTemporario.Rows.Count; i++)
                                        {
                                            if (dgv_CaixaTemporario.Rows.Count > 0)
                                            {
                                                Criptografia cryptography = new Criptografia(System.Configuration.ConfigurationManager.AppSettings[""]);
                                                string CNPJCrip = cryptography.Encriptar(txt_CNPJ.Text);
                                                string CodProdCrip = cryptography.Encriptar(txt_CodigodoProduto.Text);
                                                string NomedoProdutoPrecoQuiloCrip = (this.dgv_CaixaTemporario.Rows[i].Cells[3].Value?.ToString());
                                                string TipodePagamento = cryptography.Encriptar(cbb_FormadePagamento.Text);
                                                string ValorRecebidoCrip = cryptography.Encriptar(txt_ValorRecebido.Text);
                                                string TrocoCrip = cryptography.Encriptar(txt_Troco.Text);
                                                string UnidadeCrip = cryptography.Encriptar(lbl_AssociacaoCaixa.Text);
                                                string UsuarioCrip = cryptography.Encriptar(lbl_Usuario.Text);
                                                string MassaCrip = cryptography.Encriptar(txt_Massa.Text);
                                                string DataHoraCrip = DateTime.Now.ToString();

                                                VendasClasse Vendas = new VendasClasse();
                                                Vendas.NomeFantasia = txt_NomeFantasia.Text;
                                                Vendas.CNPJ = CNPJCrip;
                                                Vendas.CodigodoProduto = CodProdCrip;
                                                Vendas.NomedoProdutoPrecoQuilo = NomedoProdutoPrecoQuiloCrip;
                                                Vendas.ValordaCompra = txt_ValordaCompra.Text;
                                                Vendas.TipodePagamento = TipodePagamento;
                                                Vendas.ValorRecebido = ValorRecebidoCrip;
                                                Vendas.Troco = TrocoCrip;
                                                Vendas.Associacao = UnidadeCrip;
                                                Vendas.Usuario = UsuarioCrip;
                                                Vendas.Massa = MassaCrip;
                                                Vendas.DataHora = DataHoraCrip;
                                                Vendas.Data = DateTime.Now.ToShortDateString();
                                                Vendas.valorproduto = Decimal.Parse(this.dgv_CaixaTemporario.Rows[i].Cells[6].Value?.ToString());
                                                Vendas.Nomedoservidor = lbl_NomedoServidor.Text;

                                                ConexaoBancodeDados.NovaVenda(Vendas);

                                                this.Close();

                                            }


                                            else
                                            {
                                                    MessageBox.Show("Nenhum produto registrado.");
                                                    return;
                                                
                                            }
                                        }

                                        if(dgv_CaixaTemporario.Rows.Count != 0)
                                        {
                                            MessageBox.Show("Compra realizada com sucesso!");
                                            var Recibo = MessageBox.Show("Imprimir recibo?", "Recibo", MessageBoxButtons.YesNo);
                                            if (Recibo == DialogResult.Yes)
                                            {
                                                try
                                                {

                                                    string compradata = DateTime.Now.ToString();
                                                    string output = string.Concat(compradata.Where(Char.IsDigit));
                                                    string cliente = txt_NomeFantasia.Text;
                                                    string troco = txt_Troco.Text;
                                                    string total = txt_ValordaCompra.Text;
                                                    string recebido = txt_ValorRecebido.Text;
                                                    string forma = cbb_FormadePagamento.Text;
                                                    string nomeserivdor = lbl_NomedoServidor.Text;
                                                    string nomeArquivo = Globais.caminhoImpressao + @"\RECIBO_" + cliente + "_" + output + ".pdf";
                                                    FileStream arquivoPDF = new FileStream(nomeArquivo, FileMode.Create);

                                                    string nomeassociacao = lbl_AssociacaoCaixa.Text;
                                                    Document doc = new Document(PageSize.A4.Rotate(), 30f, 0f, 150f, 150f);
                                                    doc.SetPageSize(doc.PageSize);
                                                    PdfWriter escritorPDF = PdfWriter.GetInstance(doc, arquivoPDF);
                                                    escritorPDF.PageEvent = new ReciboImprmircs(cliente, troco, recebido, total, forma, nomeassociacao, nomeserivdor);
                                                    doc.Open();


                                                    string dados = "";

                                                    Criptografia cryptography = new Criptografia(System.Configuration.ConfigurationManager.AppSettings[""]);

                                                    string NomedoServidor = lbl_NomedoServidor.Text;
                                                    SqlConnection Conectar = new SqlConnection("Data Source=DESKTOP-" + NomedoServidor + "\\SQLEXPRESS;Initial Catalog=SGRRC1;Integrated Security=True");
                                                    Conectar.Open();
                                                    string AdministradorLogin = cryptography.Encriptar(lbl_Usuario.Text);
                                                    SqlCommand cmd = new SqlCommand("select [txt_NomeAdministrador] from [ADMINISTRADORES] WHERE [txt_AdministradorLogin] = '" + AdministradorLogin + "'", Conectar);
                                                    var NomeAdministrador = cmd.ExecuteScalar();
                                                    string AdministradorNome = NomeAdministrador.ToString();

                                                    try
                                                    {
                                                        SqlCommand cmd2 = new SqlCommand("select [EnderecoImagem] from [ASSOCIACAO] WHERE [Nome Fantasia] = '" + lbl_AssociacaoCaixa.Text + "'", Conectar);
                                                        var EnderecoImagem = cmd2.ExecuteScalar();
                                                        string Endereco = EnderecoImagem.ToString();

                                                        iTextSharp.text.Image LogotipoAssociacao = iTextSharp.text.Image.GetInstance(Endereco);
                                                        LogotipoAssociacao.ScaleToFit(140f, 120f);
                                                        LogotipoAssociacao.SetAbsolutePosition(650f, 475);
                                                        doc.Add(LogotipoAssociacao);
                                                        Conectar.Close();
                                                    }
                                                    catch
                                                    {

                                                        iTextSharp.text.Image LogotipoAssociacao = iTextSharp.text.Image.GetInstance(Globais.caminho + @"\BRANCO.png");
                                                        LogotipoAssociacao.ScaleToFit(140f, 120f);
                                                        LogotipoAssociacao.SetAbsolutePosition(650f, 480);
                                                        doc.Add(LogotipoAssociacao);
                                                    }

                                                    Conectar.Close();

                                                    //ColumnText ct = new ColumnText(escritorPDF.DirectContent);
                                                    //ct.SetSimpleColumn(new iTextSharp.text.Rectangle(0, 500, 523, 0));
                                                    //ct.AddElement(new Paragraph("Eu" + AdministradorNome + "Acuso recebimento de " + total + " de " + txt_NomeFantasia.Text + ")"));
                                                    //ct.Go();

                                                    PdfPTable tabela1 = new PdfPTable(3);
                                                    tabela1.WidthPercentage = 50;
                                                    tabela1.SpacingBefore = 50;
                                                    tabela1.AddCell("Material");
                                                    tabela1.AddCell("Massa (kg)");
                                                    tabela1.AddCell("Valor (R$)");

                                                    string UsuarioCrip = cryptography.Encriptar(lbl_Usuario.Text);

                                                    string vqueryReciboImpressao = @"SELECT [Material], [Massa], [Valor] FROM [CaixaTemporario] WHERE [Usuario] ='" + UsuarioCrip + "' ORDER BY [Material] ASC";

                                                    string Nomedoservidor = lbl_NomedoServidor.Text;
                                                    DataTable dtReciboImpressao = ConexaoBancodeDados.dql(vqueryReciboImpressao, Nomedoservidor);
                                                    for (int i2 = 0; i2 < dtReciboImpressao.Rows.Count; i2++)
                                                    {
                                                        tabela1.AddCell(new PdfPCell()).AddElement(new Paragraph(dtReciboImpressao.Rows[i2].Field<string>("Material"), new iTextSharp.text.Font(iTextSharp.text.Font.NORMAL, 10, (int)System.Drawing.FontStyle.Bold)));
                                                        tabela1.AddCell(new PdfPCell()).AddElement(new Paragraph(dtReciboImpressao.Rows[i2].Field<Decimal>("Massa").ToString(), new iTextSharp.text.Font(iTextSharp.text.Font.NORMAL, 10, (int)System.Drawing.FontStyle.Bold)));
                                                        tabela1.AddCell(new PdfPCell()).AddElement(new Paragraph(dtReciboImpressao.Rows[i2].Field<Decimal>("Valor").ToString(), new iTextSharp.text.Font(iTextSharp.text.Font.NORMAL, 10, (int)System.Drawing.FontStyle.Bold)));
                                                    }

                                                    PdfPTable Recibo1 = new PdfPTable(1);
                                                    Recibo1.TotalWidth = doc.PageSize.Width - doc.LeftMargin - doc.RightMargin;
                                                    Recibo1.SpacingAfter = 20;
                                                    PdfPCell CelulaRecibo1 = new PdfPCell((new Paragraph("Eu, " + AdministradorNome + " acuso recebimento de R$" + total + " de " + txt_NomeFantasia.Text + "," +
                                                        "")));
                                                    CelulaRecibo1.HorizontalAlignment = Element.ALIGN_CENTER;
                                                    CelulaRecibo1.Border = 0;
                                                    Recibo1.AddCell(CelulaRecibo1);
                                                    Recibo1.WriteSelectedRows(0, -1, doc.LeftMargin, escritorPDF.PageSize.GetBottom(doc.BottomMargin) + 345, escritorPDF.DirectContent);

                                                    PdfPTable Recibo2 = new PdfPTable(1);
                                                    Recibo2.TotalWidth = doc.PageSize.Width - doc.LeftMargin - doc.RightMargin;
                                                    Recibo2.SpacingAfter = 50;
                                                    PdfPCell CelulaRecibo2 = new PdfPCell((new Paragraph(" CNPJ " + txt_CNPJ.Text + " referente à venda de")));
                                                    CelulaRecibo2.HorizontalAlignment = Element.ALIGN_CENTER;
                                                    CelulaRecibo2.Border = 0;
                                                    Recibo2.AddCell(CelulaRecibo2);
                                                    Recibo2.WriteSelectedRows(0, -1, doc.LeftMargin, escritorPDF.PageSize.GetBottom(doc.BottomMargin) + 325, escritorPDF.DirectContent);



                                                    doc.Add(tabela1);
                                                    doc.Close();

                                                    string vqueryLimparCaixa = @"DELETE FROM CaixaTemporario WHERE [Unidade] = '" + lbl_AssociacaoCaixa.Text + "' AND [Usuario] ='" + lbl_Usuario.Text + "'";
                                                    ConexaoBancodeDados.dql(vqueryLimparCaixa, Nomedoservidor);

                                                    System.Diagnostics.Process.Start(Globais.caminhoImpressao + @"\RECIBO_" + cliente + "_" + output + ".pdf");
                                                }
                                                catch
                                                {
                                                    MessageBox.Show("Pasta 'Impressão' não encontrada");
                                                    return;

                                                }
                                            }
                                            else
                                            {
                                                string Nomedoservidor = lbl_NomedoServidor.Text;
                                                string vqueryLimparCaixa = @"DELETE FROM CaixaTemporario WHERE [Unidade] = '" + lbl_AssociacaoCaixa.Text + "' AND [Usuario] ='" + lbl_Usuario.Text + "'";
                                                ConexaoBancodeDados.dql(vqueryLimparCaixa, Nomedoservidor);
                                            }


                                        }
                                        else
                                        {
                                            MessageBox.Show("Nenhum produto selecionado.");
                                            return;

                                        }
                                      
                                    }





                                }
                                else
                                {

                                    MessageBox.Show("Digite um valor de massa.");
                                    return;

                                }
                            }
                            else
                            {
                                MessageBox.Show("Digite um código de produto.");
                                return;

                            }


                        }
                        else
                        {
                            MessageBox.Show("Digite um código de produto.");
                            return;
                        }



                    }
                    else
                    {
                        MessageBox.Show("Digite um código de produto.");
                        return;
                    }


                }
                else
                {
                    MessageBox.Show("Digite um CNPJ.");
                    return;

                }


            }
            else
            {
                MessageBox.Show("Digite um CNPJ.");
                return;

            }

        }      

        private void cbb_FormadePagamento_SelectedIndexChanged(object sender, EventArgs e)
        {
            if(cbb_FormadePagamento.SelectedIndex == 4)
            {
                txt_ValorRecebido.ReadOnly = false;

            }
            else
            {
                txt_ValorRecebido.ReadOnly = true;
                txt_ValorRecebido.Text = txt_ValordaCompra.Text;


            }
        }

        private void button1_Click(object sender, EventArgs e)
        {

            if (dgv_CaixaTemporario.Rows.Count == 0)
            {
                MessageBox.Show("Produto não selecionado.");
                return;

            }
            else
            {

                Caixa caixa = new Caixa();
                Criptografia cryptography = new Criptografia(System.Configuration.ConfigurationManager.AppSettings[""]);
                string UnidadeCrip = cryptography.Encriptar(lbl_AssociacaoCaixa.Text);
                string UsuarioCrip = cryptography.Encriptar(lbl_Usuario.Text);
                var IDCompras = this.dgv_CaixaTemporario.CurrentRow.Cells[0].Value;
                string IDCompra = IDCompras.ToString();
                string ExcluirItem = @"DELETE FROM CaixaTemporario WHERE [Unidade] = '" + UnidadeCrip + "' AND [Usuario] = '" +UsuarioCrip +"' AND ID_Compra = '"+IDCompra+"'";
                string Nomedoservidor = lbl_NomedoServidor.Text;
                ConexaoBancodeDados.dql(ExcluirItem, Nomedoservidor);

                string vqueryCaixa = @"SELECT [ID_Compra], [Material], [Preco], [Massa], [Valor] FROM CaixaTemporario";
                dgv_CaixaTemporario.DataSource = ConexaoBancodeDados.dql(vqueryCaixa, lbl_NomedoServidor.Text);

            }
        }

        private void button3_Click(object sender, EventArgs e)
        {
            string Nomedoservidor = lbl_NomedoServidor.Text;
            string ExcluirItem = @"DELETE FROM CaixaTemporario WHERE [Unidade] = '" + lbl_AssociacaoCaixa.Text + "' AND [Usuario] ='" + lbl_Usuario.Text + "'";
            ConexaoBancodeDados.dql(ExcluirItem, Nomedoservidor);
            this.Close();
        }
    }
}
