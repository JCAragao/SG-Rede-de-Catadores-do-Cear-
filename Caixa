        private void txt_CNPJ_KeyDown(object sender, KeyEventArgs e)
        {

        }

        private void txt_CNPJ_KeyPress(object sender, KeyPressEventArgs e)
        {
            if (!char.IsNumber(e.KeyChar) && !char.IsControl(e.KeyChar))
            {
                e.Handled = true; 
            }

            if (Convert.ToInt32(e.KeyChar) == 13)
            {
                e.Handled = true;


                string NomedoServidor = lbl_NomedoServidor.Text;
                SqlConnection Conectar = new SqlConnection("Data Source=DESKTOP-" + NomedoServidor + "\\SQLEXPRESS;Initial Catalog=SGRRC1;Integrated Security=True");

                Conectar.Open();
                
                Criptografia cryptography = new Criptografia(System.Configuration.ConfigurationManager.AppSettings[""]);

                string CNPJ = cryptography.Encriptar(txt_CNPJ.Text);
                SqlCommand cmd = new SqlCommand("select [Nome Fantasia] from CLIENTES WHERE [CNPJ] = '" + CNPJ + "'", Conectar);
                var NomeFantasia = cmd.ExecuteScalar();

                if (NomeFantasia != null)
                {

                    txt_NomeFantasia.Text = NomeFantasia.ToString();
                    Conectar.Close();
                }
                else
                {
                    MessageBox.Show("Cliente não encontrado.");
                    Conectar.Close();
                    return;
                    
                }
            }


        }

        private void txt_CodigodoProduto_KeyPress(object sender, KeyPressEventArgs e)
        {

            if ((!char.IsNumber(e.KeyChar)) && !char.IsControl(e.KeyChar))
            {
                e.Handled = true;

            }
            else
            {
                e.Handled = false;

            }

            if (Convert.ToInt32(e.KeyChar) == 13 && txt_CodigodoProduto.Text != "")
            {
                e.Handled = true;

                string NomedoServidor = lbl_NomedoServidor.Text;
                SqlConnection Conectar = new SqlConnection("Data Source=DESKTOP-" + NomedoServidor + "\\SQLEXPRESS;Initial Catalog=SGRRC1;Integrated Security=True");
                Conectar.Open();
                string Material = txt_CodigodoProduto.Text;
                SqlCommand cmd = new SqlCommand("select [Material] from METERIAIS WHERE [ID_Material] = '" + Material + "'", Conectar);
                SqlCommand cmd2 = new SqlCommand("select [Preco] from METERIAIS WHERE [ID_Material] = '" + Material + "'", Conectar);

                var MaterialNome = cmd.ExecuteScalar();
                var Preco = cmd2.ExecuteScalar();
                if (Material != null && Preco != null)
                {
                    try
                    {
                       
                        txt_NomedoProduto.Text = MaterialNome.ToString();
                        txt_PrecoSomente.Text = Preco.ToString();
                        Conectar.Close();
                    }
                    catch
                    {
                        MessageBox.Show("Material não encontrado.");
                        return;

                    }
                }
                else
                {
                    MessageBox.Show("Material não encontrado.");
                    return;
                }
             

            }

        }

        private void txt_Massa_KeyPress(object sender, KeyPressEventArgs e)
        {
            if ((!char.IsNumber(e.KeyChar)) && !char.IsControl(e.KeyChar) && e.KeyChar != '.' && e.KeyChar != ',')
            {
                e.Handled = true;

            }
            else
            {
                e.Handled = false;
            }

            if(Convert.ToInt32(e.KeyChar) == 13)
            {
                if (txt_NomedoProduto.Text != "")
                {
                    if (txt_NomeFantasia.Text != "")
                    {
                        if (txt_Massa.Text != "")
                        {
                            e.Handled = true;

                            Criptografia cryptography = new Criptografia(System.Configuration.ConfigurationManager.AppSettings[""]);
                        
                            string UnidadeCrip = cryptography.Encriptar(lbl_AssociacaoCaixa.Text);
                            string UsuarioCrip = cryptography.Encriptar(lbl_Usuario.Text);

                            CaixaTemporarioClasse CaixaTemporario = new CaixaTemporarioClasse();
                            CaixaTemporario.Material = txt_NomedoProduto.Text;
                            CaixaTemporario.Massa = Decimal.Parse(txt_Massa.Text);
                            CaixaTemporario.Preco = (Decimal.Parse(cryptography.Decriptar(txt_PrecoSomente.Text)));
                            CaixaTemporario.Valor = (Decimal.Parse((txt_Massa.Text)) * (Decimal.Parse(cryptography.Decriptar(txt_PrecoSomente.Text)))); ;
                            CaixaTemporario.Unidade = UnidadeCrip;
                            CaixaTemporario.Usuario = UsuarioCrip;
                            CaixaTemporario.NomedoServidor = lbl_NomedoServidor.Text;
                            ConexaoBancodeDados.CaixaTemporario(CaixaTemporario);

                            //this.caixaTemporarioTableAdapter.Fill(this.sGRRC1DataSetALFA.CaixaTemporario);

                            string vqueryCaixa = @"SELECT [ID_Compra], [Material], [Preco], [Massa], [Valor] FROM CaixaTemporario";
                            dgv_CaixaTemporario.DataSource = ConexaoBancodeDados.dql(vqueryCaixa, lbl_NomedoServidor.Text);
                           

                            decimal TotalValor = 0;
                            for (int i = 0; i < dgv_CaixaTemporario.Rows.Count; ++i)
                            {
                                TotalValor += Convert.ToDecimal(dgv_CaixaTemporario.Rows[i].Cells[6].Value);
                            }
                            txt_ValordaCompra.Text = TotalValor.ToString();
                        }
                        else
                        {
                            MessageBox.Show("Digite um valor de massa.");
                            return;

                        }
                    }
                    else
                    {
                        MessageBox.Show("Digite um CNPJ válido e aperte ENTRA.");
                        return;

                    }

                }
                else
                {
                    
                    MessageBox.Show("Digite um código de produto válido e aperte ENTRA.");
                    return;
                }
            }
        }

        private void txt_ValorRecebido_TabStopChanged(object sender, EventArgs e)
        {

        }

        private void txt_ValorRecebido_KeyPress(object sender, KeyPressEventArgs e)
        {
            if ((!char.IsNumber(e.KeyChar)) && !char.IsControl(e.KeyChar) && Convert.ToInt32(e.KeyChar) == 13)
            {
                e.Handled = true;
                txt_ValorRecebido.Text = txt_ValordaCompra.Text;


            }
            else
            {
                e.Handled = false;

            }

            if (cbb_FormadePagamento.SelectedIndex == 4 && e.KeyChar == 13)
            {
                if(Decimal.Parse(txt_ValordaCompra.Text) > (Decimal.Parse(txt_ValorRecebido.Text)))
                {
                    MessageBox.Show("Valor recebido insuficiente.");
                    return;
                }
                txt_Troco.Text = (Decimal.Parse(txt_ValorRecebido.Text) - Decimal.Parse(txt_ValordaCompra.Text)).ToString();


            }
        }

