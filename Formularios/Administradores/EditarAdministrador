        //Revelar senha.
        private void chb_MostrarSenhaAdicionarAdministrador_CheckedChanged(object sender, EventArgs e)
        {
            if (chb_MostrarSenhaAdicionarAdministrador.Checked == true)
            {
                txt_SenhaEditarAdministrador.UseSystemPasswordChar = false;
                txt_ConfirmarsenhaEditarAdministrador.UseSystemPasswordChar = false;

            }

            else
            {
                txt_SenhaEditarAdministrador.UseSystemPasswordChar = true;
                txt_ConfirmarsenhaEditarAdministrador.UseSystemPasswordChar = true;
            }
        }

        //Numeric Up and Down 0
        private void nud_0AdicionarAdministrador_ValueChanged(object sender, EventArgs e)
        {
            cbb_NiveldeAcessoEditarAdministrador.Items.Clear();
            cbb_NiveldeAcessoEditarAdministrador.Items.Add(nud_0EditarAdministrador.Value);
            cbb_NiveldeAcessoEditarAdministrador.SelectedIndex = 0;
        }

        //Fechar o formulário.
        private void btn_CancelarCadastrarCatador_Click(object sender, EventArgs e)
        {
            this.Close();

        }

        private void EditarAdministrador_Load(object sender, EventArgs e)
        {

            //Carregamento combobox cidades
            string Servnome = lbl_NomedoServidor.Text;
            string vqueryCidade = @"SELECT * FROM CIDADES ORDER BY ID_Cidade ASC ;";
            cbb_CidadeEditarAdministrador.Items.Clear();
            cbb_CidadeEditarAdministrador.DataSource = ConexaoBancodeDados.dql(vqueryCidade, Servnome);
            cbb_CidadeEditarAdministrador.DisplayMember = "Cidade";
            cbb_CidadeEditarAdministrador.ValueMember = "ID_Cidade";

            //Adequação dos Numeric Up and Down
            if (Globais.nivel == 0)
            {
                nud_0EditarAdministrador.Visible = true;
                nud_1EditarAdministrador.Visible = false;
            }

            if (Globais.nivel == 1)
            {
                nud_1EditarAdministrador.Visible = true;
                nud_0EditarAdministrador.Visible = false;
                cbb_NiveldeAcessoEditarAdministrador.Items.Clear();
                cbb_NiveldeAcessoEditarAdministrador.Items.Add(nud_1EditarAdministrador.Value);
                cbb_NiveldeAcessoEditarAdministrador.SelectedIndex = 0;
            }
        }

        //Salvar alterações.
        private void btn_SalvarAlteracoesEditarCatador_Click(object sender, EventArgs e)
        {
            string format = "dd/MM/yyyy";
            string DatadeNascimento = txt_DatadeNascimentoEditarAdministrador.Text;
            DateTime dateTime;
            if (DateTime.TryParseExact(DatadeNascimento, format, CultureInfo.InvariantCulture, DateTimeStyles.AllowWhiteSpaces, out dateTime))
            {

                string DatadaMatricula = txt_DatadeMatriculaEditarAdministrador.Text;
                if (DateTime.TryParseExact(DatadaMatricula, format, CultureInfo.InvariantCulture, DateTimeStyles.AllowWhiteSpaces, out dateTime))
                {

                    if (txt_NomeCompletoEditarAdministrador.Text != "")
                    {
                        if (txt_DatadeNascimentoEditarAdministrador.Text != "")
                        {

                            if (txt_DatadeMatriculaEditarAdministrador.Text != "")
                            {
                                if (cbb_GeneroEditarAdministrador.SelectedIndex != -1)
                                {
                                    if (txt_RGEditarAdministrador.Text != "")
                                    {
                                        if (txt_CPFEditarAdministrador.Text != "")
                                        {
                                            if (txt_NISEditarAdministrador.Text != "")
                                            {
                                                if (txt_CEPEditarAdministrador.Text != "")
                                                {
                                                    if (cbb_CidadeEditarAdministrador.SelectedIndex != -1)
                                                    {
                                                        if (txt_TelefoneEditarAdministrador.Text != "")
                                                        {
                                                            if (txt_BairroEditarAdministrador.Text != "")
                                                            {
                                                                if (txt_RuaEditarAdministrador.Text != "")
                                                                {
                                                                    if (txt_emailEditarAdministrador.Text != "")
                                                                    {
                                                                        if (txt_NomedeUsuarioEditarAdministrador.Text != "")
                                                                        {
                                                                            if (txt_SenhaEditarAdministrador.Text != "")
                                                                            {
                                                                                if (txt_ConfirmarsenhaEditarAdministrador.Text != "")
                                                                                {
                                                                                    if (txt_SenhaEditarAdministrador.Text == txt_ConfirmarsenhaEditarAdministrador.Text)
                                                                                    {
                                                                                        var pergunta = MessageBox.Show("Será necessário encerrar a sessão. Deseja continuar?", "Encerrar sessão", MessageBoxButtons.YesNo);

                                                                                            if (pergunta == DialogResult.Yes)
                                                                                        {
                                                                                            Criptografia cryptography = new Criptografia(System.Configuration.ConfigurationManager.AppSettings[""]);
                                                                                            string BairroCrip = cryptography.Encriptar(txt_BairroEditarAdministrador.Text);
                                                                                            string CEPCrip = cryptography.Encriptar(txt_CEPEditarAdministrador.Text);
                                                                                            string CidadeCrip = cryptography.Encriptar(cbb_CidadeEditarAdministrador.Text);
                                                                                            string CPFCrip = cryptography.Encriptar(txt_CPFEditarAdministrador.Text);
                                                                                            string DatadeMatriculaCrip = cryptography.Encriptar(txt_DatadeMatriculaEditarAdministrador.Text);
                                                                                            string DatadeNascimentoCrip = cryptography.Encriptar(txt_DatadeNascimentoEditarAdministrador.Text);
                                                                                            string emailCrip = cryptography.Encriptar(txt_emailEditarAdministrador.Text);
                                                                                            string RuaCrip = cryptography.Encriptar(txt_RuaEditarAdministrador.Text);
                                                                                            string GeneroCrip = cryptography.Encriptar(cbb_GeneroEditarAdministrador.Text);
                                                                                            string LoginCrip = cryptography.Encriptar(txt_NomedeUsuarioEditarAdministrador.Text);
                                                                                            string NISCrip = cryptography.Encriptar(txt_NISEditarAdministrador.Text);
                                                                                            string RGCrip = cryptography.Encriptar(txt_RGEditarAdministrador.Text);
                                                                                            string TelefoneCrip = cryptography.Encriptar(txt_TelefoneEditarAdministrador.Text);
                                                                                            string SenhaCrip = cryptography.Encriptar(txt_SenhaEditarAdministrador.Text);


                                                                                            EditarAdministradorClasse AdministradorAlterado = new EditarAdministradorClasse();
                                                                                            AdministradorAlterado.ID_AdministradorEditarAdministrador = txt_MatriculaAdministradorEditar.Text;
                                                                                            AdministradorAlterado.NomeAdministradorEditarAdministrador = txt_NomeCompletoEditarAdministrador.Text;
                                                                                            AdministradorAlterado.CidadeEditarAdministrador = CidadeCrip;
                                                                                            AdministradorAlterado.BairroEditarAdministrador = BairroCrip;
                                                                                            AdministradorAlterado.EnderecoEditarAdministrador = RuaCrip;
                                                                                            AdministradorAlterado.DatadeNascimentoEditarAdministrador = DatadeNascimentoCrip;
                                                                                            AdministradorAlterado.DatadeMatriculaEditarAdministrador = DatadeMatriculaCrip;
                                                                                            AdministradorAlterado.RGEditarAdministrador = RGCrip;
                                                                                            AdministradorAlterado.CPFEditarAdministrador = CPFCrip;
                                                                                            AdministradorAlterado.GeneroEditardministrador = GeneroCrip;
                                                                                            AdministradorAlterado.TelefoneEditarAdministrador = TelefoneCrip;
                                                                                            AdministradorAlterado.CEPEditarAdministrador = CEPCrip;
                                                                                            AdministradorAlterado.SenhaEditarAdministrador = SenhaCrip;
                                                                                            AdministradorAlterado.LoginEditarAdministrador = LoginCrip;
                                                                                            AdministradorAlterado.NivelEditarAdministrador = cbb_NiveldeAcessoEditarAdministrador.Text;
                                                                                            AdministradorAlterado.emailEditarAdministrador = emailCrip;
                                                                                            AdministradorAlterado.NISEditarAdministrador = NISCrip;
                                                                                            AdministradorAlterado.NomedoServidor = lbl_NomedoServidor.Text;
                                                                                            ConexaoBancodeDados.AdministradorEdite(AdministradorAlterado);
                                                                                            this.Close();

                                                                                            string NomedoServidor = lbl_NomedoServidor.Text;
                                                                                            SqlConnection Conectar = new SqlConnection("Data Source=DESKTOP-" + NomedoServidor + "\\SQLEXPRESS;Initial Catalog=SGRRC1;Integrated Security=True");

                                                                                            //Salvar alterações nas outras tabelas que utilizam o nome de login do administrador.
                                                                                            Conectar.Open();
                                                                                            SqlCommand cmd = new SqlCommand("update CATADORES set [UsuarioEditadoCatador] = '" + txt_NomedeUsuarioEditarAdministrador.Text + "' where [UsuarioEditadoCatador] = '" + lbl_NomedeUsuarioAdicionarCatador.Text + "'; update CLIENTES set [Usuario] = '" + txt_NomedeUsuarioEditarAdministrador.Text + "' where [Usuario] = '" + lbl_NomedeUsuarioAdicionarCatador.Text + "';  update COLETAS set [UsuarioEditado] = '" + txt_NomedeUsuarioEditarAdministrador.Text + "' where [UsuarioEditado] = '" + lbl_NomedeUsuarioAdicionarCatador.Text + "'; update VENDAS set [Usuario] = '" + txt_NomedeUsuarioEditarAdministrador.Text + "' where [Usuario] = '" + lbl_NomedeUsuarioAdicionarCatador.Text + "';", Conectar);
                                                                                            cmd.ExecuteScalar();
                                                                                            Conectar.Close();
                                                                                            Application.Exit();
                                                                                        }
                                                                                    }

                                                                                    else
                                                                                    {
                                                                                        MessageBox.Show("As senhas não correspondem.");
                                                                                        return;

                                                                                    }
                                                                                }
                                                                                else
                                                                                {

                                                                                    MessageBox.Show("O campo Confirmar Senha não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                                                                                    return;

                                                                                }
                                                                            }
                                                                            else
                                                                            {

                                                                                MessageBox.Show("O campo Senha não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                                                                                return;

                                                                            }
                                                                        }
                                                                        else
                                                                        {

                                                                            MessageBox.Show("O campo Nome de Usuário não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                                                                            return;

                                                                        }
                                                                    }
                                                                    else
                                                                    {

                                                                        MessageBox.Show("O campo email não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                                                                        return;

                                                                    }

                                                                }
                                                                else
                                                                {

                                                                    MessageBox.Show("O campo Rua e Número não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                                                                    return;

                                                                }
                                                            }
                                                            else
                                                            {

                                                                MessageBox.Show("O campo Bairro não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                                                                return;

                                                            }
                                                        }
                                                        else
                                                        {

                                                            MessageBox.Show("O campo Telefone não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                                                            return;

                                                        }
                                                    }
                                                    else
                                                    {

                                                        MessageBox.Show("O campo Cidade não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                                                        return;

                                                    }
                                                }
                                                else
                                                {

                                                    MessageBox.Show("O campo CEP não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                                                    return;

                                                }
                                            }
                                            else
                                            {

                                                MessageBox.Show("O campo NIS não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                                                return;

                                            }
                                        }
                                        else
                                        {

                                            MessageBox.Show("O campo CPF não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                                            return;

                                        }
                                    }
                                    else
                                    {

                                        MessageBox.Show("O campo RG não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                                        return;

                                    }
                                }
                                else
                                {

                                    MessageBox.Show("O campo Gênero não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                                    return;

                                }
                            }
                            else
                            {

                                MessageBox.Show("O campo Data de Matrícula não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                                return;

                            }
                        }
                        else
                        {

                            MessageBox.Show("O campo Data de Nascimento não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                            return;

                        }
                    }
                    else
                    {

                        MessageBox.Show("O campo Nome Completo não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                        return;

                    }
                }
                else
                {
                    MessageBox.Show("Digite uma data de matrícula num formato válido.");
                    return;

                }
            }
            else
            {
                MessageBox.Show("Digite uma data de nascimento num formato válido.");
                return;

            }
        }

        //Teclado só reconhece números.
        private void txt_RGEditarAdministrador_KeyPress(object sender, KeyPressEventArgs e)
        {
            if ((!char.IsNumber(e.KeyChar)) && !char.IsControl(e.KeyChar))
            {
                e.Handled = true;

            }
        }

        //Teclado só reconhece números.
        private void txt_CPFEditarAdministrador_KeyPress(object sender, KeyPressEventArgs e)
        {
            if ((!char.IsNumber(e.KeyChar)) && !char.IsControl(e.KeyChar))
            {
                e.Handled = true;

            }
        }

        //Teclado só reconhece números.
        private void txt_NISEditarAdministrador_KeyPress(object sender, KeyPressEventArgs e)
        {
            if ((!char.IsNumber(e.KeyChar)) && !char.IsControl(e.KeyChar))
            {
                e.Handled = true;

            }
        }

        //Teclado só reconhece números
        private void txt_CEPEditarAdministrador_KeyPress(object sender, KeyPressEventArgs e)
        {
            if ((!char.IsNumber(e.KeyChar)) && !char.IsControl(e.KeyChar))
            {
                e.Handled = true;

            }
        }

        //Teclado só reconhece números e parênteses.
        private void txt_TelefoneEditarAdministrador_KeyPress(object sender, KeyPressEventArgs e)
        {
            if ((!char.IsNumber(e.KeyChar)) && !char.IsControl(e.KeyChar) && e.KeyChar != '(' && e.KeyChar != ')' && e.KeyChar != '-')
            {
                e.Handled = true;
            }
        }
        //Teclado só reconhece números e barras.
        private void txt_DatadeNascimentoEditarAdministrador_KeyPress(object sender, KeyPressEventArgs e)
        {
            if ((!char.IsNumber(e.KeyChar)) && !char.IsControl(e.KeyChar) && e.KeyChar != '/')
            {
                e.Handled = true;

            }

        }

        //Teclado só reconhece letras.
        private void txt_BairroEditarAdministrador_KeyPress(object sender, KeyPressEventArgs e)
        {
            if ((!char.IsLetter(e.KeyChar)) && !char.IsControl(e.KeyChar) && Convert.ToInt32(e.KeyChar) != 8 && Convert.ToInt32(e.KeyChar) != 32)
            {
                e.Handled = true;
            }
        }


        //Teclado só reconhece números e barras.
        private void txt_DatadeMatriculaEditarAdministrador_KeyPress(object sender, KeyPressEventArgs e)
        {
            if ((!char.IsNumber(e.KeyChar)) && !char.IsControl(e.KeyChar) && e.KeyChar != '/')
            {
                e.Handled = true;

            }
        }

        //Alterar estilo da combobox cidades.
        private void cbb_CidadeEditarAdministrador_Click(object sender, EventArgs e)
        {
            cbb_CidadeEditarAdministrador.DropDownStyle = ComboBoxStyle.DropDownList;
        }
    }
}


