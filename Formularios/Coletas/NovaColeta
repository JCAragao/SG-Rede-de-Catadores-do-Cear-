namespace Sistema_de_Gerenciamento_da_Rede_de_Catadores_do_Ceará__Versão_Teste_
{
    public partial class AdicionarColeta : Form
    {
        public AdicionarColeta()
        {
            InitializeComponent();
        }

        //Carregamento das comboboxes
        private void AdicionarColeta_Load(object sender, EventArgs e)
        {
            Criptografia cryptography = new Criptografia(System.Configuration.ConfigurationManager.AppSettings[""]);

            string Metal = cryptography.Encriptar(rdb_MetalAdicionarColeta.Text);
            string Associacao = cryptography.Encriptar(lbl_UnidadeUsuarioColeta.Text);
            string NomedoServidor = lbl_NomedoServidor.Text;

            string vqueryMetal = @"select Material from METERIAIS where Grupo = '" + Metal + "' AND Unidade = '" + Associacao + "' ";

            cbb_MetalAdicionarColeta.DataSource = ConexaoBancodeDados.dql(vqueryMetal, NomedoServidor);
            cbb_MetalAdicionarColeta.DisplayMember = "Material";

            string Papel = cryptography.Encriptar(rdb_PapelAdicionarColeta.Text);

            string vqueryPapel = @"select Material from METERIAIS where Grupo = '" + Papel + "' AND Unidade = '" + Associacao + "' ";

            cbb_PapelAdicionarColeta.DataSource = ConexaoBancodeDados.dql(vqueryPapel, NomedoServidor);
            cbb_PapelAdicionarColeta.DisplayMember = "Material";

            string Plastico = cryptography.Encriptar(rdb_PlasticoAdicionarColeta.Text);

            string vqueryPlastico = @"select Material from METERIAIS where Grupo = '" + Plastico + "' AND Unidade = '" + Associacao + "' ";

            cbb_PlasticoAdicionarColeta.DataSource = ConexaoBancodeDados.dql(vqueryPlastico, NomedoServidor);
            cbb_PlasticoAdicionarColeta.DisplayMember = "Material";

            string Organico = cryptography.Encriptar(rdb_OrganicoAdicionarColeta.Text);

            string vqueryOrganico = @"select Material from METERIAIS where Grupo = '" + Organico + "' AND Unidade = '" + Associacao + "' ";

            cbb_OrganicoAdicionarColeta.DataSource = ConexaoBancodeDados.dql(vqueryOrganico, NomedoServidor);
            cbb_OrganicoAdicionarColeta.DisplayMember = "Material";

            string Vidro = cryptography.Encriptar(rdb_VidroAdicionarColeta.Text);

            string vqueryVidro = @"select Material from METERIAIS where Grupo = '" + Vidro + "' AND Unidade = '" + Associacao + "' ";

            cbb_VidroAdicionarColeta.DataSource = ConexaoBancodeDados.dql(vqueryVidro, NomedoServidor);
            cbb_VidroAdicionarColeta.DisplayMember = "Material";
        }

        //Adicionar nova coleta
        private void btn_AdicionarColeta_Click(object sender, EventArgs e)
        {

            if (cbb_TipodeMaterialAdicionarColeta.Text != "System.Data.DataRowView")
            {
                if (cbb_TipodeResiduoAdicionarColeta.SelectedIndex != -1)
                {
                    if (txt_MassaAdicionarColeta.Text != "")
                    {
                        if (txt_NomeCatador.Text != "")
                        {
                            string CatadorCrip = txt_Matricula.Text;
                            string DatadaColetaCrip = txt_DatadaColeta.Text;
                            string MassaCrip = txt_MassaAdicionarColeta.Text;
                            decimal MassaIntCrip = Decimal.Parse(txt_MassaAdicionarColeta.Text);
                            string TipodeResiduoCrip = cbb_TipodeResiduoAdicionarColeta.Text;
                            string TipodeMaterialCrip = cbb_TipodeMaterialAdicionarColeta.Text;
                            string RaciclavelCrip = txt_ReciclavelAdicionarColeta.Text;
                            string AdministradorCrip = lbl_UsuarioColeta.Text;
                            string UnidadeCrip = lbl_UnidadeUsuarioColeta.Text;
                            string NomedoServidor = lbl_NomedoServidor.Text;

                            AdicionarColetaClasse novacoleta = new AdicionarColetaClasse();
                            novacoleta.CatadorAdicionarColeta = CatadorCrip;
                            novacoleta.DatadaColetaAdicionarColeta = DateTime.Parse(DateTime.Now.ToShortDateString());
                            novacoleta.MassaAdicionarColeta = MassaCrip;
                            novacoleta.MassaINTAdicionarColeta = MassaIntCrip;
                            novacoleta.TipodeResiduoAdicionarColeta = TipodeResiduoCrip;
                            novacoleta.TipodeMaterialAdicionarColeta = TipodeMaterialCrip;
                            novacoleta.ReciclavelAdicionarColeta = RaciclavelCrip;
                            novacoleta.AdministradorAdicionarColeta = AdministradorCrip;
                            novacoleta.UnidadeAdicionarColeta = UnidadeCrip;
                            novacoleta.NomedoServidor = NomedoServidor;
                            ConexaoBancodeDados.ColetaNew(novacoleta);
                            this.Close();
                        }
                        else
                        {

                            MessageBox.Show("O campo Catador não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                            return;
                        }
                    }
                    else
                    {

                        MessageBox.Show("O campo massa não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                        return;
                    }
                }
                else
                {

                    MessageBox.Show("O campo Tipo de Resíduo não foi preenchido", "campo inválido", MessageBoxButtons.OK);
                    return;
                }
            }
            else
            {
                MessageBox.Show("O campo Tipo de Material não foi preenchido. Clique na lista e escolha.", "Campo inválido", MessageBoxButtons.OK);
                return;
            }
        }

        //Somente números e '/' são aceitos
        private void txt_DatadaColeta_KeyPress(object sender, KeyPressEventArgs e)
        {
            if ((!char.IsNumber(e.KeyChar)) && !char.IsControl(e.KeyChar) && e.KeyChar != '/')
            {
                e.Handled = true;
            }
        }

        //Somente números e ',' são aceitos.
        private void txt_MassaAdicionarColeta_KeyPress(object sender, KeyPressEventArgs e)
        {
            if ((!char.IsNumber(e.KeyChar)) && !char.IsControl(e.KeyChar) && e.KeyChar != ',' && e.KeyChar != '.')
            {
                e.Handled = true;
            }
        }

       //Eventos de marcação do radiobutton Metal
        private void rdb_MetalicoFerrosoAdicionarColeta_CheckedChanged(object sender, EventArgs e)
        {
            cbb_MetalAdicionarColeta.Visible = true;
            cbb_PapelAdicionarColeta.Text = "";
            cbb_PlasticoAdicionarColeta.Text = "";
            cbb_VidroAdicionarColeta.Text = "";

            cbb_PapelAdicionarColeta.Visible = false;
            cbb_PlasticoAdicionarColeta.Visible = false;
            cbb_VidroAdicionarColeta.Visible = false;

            cbb_OrganicoAdicionarColeta.Text = "";
            cbb_OrganicoAdicionarColeta.Visible = false;


        }

        //A combobox de envio de tipo de material receberá o item selecionado da combobox do metal.
        private void cbb_MetalicoFerrosoAdicionarColeta_SelectedIndexChanged(object sender, EventArgs e)
        {
            cbb_TipodeMaterialAdicionarColeta.Items.Clear();
            cbb_TipodeMaterialAdicionarColeta.Items.Add(cbb_MetalAdicionarColeta.Text);
            cbb_TipodeMaterialAdicionarColeta.SelectedIndex = 0;
        }

        //Eventos de marcação do radiobutton Papel
        private void rdb_MetalicoNaoFerrosoAdicionarColeta_CheckedChanged(object sender, EventArgs e)
        {
            cbb_PapelAdicionarColeta.Visible = true;

            cbb_MetalAdicionarColeta.Text = "";
            cbb_PlasticoAdicionarColeta.Text = "";
            cbb_VidroAdicionarColeta.Text = "";
            cbb_OrganicoAdicionarColeta.Text = "";


            cbb_MetalAdicionarColeta.Visible = false;
            cbb_VidroAdicionarColeta.Visible = false;
            cbb_PlasticoAdicionarColeta.Visible = false;
            cbb_OrganicoAdicionarColeta.Visible = false;
        }

        //A combobox de envio de tipo de material receberá o item selecionado da combobox do papel.
        private void cbb_MetalicoNaoFerrosoAdicionarColeta_SelectedIndexChanged(object sender, EventArgs e)
        {
            cbb_TipodeMaterialAdicionarColeta.Items.Clear();
            cbb_TipodeMaterialAdicionarColeta.Items.Add(cbb_PapelAdicionarColeta.Text);
            cbb_TipodeMaterialAdicionarColeta.SelectedIndex = 0;
        }

        //Eventos de marcação do radiobutton do Plástico
        private void rdb_NaoMetalicoOrganicoAdicionarColeta_CheckedChanged(object sender, EventArgs e)
        {
            cbb_PlasticoAdicionarColeta.Visible = true;
            cbb_MetalAdicionarColeta.Text = "";
            cbb_PapelAdicionarColeta.Text = "";
            cbb_VidroAdicionarColeta.Text = "";
            cbb_OrganicoAdicionarColeta.Text = "";
            cbb_OrganicoAdicionarColeta.Visible = false;

            cbb_MetalAdicionarColeta.Visible = false;
            cbb_PapelAdicionarColeta.Visible = false;
            cbb_VidroAdicionarColeta.Visible = false;
        }

        //A combobox de envio de tipo de material receberá o item selecionado da combobox do plástico.
        private void cbb_NaoMetalicoOrganicoAdicionarColeta_SelectedIndexChanged(object sender, EventArgs e)
        {
            cbb_TipodeMaterialAdicionarColeta.Items.Clear();
            cbb_TipodeMaterialAdicionarColeta.Items.Add(cbb_PlasticoAdicionarColeta.Text);
            cbb_TipodeMaterialAdicionarColeta.SelectedIndex = 0;
        }

        //Eventos de marcação do radiobutton do vidro.
        private void rdb_NaoMetalicoInorganicoAdicionarColeta_CheckedChanged(object sender, EventArgs e)
        {
            cbb_OrganicoAdicionarColeta.Text = "";
            cbb_OrganicoAdicionarColeta.Visible = false;
            cbb_VidroAdicionarColeta.Visible = true;
            cbb_MetalAdicionarColeta.Text = "";
            cbb_PapelAdicionarColeta.Text = "";
            cbb_PlasticoAdicionarColeta.Text = "";


            cbb_MetalAdicionarColeta.Visible = false;
            cbb_PapelAdicionarColeta.Visible = false;
            cbb_PlasticoAdicionarColeta.Visible = false;
        }

        //A combobox de envio de tipo de material receberá o item selecionado da combobox do vidro.
        private void cbb_NaoMetalicoInorganicoAdicionarColeta_SelectedIndexChanged(object sender, EventArgs e)
        {
            cbb_TipodeMaterialAdicionarColeta.Items.Clear();
            cbb_TipodeMaterialAdicionarColeta.Items.Add(cbb_VidroAdicionarColeta.Text);
            cbb_TipodeMaterialAdicionarColeta.SelectedIndex = 0;
        }


        private void checkBox1_CheckedChanged(object sender, EventArgs e)
        {
            if (checkBox1.Checked == true)
            {
                txt_ReciclavelAdicionarColeta.Text = "Reciclável";
            }

            else
            {
                txt_ReciclavelAdicionarColeta.Text = "Não Reciclável";

            }
        }    

        //Fechar o formulário
        private void btn_Cancelar_Click(object sender, EventArgs e)
        {
            this.Close();
        }

        private void rdb_OrganicoAdicionarColeta_CheckedChanged(object sender, EventArgs e)
        {
            cbb_MetalAdicionarColeta.Text = "";
            cbb_PapelAdicionarColeta.Text = "";
            cbb_VidroAdicionarColeta.Text = "";
            cbb_VidroAdicionarColeta.Text = "";

            cbb_PlasticoAdicionarColeta.Visible = false;
            cbb_MetalAdicionarColeta.Visible = false;
            cbb_PapelAdicionarColeta.Visible = false;
            cbb_VidroAdicionarColeta.Visible = false;
            cbb_OrganicoAdicionarColeta.Visible = true;
        }

        private void cbb_OrganicoAdicionarColeta_SelectedIndexChanged(object sender, EventArgs e)
        {
            cbb_TipodeMaterialAdicionarColeta.Items.Clear();
            cbb_TipodeMaterialAdicionarColeta.Items.Add(cbb_OrganicoAdicionarColeta.Text);
            cbb_TipodeMaterialAdicionarColeta.SelectedIndex = 0;
        }

        //Consulta da correspondência da matrícula informada com o nome associado no banco de dados
        private void txt_Matricula_KeyPress(object sender, KeyPressEventArgs e)
        {
            if (Convert.ToInt32(e.KeyChar) == 13)
                {
                    e.Handled = true;


                    string NomedoServidor = lbl_NomedoServidor.Text;
                    SqlConnection Conectar = new SqlConnection("Data Source=DESKTOP-" + NomedoServidor + "\\SQLEXPRESS;Initial Catalog=SGRRC1;Integrated Security=True");
                    Conectar.Open();
                    string IDCatador = txt_Matricula.Text;
                    SqlCommand cmd = new SqlCommand("select [Catador] from CATADORES WHERE [Matrícula] = '" + IDCatador + "'", Conectar);
                    var Catador = cmd.ExecuteScalar();

                if (Catador != null)
                {

                    txt_NomeCatador.Text = Catador.ToString();
                    Conectar.Close();
                }
                else
                {
                    if (txt_Matricula.Text == "0")
                    {
                        txt_NomeCatador.Text = "Catador não associado";

                    }

                    else
                    {
                        MessageBox.Show("Catador não encontrado.");
                        Conectar.Close();
                        return;
                    }
                }
            }
    }

  
    }
