        }

        //Eventos de marcação do radiobutton Vidro.
        private void rdb_NaoMetalicoInorganicoColetasEditar_CheckedChanged(object sender, EventArgs e)
        {
            cbb_VidroColetasEditar.Visible = true;
            cbb_MetalColetasEditar.Text = "";
            cbb_PapelColetasEditar.Text = "";
            cbb_PlasticoColetasEditar.Text = "";
