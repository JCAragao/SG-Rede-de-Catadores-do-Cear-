            this.Icon = ((System.Drawing.Icon)(resources.GetObject("$this.Icon")));
            this.MaximizeBox = false;
            this.MaximumSize = new System.Drawing.Size(510, 289);
            this.MinimizeBox = false;
            this.MinimumSize = new System.Drawing.Size(510, 289);
            this.Name = "ColetasEditar";
