        public ColetasEditar(ColetasGestao coletasGestao)
        {
            InitializeComponent();

        }

        //Carregamento das comboboxes de Tipo de Material e verificação
        //de ocorrência de alteração de informações no banco de dados.
        public void ColetasEditar_Load(object sender, EventArgs e)
        {

            Criptografia cryptography = new Criptografia(System.Configuration.ConfigurationManager.AppSettings[""]);

            string Metal = cryptography.Encriptar(rdb_MetalColetasEditar.Text);
            string Associacao = cryptography.Encriptar(lbl_UnidadeUsuarioColeta.Text);
            string NomedoServidor = lbl_NomedoServidor.Text;

            string vqueryMetal = @"select Material from METERIAIS where Grupo = '" + Metal + "' AND Unidade = '" + Associacao + "' ";

            cbb_MetalColetasEditar.DataSource = ConexaoBancodeDados.dql(vqueryMetal, NomedoServidor);
            cbb_MetalColetasEditar.DisplayMember = "Material";

            string Papel = cryptography.Encriptar(rdb_PapelAdicionarColeta.Text);

            string vqueryPapel = @"select Material from METERIAIS where Grupo = '" + Papel + "' AND Unidade = '" + Associacao + "' ";

            cbb_PapelColetasEditar.DataSource = ConexaoBancodeDados.dql(vqueryPapel, NomedoServidor);
            cbb_PapelColetasEditar.DisplayMember = "Material";

            string Plastico = cryptography.Encriptar(rdb_PlasticoColetasEditar.Text);

            string vqueryPlastico = @"select Material from METERIAIS where Grupo = '" + Plastico + "' AND Unidade = '" + Associacao + "' ";

            cbb_PlasticoColetasEditar.DataSource = ConexaoBancodeDados.dql(vqueryPlastico, NomedoServidor);
            cbb_PlasticoColetasEditar.DisplayMember = "Material";

            string Organico = cryptography.Encriptar(rdb_Organico.Text);

            string vqueryOrganico = @"select Material from METERIAIS where Grupo = '" + Organico + "' AND Unidade = '" + Associacao + "' ";

            cbb_OrganicoEditar.DataSource = ConexaoBancodeDados.dql(vqueryOrganico, NomedoServidor);
            cbb_OrganicoEditar.DisplayMember = "Material";

            string Vidro = cryptography.Encriptar(rdb_VidroColetasEditar.Text);

            string vqueryVidro = @"select Material from METERIAIS where Grupo = '" + Vidro + "' AND Unidade = '" + Associacao + "' ";

            cbb_VidroColetasEditar.DataSource = ConexaoBancodeDados.dql(vqueryVidro, NomedoServidor);
            cbb_VidroColetasEditar.DisplayMember = "Material";

            cbb_TipodeMaterialColetasEditarEnviar.Text = cbb_TipodeMaterialColetasEditar.Text;

            SqlConnection Conectar = new SqlConnection("Data Source=DESKTOP-" + NomedoServidor + "\\SQLEXPRESS;Initial Catalog=SGRRC1;Integrated Security=True");
            Conectar.Open();
            string IDCatador = txt_CatadorRegistro.Text;
            SqlCommand cmd = new SqlCommand("select [Catador] from CATADORES WHERE [Matrícula] = '" + IDCatador + "'", Conectar);
            var Catador = cmd.ExecuteScalar();

            string Matricula = txt_RegistrodaColetaColetasEditar.Text;
            SqlCommand cmd2 = new SqlCommand("select [UsuarioEditado] from COLETAS WHERE [ID_Coleta] = '" + Matricula + "'", Conectar);
            var Matricula2 = cmd2.ExecuteScalar();

            SqlCommand cmd3 = new SqlCommand("select [DataEditado] from COLETAS WHERE [ID_Coleta] = '" + Matricula + "'", Conectar);
            var DataEditado = cmd3.ExecuteScalar();

            if (Matricula2 != null)
            {
                lbl_Editadopor.Visible = true;
                lbl_UsuarioEditado.Visible = true;
                string Matricula3 = Matricula2.ToString();
                lbl_UsuarioEditado.Text = Matricula3;

                lbl_DataEditada.Visible = true;
                string DataEditado2 = DataEditado.ToString();
                lbl_DataEditada.Text = DataEditado2;

            }
            Conectar.Close();
            if (Catador != null)
            {

                txt_CatadorNome.Text = Catador.ToString();
                Conectar.Close();
            }
        }

        //Somente números e '/' são aceitos.
        private void txt_DatadaColeta_KeyPress(object sender, KeyPressEventArgs e)
        {
            if ((!char.IsNumber(e.KeyChar)) && !char.IsControl(e.KeyChar) && e.KeyChar != '/')
            {
                e.Handled = true;
            }
        }

        //Somente números e ',' são aceitos.
        private void txt_MassaAdicionarColeta_KeyPress(object sender, KeyPressEventArgs e)
        {
            if ((!char.IsNumber(e.KeyChar)) && !char.IsControl(e.KeyChar) && e.KeyChar != ',' && e.KeyChar != '.')
            {
                e.Handled = true;
            }

        }

        //Eventos de marcação do radiobutton Metal
        private void rdb_MetalColetasEditar_CheckedChanged(object sender, EventArgs e)
        {
            cbb_MetalColetasEditar.Visible = true;
            cbb_PapelColetasEditar.Text = "";
            cbb_PlasticoColetasEditar.Text = "";
            cbb_VidroColetasEditar.Text = "";
            cbb_OrganicoEditar.Visible = false;
            cbb_OrganicoEditar.Text = "";
            cbb_PapelColetasEditar.Visible = false;
            cbb_PlasticoColetasEditar.Visible = false;
            cbb_VidroColetasEditar.Visible = false;
            cbb_TipodeMaterialColetasEditar.Visible = false;
            cbb_TipodeMaterialColetasEditar.Text = "";

        }

        //A combobox de envio de tipo de material receberá o item selecionado da combobox do metal.
        private void cbb_MetalColetasEditar_SelectedIndexChanged(object sender, EventArgs e)
        {
            cbb_TipodeMaterialColetasEditarEnviar.Items.Clear();
            cbb_TipodeMaterialColetasEditarEnviar.Items.Add(cbb_MetalColetasEditar.Text);
            cbb_TipodeMaterialColetasEditarEnviar.SelectedIndex = 0;
        }

        //Eventos de marcação do radiobutton Papel.
        private void rdb_PapelColetaEditar_CheckedChanged(object sender, EventArgs e)
        {
            cbb_PapelColetasEditar.Visible = true;
            cbb_MetalColetasEditar.Text = "";
            cbb_VidroColetasEditar.Text = "";
            cbb_PlasticoColetasEditar.Text = "";
            cbb_OrganicoEditar.Visible = false;
            cbb_OrganicoEditar.Text = "";
            cbb_MetalColetasEditar.Visible = false;
            cbb_VidroColetasEditar.Visible = false;
            cbb_PlasticoColetasEditar.Visible = false;
            cbb_TipodeMaterialColetasEditar.Visible = false;
            cbb_TipodeMaterialColetasEditar.Text = "";
        }

        //A combobox de envio de tipo de material receberá o item selecionado da combobox do papel
        private void cbb_Papel_SelectedIndexChanged(object sender, EventArgs e)
        {
            cbb_TipodeMaterialColetasEditarEnviar.Items.Clear();
            cbb_TipodeMaterialColetasEditarEnviar.Items.Add(cbb_PapelColetasEditar.Text);
            cbb_TipodeMaterialColetasEditarEnviar.SelectedIndex = 0;
        }

        //Eventos de marcação do radiobutton Plástico.
        private void rdb_PlasticoColetasEditar_CheckedChanged(object sender, EventArgs e)
        {
            cbb_PlasticoColetasEditar.Visible = true;
            cbb_MetalColetasEditar.Text = "";
            cbb_PapelColetasEditar.Text = "";
            cbb_VidroColetasEditar.Text = "";
            cbb_OrganicoEditar.Visible = false;
            cbb_OrganicoEditar.Text = "";
            cbb_MetalColetasEditar.Visible = false;
            cbb_PapelColetasEditar.Visible = false;
            cbb_VidroColetasEditar.Visible = false;
            cbb_TipodeMaterialColetasEditar.Visible = false;
            cbb_TipodeMaterialColetasEditar.Text = "";
        }

        //A combobox de envio de tipo de material receberá o item selecionado da combobox do plástico.
        private void cbb_PlasticoColetasEditar_SelectedIndexChanged(object sender, EventArgs e)
