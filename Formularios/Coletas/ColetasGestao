                btn_PesquisarporUltimoDia.Visible = false;
                btn_PesquisarTresMeses.Visible = false;
                btn_PesquisarUltimaSemana.Visible = false;
                btn_PesquisarUltimoMes.Visible = true;
                btn_PorUltAnoPesquisar.Visible = false;
                button5.Visible = false;


            }

            if (cbb_Pesquisa.SelectedIndex == 7)
            {
                btn_DoisMesesImprimir.Visible = true;
                txt_DigitedaColeta.Visible = false;
                nud_Mes.Visible = false;
                txt_Ano.Visible = false;
                btn_PesquisarporAno.Visible = false;
                btn_PesquisarMeseAno.Visible = false;
                cbb_Pesquisa.Visible = true;
                btn_PesquisarporDataColetasGestao.Visible = false;
                btn_PesquisarporAssociacao.Visible = false;
                btn_PesquisarporAssociacaoeData.Visible = false;
                btn_PesquisarPorCatadorColetasGestao.Visible = false;
                btn_PesquisarporDoisMeses.Visible = true;
                btn_PesquisarporUltimaQuinzena.Visible = false;
                btn_PesquisarporUltimoDia.Visible = false;
                btn_PesquisarTresMeses.Visible = false;
                btn_PesquisarUltimaSemana.Visible = false;
                btn_PesquisarUltimoMes.Visible = false;
                btn_PorUltAnoPesquisar.Visible = false;
                button5.Visible = false;

            }

            if (cbb_Pesquisa.SelectedIndex == 8)
            {
                btn_TresMesesImprimir.Visible = true;
                txt_DigitedaColeta.Visible = false;
                nud_Mes.Visible = false;
                txt_Ano.Visible = false;
                btn_PesquisarporAno.Visible = false;
                btn_PesquisarMeseAno.Visible = false;
                cbb_Pesquisa.Visible = true;
                btn_PesquisarporDataColetasGestao.Visible = false;
                btn_PesquisarporAssociacao.Visible = false;
                btn_PesquisarporAssociacaoeData.Visible = false;
                btn_PesquisarPorCatadorColetasGestao.Visible = false;
                btn_PesquisarporDoisMeses.Visible = false;
                btn_PesquisarporUltimaQuinzena.Visible = false;
                btn_PesquisarporUltimoDia.Visible = false;
                btn_PesquisarTresMeses.Visible = true;
                btn_PesquisarUltimaSemana.Visible = false;
                btn_PesquisarUltimoMes.Visible = false;
                btn_PorUltAnoPesquisar.Visible = false;
                button5.Visible = false;

            }

            if (cbb_Pesquisa.SelectedIndex == 9)
            {

                btn_PorUltAnoPesquisar.Visible = true;
                btn_UltimoAno.Visible = true;
                txt_DigitedaColeta.Visible = false;
                nud_Mes.Visible = false;
                txt_Ano.Visible = false;
                btn_PesquisarporAno.Visible = false;
                btn_PesquisarMeseAno.Visible = false;
                cbb_Pesquisa.Visible = true;
                btn_PesquisarporDataColetasGestao.Visible = false;
                btn_PesquisarporAssociacao.Visible = false;
                btn_PesquisarporAssociacaoeData.Visible = false;
                btn_PesquisarPorCatadorColetasGestao.Visible = false;
                btn_PesquisarporDoisMeses.Visible = false;
                btn_PesquisarporUltimaQuinzena.Visible = false;
                btn_PesquisarporUltimoDia.Visible = false;
                btn_PesquisarTresMeses.Visible = false;
                btn_PesquisarUltimaSemana.Visible = false;
                btn_PesquisarUltimoMes.Visible = false;
                button5.Visible = false;
            }

            if (cbb_Pesquisa.SelectedIndex == 10)
            {
                txt_DigitedaColeta.Visible = true;
                txt_DigitedaColeta.Enabled = true;
                txt_DigitedaColeta.BackColor = Color.White;
                txt_NomeCatador.Enabled = true;
                txt_NomeCatador.BackColor = Color.White;
                nud_Mes.Visible = false;
                txt_Ano.Visible = false;
                btn_PesquisarporAno.Visible = false;
                btn_PesquisarMeseAno.Visible = false;
                cbb_Pesquisa.Visible = false;
                btn_PesquisarporDataColetasGestao.Visible = false;
                btn_PesquisarporAssociacao.Visible = false;
                btn_PesquisarporAssociacaoeData.Visible = false;
                btn_PesquisarPorCatadorColetasGestao.Visible = false;
                btn_PesquisarporDoisMeses.Visible = false;
                btn_PesquisarporUltimaQuinzena.Visible = false;
                btn_PesquisarporUltimoDia.Visible = false;
                btn_PesquisarTresMeses.Visible = false;
                btn_PesquisarUltimaSemana.Visible = false;
                btn_PesquisarUltimoMes.Visible = false;
                btn_PorUltAnoPesquisar.Visible = false;
                button5.Visible = true;
                txt_NomeCatador.Visible = true;
                txt_NomeCatadorNome.Visible = true;
                lbl_CatadorColetasGestao.Visible = true;
            }

        }
   
        //Eventos de pesquisa por ano e mês específico
        private void btn_PesquisarMeseAno_Click(object sender, EventArgs e)
        {
            string Nomedoservidor = lbl_NomedoServidor.Text;
                string vqueryColetaData = @"select*from COLETAS where [Mês] = '" + nud_Mes.Value.ToString() + "' and [Ano] = '" + txt_Ano.Text + "' and [Unidade] = '" + lbl_Unidade.Text+ "' order by ID_Coleta ASC";
                dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryColetaData, Nomedoservidor);

                if (dgv_ColetasGestao.RowCount != 0)
                {
                    decimal TotalCatadorData = 0;
                    for (int i = 0; i < dgv_ColetasGestao.Rows.Count; ++i)
                    {
                        TotalCatadorData += Convert.ToDecimal(dgv_ColetasGestao.Rows[i].Cells[2].Value);

                    }
                    lbl_MassaTotalCatadorData.Text = TotalCatadorData.ToString();
                    lbl_MassaTotalCatadorData.Visible = true;

                }

            
        }

        //Eventos de pesquisa por ano específico
        private void btn_PesquisarporAno_Click(object sender, EventArgs e)
        {

            string Nomedoservidor = lbl_NomedoServidor.Text;
                string vqueryColetaData = @"select*from COLETAS where [Ano] = '" + txt_Ano.Text + "' and [Unidade] = '" + lbl_Unidade.Text + "' order by ID_Coleta ASC";
                dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryColetaData, Nomedoservidor);

                if (dgv_ColetasGestao.RowCount != 0)
                {
                    decimal TotalCatadorData = 0;
                    for (int i = 0; i < dgv_ColetasGestao.Rows.Count; ++i)
                    {
                        TotalCatadorData += Convert.ToDecimal(dgv_ColetasGestao.Rows[i].Cells[2].Value);

                    }
                    lbl_MassaTotalCatadorData.Text = TotalCatadorData.ToString();
                    lbl_MassaTotalCatadorData.Visible = true;

                }
            
        }

        //Eventos de marcação do checkbox da barra de pesquisa
        private void checkBox1_CheckedChanged(object sender, EventArgs e)
        {

            if (chb_Pesquisa.Checked == true)
            {
                cbb_Pesquisa.Visible = true;
                txt_Ano.Visible = false;
                nud_Mes.Visible = false;
                txt_DigitedaColeta.Visible = false;
            }
        }

        //Impressão últimos dois mêses
        private void button1_Click_2(object sender, EventArgs e)
        {
            try
            {
                if (btn_PesquisarporDoisMeses.Visible == true)
                {
                    {
                        if (dgv_ColetasGestao.Rows.Count == 0)
                        {
                            MessageBox.Show("Sem coletas a mostrar.");
                            return;
                        }
                        else
                        {
                            string output = string.Concat(DateTime.Now.ToString().Where(Char.IsDigit));
                            string catadordata = "Últimos dois Mêses";
                            string sernome = lbl_NomedoServidor.Text;
                            string nomeassociacao = lbl_Unidade.Text;
                            string nomedocatador = txt_NomeCatadorNome.Text;
                            string nomeArquivo = Globais.caminhoImpressao + @"\COLETAS_Últimos_Dois_Mêses_Até" + output + ".pdf";
                            FileStream arquivoPDF = new FileStream(nomeArquivo, FileMode.Create);
                            Document doc = new Document(PageSize.A4.Rotate(), 30f, 0f, 120f, 150f);
                            doc.SetPageSize(doc.PageSize);
                            PdfWriter escritorPDF = PdfWriter.GetInstance(doc, arquivoPDF);
                            escritorPDF.PageEvent = new CabecalhoCatadoreData(catadordata, sernome, nomedocatador, nomeassociacao);


                            doc.Open();
                            string dados = "";



                            PdfPTable tabela = new PdfPTable(6);
                            tabela.TotalWidth = 500;


                            tabela.AddCell("Data da Coleta");
                            tabela.AddCell("Catador");
                            tabela.AddCell("Massa(Kg)");
                            tabela.AddCell("TipodeResiduo");
                            tabela.AddCell("TipodeMaterial");
                            tabela.AddCell("Associação");

                            string data = txt_DigitedaColeta.Text;
                            string vqueryColetaImpressao = @"SELECT [ID_Coleta], [Data da Coleta], [Catador], [Massa], [TipodeResiduo], [TipodeMaterial], [Unidade], [Administrador] FROM COLETAS WHERE [Unidade] = '" + lbl_Unidade.Text + "' and [Data da Coleta] between (select dateadd(month, -2, getdate())) and (select dateadd(month, +0, getdate()));";
                            string Nomedoservidor = lbl_NomedoServidor.Text;

                            DataTable dtColetaImpressao = ConexaoBancodeDados.dql(vqueryColetaImpressao, Nomedoservidor);
                            for (int i = 0; i < dtColetaImpressao.Rows.Count; i++)
                            {
                                tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Data da Coleta"));
                                tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Catador"));
                                tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Massa"));
                                tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("TipodeResiduo"));
                                tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("TipodeMaterial"));
                                tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Unidade"));
                            }


                            doc.Add(tabela);
                            doc.Close();

                            System.Diagnostics.Process.Start(Globais.caminhoImpressao + @"\COLETAS_Últimos_Dois_Mêses_Até" + output + ".pdf");

                        }
                    }
                }
                    
            }
            catch
            {
                MessageBox.Show("Pasta 'Impressão' não encontrada.");
                return;

            }

        } 
       
        //Impressão últimos três mêses
        private void btn_TresMesesImprimir_Click(object sender, EventArgs e)
        {
            try
            {
                if (dgv_ColetasGestao.Rows.Count == 0)
                {
                    MessageBox.Show("Sem coletas a mostrar.");
                    return;
                }
                else
                {
                    string catadordata = "Últimos Três Mêses";
                    string output = string.Concat(DateTime.Now.ToString().Where(Char.IsDigit));

                    string nomedocatador = lbl_Unidade.Text;
                    string nomeassociacao = lbl_Unidade.Text;
                    string sernome = lbl_NomedoServidor.Text;
                    string nomeArquivo = Globais.caminhoImpressao + @"\COLETAS_" + nomedocatador + "_Últimos_Três_Mêses_Até_" + output + ".pdf";
                    FileStream arquivoPDF = new FileStream(nomeArquivo, FileMode.Create);
                    Document doc = new Document(PageSize.A4.Rotate(), 30f, 0f, 120f, 150f);
                    doc.SetPageSize(doc.PageSize);
                    PdfWriter escritorPDF = PdfWriter.GetInstance(doc, arquivoPDF);
                    escritorPDF.PageEvent = new CabecalhoCatadoreData(catadordata, sernome, nomedocatador, nomeassociacao);


                    doc.Open();
                    string dados = "";



                    PdfPTable tabela = new PdfPTable(6);
                    tabela.TotalWidth = 500;


                    tabela.AddCell("Data da Coleta");
                    tabela.AddCell("Catador");
                    tabela.AddCell("Massa(Kg)");
                    tabela.AddCell("TipodeResiduo");
                    tabela.AddCell("TipodeMaterial");
                    tabela.AddCell("Associação");

                    string data = txt_DigitedaColeta.Text;
                    string vqueryColetaImpressao = @"select*from COLETAS where [Unidade] = '" + lbl_Unidade.Text + "' and [Data da Coleta] between (select dateadd(month, -3, getdate())) and (select dateadd(month, +0, getdate()));";
                    string Nomedoservidor = lbl_NomedoServidor.Text;

                    DataTable dtColetaImpressao = ConexaoBancodeDados.dql(vqueryColetaImpressao, Nomedoservidor);
                    for (int i = 0; i < dtColetaImpressao.Rows.Count; i++)
                    {
                        tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Data da Coleta"));
                        tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Catador"));
                        tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Massa"));
                        tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("TipodeResiduo"));
                        tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("TipodeMaterial"));
                        tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Unidade"));
                    }


                    doc.Add(tabela);
                    doc.Close();

                    System.Diagnostics.Process.Start(Globais.caminhoImpressao + @"\COLETAS_" + nomedocatador + "_Últimos_Três_Mêses_Até_" + output + ".pdf");

                }
            }
            catch
            {
                MessageBox.Show("Pasta 'Impressão' não encontrada");
                return;

            }
        }
        
        //Impressão última quinzena
        private void btn_QuinzenaImprimir_Click(object sender, EventArgs e)
        {
            try
            {

                if (dgv_ColetasGestao.Rows.Count == 0)
                {
                    MessageBox.Show("Sem coletas a mostrar.");
                    return;
                }
                else
                {
                    string catadordata = "Última quinzena";
                    string output = string.Concat(DateTime.Now.ToString().Where(Char.IsDigit));
                    string sernome = lbl_NomedoServidor.Text;
                    string nomedocatador = txt_NomeCatadorNome.Text;
                    string nomeassociacao = lbl_Unidade.Text;
                    string nomeArquivo = Globais.caminhoImpressao + @"\COLETAS_ULTIMAQUINZENA_" + output + ".pdf";
                    FileStream arquivoPDF = new FileStream(nomeArquivo, FileMode.Create);
                    Document doc = new Document(PageSize.A4.Rotate(), 30f, 0f, 120f, 150f);
                    doc.SetPageSize(doc.PageSize);
                    PdfWriter escritorPDF = PdfWriter.GetInstance(doc, arquivoPDF);
                    escritorPDF.PageEvent = new CabecalhoCatadoreData(catadordata, sernome, nomedocatador, nomeassociacao);


                    doc.Open();
                    string dados = "";



                    PdfPTable tabela = new PdfPTable(6);
                    tabela.TotalWidth = 500;


                    tabela.AddCell("Data da Coleta");
                    tabela.AddCell("Catador");
                    tabela.AddCell("Massa(Kg)");
                    tabela.AddCell("TipodeResiduo");
                    tabela.AddCell("TipodeMaterial");
                    tabela.AddCell("Associação");

                    string data = txt_DigitedaColeta.Text;
                    string unidade = lbl_Unidade.Text;
                    string vqueryColetaImpressao = @"SELECT [ID_Coleta], [Data da Coleta], [Catador], [Massa], [TipodeResiduo], [TipodeMaterial], [Unidade], [Administrador] FROM COLETAS WHERE [Unidade] = '" + unidade + "' AND [Data da Coleta] between (select dateadd(day, -15, getdate())) and (select dateadd(day, +0, getdate()));";
                    string Nomedoservidor = lbl_NomedoServidor.Text;


                    DataTable dtColetaImpressao = ConexaoBancodeDados.dql(vqueryColetaImpressao, Nomedoservidor);
                    for (int i = 0; i < dtColetaImpressao.Rows.Count; i++)
                    {
                        tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Data da Coleta"));
                        tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Catador"));
                        tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Massa"));
                        tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("TipodeResiduo"));
                        tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("TipodeMaterial"));
                        tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Unidade"));
                    }


                    doc.Add(tabela);
                    doc.Close();

                    System.Diagnostics.Process.Start(Globais.caminhoImpressao + @"\COLETAS_ULTIMAQUINZENA_" + output + ".pdf");

                }
            }
            catch
            {
                MessageBox.Show("Pasta 'Impressão' não encontrada.");
                return;

            }
        }

        //Impressão último dia
        private void btn_UltimoDia_Click(object sender, EventArgs e)
        {
            try
            {
                if (dgv_ColetasGestao.Rows.Count == 0)
                {
                    MessageBox.Show("Sem coletas a mostrar.");
                    return;
                }
                else
                {
                    string catadordata = lbl_Unidade.Text;
                    string nomeassociacao = lbl_Unidade.Text;
                    string ontem = "Último dia";
                    string nomedocatador = txt_NomeCatadorNome.Text;
                    string sernome = lbl_NomedoServidor.Text;
                    string nomeArquivo = Globais.caminhoImpressao + @"\COLETAS_" + nomedocatador + "ÚltimoDia.pdf";
                    FileStream arquivoPDF = new FileStream(nomeArquivo, FileMode.Create);
                    Document doc = new Document(PageSize.A4.Rotate(), 30f, 0f, 120f, 150f);
                    doc.SetPageSize(doc.PageSize);
                    PdfWriter escritorPDF = PdfWriter.GetInstance(doc, arquivoPDF);
                    escritorPDF.PageEvent = new CabecalhoCatadoreData(ontem, sernome, nomedocatador, nomeassociacao);


                    doc.Open();
                    string dados = "";



                    PdfPTable tabela = new PdfPTable(6);
                    tabela.TotalWidth = 500;


                    tabela.AddCell("Data da Coleta");
                    tabela.AddCell("Catador");
                    tabela.AddCell("Massa(Kg)");
                    tabela.AddCell("TipodeResiduo");
                    tabela.AddCell("TipodeMaterial");
                    tabela.AddCell("Associação");

                    string data = txt_DigitedaColeta.Text;
                    string vqueryColetaImpressao = @"select*from COLETAS where [Unidade] = '" + lbl_Unidade.Text + "' and [Data da Coleta] between (select dateadd(day, -2, getdate())) and (select dateadd(day, +1, getdate()));";
                    string Nomedoservidor = lbl_NomedoServidor.Text;

                    DataTable dtColetaImpressao = ConexaoBancodeDados.dql(vqueryColetaImpressao, Nomedoservidor);
                    for (int i = 0; i < dtColetaImpressao.Rows.Count; i++)
                    {
                        tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Data da Coleta"));
                        tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Catador"));
                        tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Massa"));
                        tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("TipodeResiduo"));
                        tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("TipodeMaterial"));
                        tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Unidade"));
                    }


                    doc.Add(tabela);
                    doc.Close();

                    System.Diagnostics.Process.Start(Globais.caminhoImpressao + @"\COLETAS_" + nomedocatador + "ÚltimoDia.pdf");

                }
            }
            catch
            {
                MessageBox.Show("Pasta 'Impressão' não encontrada.");
                return;

            }
        }

