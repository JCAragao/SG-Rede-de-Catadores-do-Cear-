                                                                                tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("TipodeResiduo"));
                                                                                tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("TipodeMaterial"));
                                                                                tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Unidade"));
                                                                            }


                                                                            doc.Add(tabela);
                                                                            doc.Close();

                                                                            System.Diagnostics.Process.Start(Globais.caminhoImpressao + @"\COLETAS_" + nomedocatador + "_EM_" + output + ".pdf");

                                                                        }


                                                                        if (btn_PesquisarUltimaSemana.Visible == true)
                                                                        {
                                                                            {
                                                                                if (dgv_ColetasGestao.Rows.Count == 0)
                                                                                {
                                                                                    MessageBox.Show("Sem coletas a mostrar.");
                                                                                    return;
                                                                                }
                                                                                else
                                                                                {
                                                                                    string catadordata = "Última Semana";
                                                                                    string output = string.Concat(catadordata.Where(Char.IsDigit));
                                                                                    string sernome = lbl_NomedoServidor.Text;
                                                                                    string nomedocatador = lbl_Unidade.Text;
                                                                                    string nomeassociacao = lbl_Unidade.Text;
                                                                                    string nomeArquivo = Globais.caminhoImpressao + @"\COLETAS_" + nomedocatador + "Última_Semana_" + output + ".pdf";
                                                                                    FileStream arquivoPDF = new FileStream(nomeArquivo, FileMode.Create);
                                                                                    Document doc = new Document(PageSize.A4.Rotate(), 30f, 0f, 120f, 150f);
                                                                                    doc.SetPageSize(doc.PageSize);
                                                                                    PdfWriter escritorPDF = PdfWriter.GetInstance(doc, arquivoPDF);
                                                                                    escritorPDF.PageEvent = new CabecalhoCatadoreData(catadordata, sernome, nomedocatador, nomeassociacao);


                                                                                    doc.Open();
                                                                                    string dados = "";



                                                                                    PdfPTable tabela = new PdfPTable(6);
                                                                                    tabela.TotalWidth = 500;


                                                                                    tabela.AddCell("Data da Coleta");
                                                                                    tabela.AddCell("Catador");
                                                                                    tabela.AddCell("Massa(Kg)");
                                                                                    tabela.AddCell("TipodeResiduo");
                                                                                    tabela.AddCell("TipodeMaterial");
                                                                                    tabela.AddCell("Associação");

                                                                                    string data = txt_DigitedaColeta.Text;
                                                                                    string vqueryColetaImpressao = @"select*from COLETAS where [Unidade] = '" + lbl_Unidade.Text + "' and [Data da Coleta] between (select dateadd(week, -1, getdate())) and (select dateadd(week, +0, getdate()));";
                                                                                    string Nomedoservidor = lbl_NomedoServidor.Text;

                                                                                    DataTable dtColetaImpressao = ConexaoBancodeDados.dql(vqueryColetaImpressao, Nomedoservidor);
                                                                                    for (int i = 0; i < dtColetaImpressao.Rows.Count; i++)
                                                                                    {
                                                                                        tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Data da Coleta"));
                                                                                        tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Catador"));
                                                                                        tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Massa"));
                                                                                        tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("TipodeResiduo"));
                                                                                        tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("TipodeMaterial"));
                                                                                        tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Unidade"));
                                                                                    }


                                                                                    doc.Add(tabela);
                                                                                    doc.Close();

                                                                                    System.Diagnostics.Process.Start(Globais.caminhoImpressao + @"\COLETAS_" + nomedocatador + "Última_Semana_" + output + ".pdf");

                                                                                }


                                                                                if (btn_PesquisarUltimoMes.Visible == true)
                                                                                {
                                                                                    {
                                                                                        if (dgv_ColetasGestao.Rows.Count == 0)
                                                                                        {
                                                                                            MessageBox.Show("Sem coletas a mostrar.");
                                                                                            return;
                                                                                        }
                                                                                        else
                                                                                        {
                                                                                            string catadordata = "Último Mês";
                                                                                            string output = string.Concat(DateTime.Now.ToString().Where(Char.IsDigit));
                                                                                            string sernome = lbl_NomedoServidor.Text;
                                                                                            string nomedocatador = lbl_Unidade.Text;
                                                                                            string nomeassociacao = lbl_Unidade.Text;
                                                                                            string nomeArquivo = Globais.caminhoImpressao + @"\COLETAS_" + nomedocatador + "_Último_Mês_Até_" + output + ".pdf";
                                                                                            FileStream arquivoPDF = new FileStream(nomeArquivo, FileMode.Create);
                                                                                            Document doc = new Document(PageSize.A4.Rotate(), 30f, 0f, 120f, 150f);
                                                                                            doc.SetPageSize(doc.PageSize);
                                                                                            PdfWriter escritorPDF = PdfWriter.GetInstance(doc, arquivoPDF);
                                                                                            escritorPDF.PageEvent = new CabecalhoCatadoreData(catadordata, sernome, nomedocatador, nomeassociacao);


                                                                                            doc.Open();
                                                                                            string dados = "";



                                                                                            PdfPTable tabela = new PdfPTable(6);
                                                                                            tabela.TotalWidth = 500;


                                                                                            tabela.AddCell("Data da Coleta");
                                                                                            tabela.AddCell("Catador");
                                                                                            tabela.AddCell("Massa(Kg)");
                                                                                            tabela.AddCell("TipodeResiduo");
                                                                                            tabela.AddCell("TipodeMaterial");
                                                                                            tabela.AddCell("Associação");

                                                                                            string data = txt_DigitedaColeta.Text;
                                                                                            string Nomedoservidor = lbl_NomedoServidor.Text;
                                                                                            string vqueryColetaImpressao = @"select*from COLETAS where [Unidade] = '" + lbl_Unidade.Text + "' and [Data da Coleta] between (select dateadd(month, -1, getdate())) and (select dateadd(month, +1, getdate())); ";


                                                                                            DataTable dtColetaImpressao = ConexaoBancodeDados.dql(vqueryColetaImpressao, Nomedoservidor);
                                                                                            for (int i = 0; i < dtColetaImpressao.Rows.Count; i++)
                                                                                            {
                                                                                                tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Data da Coleta"));
                                                                                                tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Catador"));
                                                                                                tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Massa"));
                                                                                                tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("TipodeResiduo"));
                                                                                                tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("TipodeMaterial"));
                                                                                                tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Unidade"));
                                                                                            }


                                                                                            doc.Add(tabela);
                                                                                            doc.Close();

                                                                                            System.Diagnostics.Process.Start(Globais.caminhoImpressao + @"\COLETAS_" + nomedocatador + "_Último_Mês_Até_" + output + ".pdf");

                                                                                        }

                                                                                    }

                                                                                }

                                                                            }

                                                                        }
                                                                    }
                                                                }


                                                            }
                                                        }
                                                    }

                                                }


                                            }
                                        }
                                    }
                                }

                            }
                        }
                    }
                }
            }
            catch
            {
                MessageBox.Show("Pasta 'Impressão' não encontrada");
                return;

            }
        }
        
        //Exclusão de coleta e atualização do Data Grid View
        private void btn_ExcluirEditarColetasGestao_Click(object sender, EventArgs e)
        {
            if (dgv_ColetasGestao.Rows.Count == 0)
            {
                MessageBox.Show("Sem coletas a mostrar.");
                return;

            }
            else
            {
                var Aviso = MessageBox.Show("Esta ação será irreversível. Deseja continuar?", "Exclusão de coleta", MessageBoxButtons.YesNo);

                if (Aviso == DialogResult.Yes)
                {
                    string IDdaColeta = this.dgv_ColetasGestao.CurrentRow.Cells[5].Value.ToString();
                    string ExclusaoColeta = @"DELETE FROM COLETAS WHERE [ID_Coleta] ='" + IDdaColeta + "';";
                    string Nomedoservidor = lbl_NomedoServidor.Text;
                    ConexaoBancodeDados.dql(ExclusaoColeta, Nomedoservidor);
                    MessageBox.Show("Coleta excluída com sucesso!");

                    if(btn_PesquisarUltimaSemana.Visible == true)
                    {
                        string vqueryColetaData = @"select*from COLETAS where [Unidade] = '" + lbl_Unidade.Text + "' and [Data da Coleta]  between (select dateadd(week, -1, getdate())) and (select dateadd(week, +0, gerdate()))" +
                            ", getdate()))\r\n";
                        dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryColetaData, Nomedoservidor);

                        if (dgv_ColetasGestao.RowCount != 0)
                        {
                            decimal TotalCatadorData = 0;
                            for (int i = 0; i < dgv_ColetasGestao.Rows.Count; ++i)
                            {
                                TotalCatadorData += Convert.ToDecimal(dgv_ColetasGestao.Rows[i].Cells[2].Value);

                            }
                            lbl_MassaTotalCatadorData.Visible = true;
                            lbl_MassaTotalCatadorData.Text = TotalCatadorData.ToString();
                        }


                    }

                    if (btn_PesquisarporAno.Visible == true)
                    {
                        string vqueryColetaData = @"select*from COLETAS where [Ano] = '" + txt_Ano.Text + "' and [Unidade] = '" + lbl_Unidade.Text + "' order by ID_Coleta ASC";
                        dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryColetaData, Nomedoservidor);

                        if (dgv_ColetasGestao.RowCount != 0)
                        {
                            decimal TotalCatadorData = 0;
                            for (int i = 0; i < dgv_ColetasGestao.Rows.Count; ++i)
                            {
                                TotalCatadorData += Convert.ToDecimal(dgv_ColetasGestao.Rows[i].Cells[2].Value);

                            }
                            lbl_MassaTotalCatadorData.Text = TotalCatadorData.ToString();
                            lbl_MassaTotalCatadorData.Visible = true;

                        }

                    }

                    if(btn_PesquisarMeseAno.Visible == true)
                    {
                        string vqueryColetaData = @"select*from COLETAS where [Mês] = '" + nud_Mes.Value.ToString() + "' and [Ano] = '" + txt_Ano.Text + "' and [Unidade] = '" + lbl_Unidade.Text + "' order by ID_Coleta ASC";
                        dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryColetaData, Nomedoservidor);

                        if (dgv_ColetasGestao.RowCount != 0)
                        {
                            decimal TotalCatadorData = 0;
                            for (int i = 0; i < dgv_ColetasGestao.Rows.Count; ++i)
                            {
                                TotalCatadorData += Convert.ToDecimal(dgv_ColetasGestao.Rows[i].Cells[2].Value);

                            }
                            lbl_MassaTotalCatadorData.Text = TotalCatadorData.ToString();
                            lbl_MassaTotalCatadorData.Visible = true;

                        }

                    }

                    if(btn_PorUltAnoPesquisar.Visible == true)
                    {
                        string vqueryColetaData = @"select*from COLETAS where [Unidade] = '" + lbl_Unidade.Text + "' and [Data da Coleta] between (select dateadd(year, -1, getdate())) and  (select dateadd(year, +1, getdate())) ;\r\n";
                        dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryColetaData, Nomedoservidor);

                        if (dgv_ColetasGestao.RowCount != 0)
                        {
                            decimal TotalCatadorData = 0;
                            for (int i = 0; i < dgv_ColetasGestao.Rows.Count; ++i)
                            {
                                TotalCatadorData += Convert.ToDecimal(dgv_ColetasGestao.Rows[i].Cells[2].Value);

                            }
                            lbl_MassaTotalCatadorData.Visible = true;
                            lbl_MassaTotalCatadorData.Text = TotalCatadorData.ToString();
                        }

                    }

                    if(btn_PesquisarporDoisMeses.Visible == true)
                    {
                        string vqueryColetaData = @"select*from COLETAS where [Unidade] = '" + lbl_Unidade.Text + "' and [Data da Coleta] between (select dateadd(month, -2, getdate())) and (select dateadd(month, +0, getdate()));";
                        dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryColetaData, Nomedoservidor);

                        if (dgv_ColetasGestao.RowCount != 0)
                        {
                            decimal TotalCatadorData = 0;
                            for (int i = 0; i < dgv_ColetasGestao.Rows.Count; ++i)
                            {
                                TotalCatadorData += Convert.ToDecimal(dgv_ColetasGestao.Rows[i].Cells[2].Value);

                            }
                            lbl_MassaTotalCatadorData.Visible = true;

                            lbl_MassaTotalCatadorData.Text = TotalCatadorData.ToString();
                        }


                    }

                    if(btn_PesquisarTresMeses.Visible == true)
                    {

                        string vqueryColetaData = @"select*from COLETAS where [Unidade] = '" + lbl_Unidade.Text + "' and [Data da Coleta] between (select dateadd(month, -3, getdate())) and (select dateadd(month, +0, getdate()));";
                        dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryColetaData, Nomedoservidor);

                        if (dgv_ColetasGestao.RowCount != 0)
                        {
                            decimal TotalCatadorData = 0;
                            for (int i = 0; i < dgv_ColetasGestao.Rows.Count; ++i)
                            {
                                TotalCatadorData += Convert.ToDecimal(dgv_ColetasGestao.Rows[i].Cells[2].Value);

                            }
                            lbl_MassaTotalCatadorData.Text = TotalCatadorData.ToString();
                            lbl_MassaTotalCatadorData.Visible = true;

                        }

                    }

                    if (btn_PesquisarporUltimaQuinzena.Visible == true)
                    {
                        string vqueryColetaData = @"select*from COLETAS where [Unidade] = '" + lbl_Unidade.Text + "' and [Data da Coleta] between (select dateadd(day, -15, getdate())) and (select dateadd(day, +0, getdate()));";
                        dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryColetaData, Nomedoservidor);

                        if (dgv_ColetasGestao.RowCount != 0)
                        {
                            decimal TotalCatadorData = 0;
                            for (int i = 0; i < dgv_ColetasGestao.Rows.Count; ++i)
                            {
                                TotalCatadorData += Convert.ToDecimal(dgv_ColetasGestao.Rows[i].Cells[2].Value);

                            }
                            lbl_MassaTotalCatadorData.Visible = true;

                            lbl_MassaTotalCatadorData.Text = TotalCatadorData.ToString();
                        }

                    }

                    if(btn_PesquisarUltimoMes.Visible == true)
                    {
                        string vqueryColetaData = @"select*from COLETAS where [Unidade] = '" + lbl_Unidade.Text + "' and [Data da Coleta] between (select dateadd(month, -1, getdate())) and (select dateadd(month, +1, getdate()));";
                        dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryColetaData, Nomedoservidor);

                        if (dgv_ColetasGestao.RowCount != 0)
                        {
                            decimal TotalCatadorData = 0;
                            for (int i = 0; i < dgv_ColetasGestao.Rows.Count; ++i)
                            {
                                TotalCatadorData += Convert.ToDecimal(dgv_ColetasGestao.Rows[i].Cells[2].Value);

                            }
                            lbl_MassaTotalCatadorData.Text = TotalCatadorData.ToString();
                            lbl_MassaTotalCatadorData.Visible = true;

                        }


                    }

                    if (btn_PesquisarporUltimoDia.Visible == true)
                    {
                        string vqueryColetaData = @"select*from COLETAS where [Unidade] = '" + lbl_Unidade.Text + "' and [Data da Coleta] between (select dateadd(day, -2, getdate())) and (select dateadd(day, +1, getdate()));";
                        dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryColetaData, Nomedoservidor);

                        if (dgv_ColetasGestao.RowCount != 0)
                        {
                            decimal TotalCatadorData = 0;
                            for (int i = 0; i < dgv_ColetasGestao.Rows.Count; ++i)
                            {
                                TotalCatadorData += Convert.ToDecimal(dgv_ColetasGestao.Rows[i].Cells[2].Value);

                            }
                            lbl_MassaTotalCatadorData.Text = TotalCatadorData.ToString();
                            lbl_MassaTotalCatadorData.Visible = true;

                        }

                    }

                    if (btn_PesquisarPorCatadorColetasGestao.Visible == true)
                    {
                        string Catador = txt_NomeCatador.Text;
                        string vqueryColetas = @"SELECT [ID_Coleta], [Data da Coleta], [Catador], [Massa], [MassaINT], [TipodeResiduo], [TipodeMaterial], [Unidade], [Administrador] FROM COLETAS WHERE  [Catador] ='" + Catador + "' ORDER BY [Data da Coleta] DESC ;";
                        dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryColetas, Nomedoservidor);


                        if (dgv_ColetasGestao.RowCount != 0)
                        {
                            decimal TotalCatadorData = 0;
                            for (int i = 0; i < dgv_ColetasGestao.Rows.Count; ++i)
                            {
                                TotalCatadorData += Convert.ToDecimal(dgv_ColetasGestao.Rows[i].Cells[9].Value);

                            }
                            lbl_MassaTotalCatadorData.Text = TotalCatadorData.ToString();
                        }

                    }

                    if (btn_PesquisarporDataColetasGestao.Visible == true)
                    {
                        if (txt_DigitedaColeta.Text != null)
                        {

                            string format = "dd/MM/yyyy";
                            string DatadeColeta = txt_DigitedaColeta.Text;
                            DateTime dateTime;
                            if (DateTime.TryParseExact(DatadeColeta, format, CultureInfo.InvariantCulture, DateTimeStyles.AllowWhiteSpaces, out dateTime)) 
                            {
                                string DatadaColeta = txt_DigitedaColeta.Text;
                                string Catador = txt_NomeCatador.Text;
                                string vqueryColetas = @"SELECT [ID_Coleta], [Data da Coleta], [Catador], [Massa], [MassaINT], [TipodeResiduo], [TipodeMaterial], [Unidade], [Administrador] FROM COLETAS WHERE [Data da Coleta] = '" + DatadaColeta + "' ORDER BY ID_Coleta ASC ;";
                                dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryColetas, Nomedoservidor);
                                this.dgv_ColetasGestao.Columns[7].Visible = false;
                                lbl_MassaTotalCatador.Visible = false;
                                lbl_MassaTotalCatadorData.Visible = true;
                                lbl_MassaTotalData.Visible = false;

                                if (dgv_ColetasGestao.RowCount != 0)
                                {
                                    decimal TotalCatadorData = 0;
                                    for (int i = 0; i < dgv_ColetasGestao.Rows.Count; ++i)
                                    {
                                        TotalCatadorData += Convert.ToDecimal(dgv_ColetasGestao.Rows[i].Cells[9].Value);

                                    }
                                    lbl_MassaTotalCatadorData.Text = TotalCatadorData.ToString();
                                }


                                btn_EditarColetasGestao.Visible = true;
                                btn_ExcluirEditarColetasGestao.Visible = true;
                            }
                            else
                            {
                                MessageBox.Show("Digite um formato válido de data.");
                                return;
                            }
                        }
                        else
                        {
                            MessageBox.Show("Digite a data da coleta ou realize uma pesquisa por data.");
                            return;

                        }

                    }
                }
                else
                {
                    return;
                }
            }
        }
        
        //Eventos de marcação do radiobutton porAssociação
        private void rdb_PorAssoicacao_CheckedChanged(object sender, EventArgs e)
        {
            txt_NomeCatador.Visible = false;
            txt_NomeCatadorNome.Visible = false;
            rdb_PorCatadorColetasGestao.Checked = false;
            btn_PesquisarporAssociacao.Visible = true;
            txt_NomeCatador.Enabled = false;
            lbl_CatadorColetasGestao.Visible = false;
            txt_DigitedaColeta.ReadOnly = true;
            txt_DigitedaColeta.BackColor = Color.LightGray;
            btn_PesquisarporAssociacaoeData.Visible = false;
            btn_PesquisarPorCatadorColetasGestao.Visible = false;
            btn_PesquisarporDataColetasGestao.Visible = false;
            button5.Visible = false;
        } 
        
        //Consultar a correspondência do número de matrícula ao nome do catador
        private void txt_Catador_KeyPress(object sender, KeyPressEventArgs e)
        {
            if (Convert.ToInt32(e.KeyChar) == 13 && txt_NomeCatador.Text != "")
            {
                e.Handled = true;


                string NomedoServidor = lbl_NomedoServidor.Text;
                SqlConnection Conectar = new SqlConnection("Data Source=DESKTOP-" + NomedoServidor + "\\SQLEXPRESS;Initial Catalog=SGRRC1;Integrated Security=True");
                Conectar.Open();
                string IDCatador = txt_NomeCatador.Text;
                SqlCommand cmd = new SqlCommand("select [Catador] from CATADORES WHERE [Matrícula] = '" + IDCatador + "'", Conectar);
                var Catador = cmd.ExecuteScalar();

                if (txt_NomeCatador.Text == "0")
                {
                    txt_NomeCatadorNome.Text = "Catador não associado";

                }

                if (Catador != null)
                {
                    txt_NomeCatadorNome.Text = Catador.ToString();
                    Conectar.Close();

                }
                else
                {
                    if (txt_NomeCatador.Text != "0")
                    {

                        MessageBox.Show("Catador não encontrado.");
                        Conectar.Close();
                        return;
                    }
                }

            }
        }
        
        //Atualização do Data Grid View após alteração dos dados.
        private void dgv_ColetasGestao_MouseEnter(object sender, EventArgs e)
        {
            btn_EditarColetasGestao.Enabled = true;

            if (dgv_ColetasGestao.RowCount != 0)
            {

                if (btn_PesquisarUltimaSemana.Visible == true)
                {
                    string Nomedoservidor = lbl_NomedoServidor.Text;
                    string vqueryColetaData = @"select*from COLETAS where [Unidade] = '" + lbl_Unidade.Text + "' and [Data da Coleta]  between (select dateadd(week, -1, getdate())) and (select dateadd(week, +0, getdate()))\r\n";
                    dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryColetaData, Nomedoservidor);

                    if (dgv_ColetasGestao.RowCount != 0)
                    {
                        decimal TotalCatadorData = 0;
                        for (int i = 0; i < dgv_ColetasGestao.Rows.Count; ++i)
                        {
                            TotalCatadorData += Convert.ToDecimal(dgv_ColetasGestao.Rows[i].Cells[2].Value);

                        }
                        lbl_MassaTotalCatadorData.Visible = true;
                        lbl_MassaTotalCatadorData.Text = TotalCatadorData.ToString();
                    }


                }

                if (btn_PesquisarporAno.Visible == true)
                {
                    string Nomedoservidor = lbl_NomedoServidor.Text;
                    string vqueryColetaData = @"select*from COLETAS where [Ano] = '" + txt_Ano.Text + "' and [Unidade] = '" + lbl_Unidade.Text + "' order by ID_Coleta ASC";
                    dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryColetaData, Nomedoservidor);

                    if (dgv_ColetasGestao.RowCount != 0)
                    {
                        decimal TotalCatadorData = 0;
                        for (int i = 0; i < dgv_ColetasGestao.Rows.Count; ++i)
                        {
                            TotalCatadorData += Convert.ToDecimal(dgv_ColetasGestao.Rows[i].Cells[2].Value);

                        }
                        lbl_MassaTotalCatadorData.Text = TotalCatadorData.ToString();
                        lbl_MassaTotalCatadorData.Visible = true;

                    }

                }

                if (btn_PesquisarMeseAno.Visible == true)
                {
                    string Nomedoservidor = lbl_NomedoServidor.Text;
                    string vqueryColetaData = @"select*from COLETAS where [Mês] = '" + nud_Mes.Value.ToString() + "' and [Ano] = '" + txt_Ano.Text + "' and [Unidade] = '" + lbl_Unidade.Text + "' order by ID_Coleta ASC";
                    dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryColetaData, Nomedoservidor);

                    if (dgv_ColetasGestao.RowCount != 0)
                    {
                        decimal TotalCatadorData = 0;
                        for (int i = 0; i < dgv_ColetasGestao.Rows.Count; ++i)
                        {
                            TotalCatadorData += Convert.ToDecimal(dgv_ColetasGestao.Rows[i].Cells[2].Value);

                        }
                        lbl_MassaTotalCatadorData.Text = TotalCatadorData.ToString();
                        lbl_MassaTotalCatadorData.Visible = true;

                    }

                }

                if (btn_PorUltAnoPesquisar.Visible == true)
                {
                    string Nomedoservidor = lbl_NomedoServidor.Text;
                    string vqueryColetaData = @"select*from COLETAS where [Unidade] = '" + lbl_Unidade.Text + "' and [Data da Coleta] between (select dateadd(year, -1, getdate())) and  (select dateadd(year, +1, getdate())) ;\r\n";
                    dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryColetaData, Nomedoservidor);

                    if (dgv_ColetasGestao.RowCount != 0)
                    {
                        decimal TotalCatadorData = 0;
                        for (int i = 0; i < dgv_ColetasGestao.Rows.Count; ++i)
                        {
                            TotalCatadorData += Convert.ToDecimal(dgv_ColetasGestao.Rows[i].Cells[2].Value);

                        }
                        lbl_MassaTotalCatadorData.Visible = true;
                        lbl_MassaTotalCatadorData.Text = TotalCatadorData.ToString();
                    }

                }

                if (btn_PesquisarporDoisMeses.Visible == true)
                {
                    string Nomedoservidor = lbl_NomedoServidor.Text;
                    string vqueryColetaData = @"select*from COLETAS where [Unidade] = '" + lbl_Unidade.Text + "' and [Data da Coleta] between (select dateadd(month, -2, getdate())) and (select dateadd(month, +0, getdate()));";
                    dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryColetaData, Nomedoservidor);

                    if (dgv_ColetasGestao.RowCount != 0)
                    {
                        decimal TotalCatadorData = 0;
                        for (int i = 0; i < dgv_ColetasGestao.Rows.Count; ++i)
                        {
                            TotalCatadorData += Convert.ToDecimal(dgv_ColetasGestao.Rows[i].Cells[2].Value);

                        }
                        lbl_MassaTotalCatadorData.Visible = true;

                        lbl_MassaTotalCatadorData.Text = TotalCatadorData.ToString();
                    }


                }

                if (btn_PesquisarTresMeses.Visible == true)
                {
                    string Nomedoservidor = lbl_NomedoServidor.Text;
                    string vqueryColetaData = @"select*from COLETAS where [Unidade] = '" + lbl_Unidade.Text + "' and [Data da Coleta] between (select dateadd(month, -3, getdate())) and (select dateadd(month, +0, getdate()));";
                    dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryColetaData, Nomedoservidor);

                    if (dgv_ColetasGestao.RowCount != 0)
                    {
                        decimal TotalCatadorData = 0;
                        for (int i = 0; i < dgv_ColetasGestao.Rows.Count; ++i)
                        {
                            TotalCatadorData += Convert.ToDecimal(dgv_ColetasGestao.Rows[i].Cells[2].Value);

                        }
                        lbl_MassaTotalCatadorData.Text = TotalCatadorData.ToString();
                        lbl_MassaTotalCatadorData.Visible = true;

                    }

                }

                if (btn_PesquisarporUltimaQuinzena.Visible == true)
                {
                    string Nomedoservidor = lbl_NomedoServidor.Text;
                    string vqueryColetaData = @"select*from COLETAS where [Unidade] = '" + lbl_Unidade.Text + "' and [Data da Coleta] between (select dateadd(day, -15, getdate())) and (select dateadd(day, +0, getdate()));";
                    dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryColetaData, Nomedoservidor);

                    if (dgv_ColetasGestao.RowCount != 0)
                    {
                        decimal TotalCatadorData = 0;
                        for (int i = 0; i < dgv_ColetasGestao.Rows.Count; ++i)
                        {
                            TotalCatadorData += Convert.ToDecimal(dgv_ColetasGestao.Rows[i].Cells[2].Value);

                        }
                        lbl_MassaTotalCatadorData.Visible = true;

                        lbl_MassaTotalCatadorData.Text = TotalCatadorData.ToString();
                    }

                }

                if (btn_PesquisarUltimoMes.Visible == true)
                {
                    string Nomedoservidor = lbl_NomedoServidor.Text;
                    string vqueryColetaData = @"select*from COLETAS where [Unidade] = '" + lbl_Unidade.Text + "' and [Data da Coleta] between (select dateadd(month, -1, getdate())) and (select dateadd(month, +1, getdate()));";
                    dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryColetaData, Nomedoservidor);

                    if (dgv_ColetasGestao.RowCount != 0)
                    {
                        decimal TotalCatadorData = 0;
                        for (int i = 0; i < dgv_ColetasGestao.Rows.Count; ++i)
                        {
                            TotalCatadorData += Convert.ToDecimal(dgv_ColetasGestao.Rows[i].Cells[2].Value);

                        }
                        lbl_MassaTotalCatadorData.Text = TotalCatadorData.ToString();
                        lbl_MassaTotalCatadorData.Visible = true;

                    }


                }

                if (btn_PesquisarporUltimoDia.Visible == true)
                {
                    string Nomedoservidor = lbl_NomedoServidor.Text;
                    string vqueryColetaData = @"select*from COLETAS where [Unidade] = '" + lbl_Unidade.Text + "' and [Data da Coleta] between (select dateadd(day, -2, getdate())) and (select dateadd(day, +1, getdate()));";
                    dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryColetaData, Nomedoservidor);

                    if (dgv_ColetasGestao.RowCount != 0)
                    {
                        decimal TotalCatadorData = 0;
                        for (int i = 0; i < dgv_ColetasGestao.Rows.Count; ++i)
                        {
                            TotalCatadorData += Convert.ToDecimal(dgv_ColetasGestao.Rows[i].Cells[2].Value);

                        }
                        lbl_MassaTotalCatadorData.Text = TotalCatadorData.ToString();
                        lbl_MassaTotalCatadorData.Visible = true;

                    }

                }

                if (btn_PesquisarPorCatadorColetasGestao.Visible == true)
                {
                    string Nomedoservidor = lbl_NomedoServidor.Text;
                    string Catador = txt_NomeCatador.Text;
                    string vqueryColetas = @"SELECT [ID_Coleta], [Data da Coleta], [Catador], [Massa], [MassaINT], [TipodeResiduo], [TipodeMaterial], [Unidade], [Administrador] FROM COLETAS WHERE  [Catador] ='" + Catador + "' ORDER BY [Data da Coleta] DESC ;";
                    dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryColetas, Nomedoservidor);


                    if (dgv_ColetasGestao.RowCount != 0)
                    {
                        decimal TotalCatadorData = 0;
                        for (int i = 0; i < dgv_ColetasGestao.Rows.Count; ++i)
                        {
                            TotalCatadorData += Convert.ToDecimal(dgv_ColetasGestao.Rows[i].Cells[9].Value);

                        }
                        lbl_MassaTotalCatadorData.Text = TotalCatadorData.ToString();
                    }

                }

                if (btn_PesquisarporDataColetasGestao.Visible == true)
                {
                    if (txt_DigitedaColeta.Text != null)
                    {

                        string format = "dd/MM/yyyy";
                        string DatadeColeta = txt_DigitedaColeta.Text;
                        DateTime dateTime;
                        string Nomedoservidor = lbl_NomedoServidor.Text;
                        if (DateTime.TryParseExact(DatadeColeta, format, CultureInfo.InvariantCulture, DateTimeStyles.AllowWhiteSpaces, out dateTime))
                        {
                            string DatadaColeta = txt_DigitedaColeta.Text;
                            string vqueryColetas = @"SELECT [ID_Coleta], [Data da Coleta], [Catador], [Massa], [MassaINT], [TipodeResiduo], [TipodeMaterial], [Unidade], [Administrador] FROM COLETAS WHERE [Data da Coleta] = '" + DatadaColeta + "' ORDER BY Catador ASC ;";
                            dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryColetas, Nomedoservidor);


                            this.dgv_ColetasGestao.Columns[7].Visible = false;
                            lbl_MassaTotalCatador.Visible = false;
                            lbl_MassaTotalCatadorData.Visible = true;
                            lbl_MassaTotalData.Visible = false;

                            if (dgv_ColetasGestao.RowCount != 0)
                            {
                                decimal TotalCatadorData = 0;
                                for (int i = 0; i < dgv_ColetasGestao.Rows.Count; ++i)
                                {
                                    TotalCatadorData += Convert.ToDecimal(dgv_ColetasGestao.Rows[i].Cells[9].Value);

                                }
                                lbl_MassaTotalCatadorData.Text = TotalCatadorData.ToString();
                            }


                            btn_EditarColetasGestao.Visible = true;
                            btn_ExcluirEditarColetasGestao.Visible = true;
                        }

                    }

                }
            }
        }
        
        //Eventos de busca por semana
        private void btn_PesquisarUltimaSemana_Click(object sender, EventArgs e)
        {

                string Nomedoservidor = lbl_NomedoServidor.Text;
                string vqueryColetaData = @"select*from COLETAS where [Unidade] = '"+lbl_Unidade.Text+ "' and [Data da Coleta]  between (select dateadd(week, -1, getdate())) and (select dateadd(week, +0, getdate()))\r\n";
                dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryColetaData, Nomedoservidor);

            if (dgv_ColetasGestao.RowCount != 0)
            {
                decimal TotalCatadorData = 0;
                for (int i = 0; i < dgv_ColetasGestao.Rows.Count; ++i)
                {
                    TotalCatadorData += Convert.ToDecimal(dgv_ColetasGestao.Rows[i].Cells[2].Value);

                }
                lbl_MassaTotalCatadorData.Visible = true;
                lbl_MassaTotalCatadorData.Text = TotalCatadorData.ToString();
            }
        }
        
