    public partial class ColetasGestao : Form
    {

        public static string ID_Selecionado;
        public ColetasGestao()
        {
            InitializeComponent();
        }

        //Carregamento do Data Grid View
        private void ColetasGestao_Load(object sender, EventArgs e)
        {
          
            string DatadaColeta = txt_DigitedaColeta.Text;
            string Catador = txt_NomeCatador.Text;
            string Nomedoservidor = lbl_NomedoServidor.Text;

            string vqueryColetas = @"SELECT [ID_Coleta], [Data da Coleta], [Catador], [Massa], [TipodeResiduo], [TipodeMaterial], [Cidade], [Reciclavel] FROM COLETAS WHERE [Data da Coleta] ='" + DatadaColeta + "' and [Catador] = '" + Catador + "' ORDER BY ID_Coleta ASC;";
            dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryColetas, Nomedoservidor);



           
            this.dgv_ColetasGestao.Columns[7].Visible = false;

          
        }     
      
        //Eventos de busca por associado e data da coleta.
        private void button5_Click(object sender, EventArgs e)
        {
            if (txt_DigitedaColeta.Text != null)
            {
                string Nomedoservidor = lbl_NomedoServidor.Text;
                string format = "dd/MM/yyyy";
                string DatadeColeta = txt_DigitedaColeta.Text;
                DateTime dateTime;
                if (DateTime.TryParseExact(DatadeColeta, format, CultureInfo.InvariantCulture, DateTimeStyles.AllowWhiteSpaces, out dateTime))
                {
                    string DatadaColeta = txt_DigitedaColeta.Text;
                    string Catador = txt_NomeCatador.Text;
                    string vqueryColetas = @"SELECT [ID_Coleta], [Data da Coleta], [Catador], [Massa], [MassaINT], [TipodeResiduo], [TipodeMaterial], [Unidade], [Administrador] FROM COLETAS WHERE [Data da Coleta] ='" + DatadaColeta + "' and [Catador] = '" + Catador + "' ORDER BY ID_Coleta ASC ;";
                    dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryColetas, Nomedoservidor);
                    this.dgv_ColetasGestao.Columns[7].Visible = false;
                    lbl_MassaTotalCatador.Visible = false;
                    lbl_MassaTotalCatadorData.Visible = true;
                    lbl_MassaTotalData.Visible = false;

                    decimal TotalCatadorData = 0;
                    for (int i = 0; i < dgv_ColetasGestao.Rows.Count; ++i)
                    {
                        TotalCatadorData += Convert.ToDecimal(dgv_ColetasGestao.Rows[i].Cells[9].Value);

                    }
                    lbl_MassaTotalCatadorData.Text = TotalCatadorData.ToString();


                    btn_EditarColetasGestao.Visible = true;
                    btn_ExcluirEditarColetasGestao.Visible = true;
                }
                else
                {
                    MessageBox.Show("Digite um formato válido de data.");
                    return;
                }
            }
            else
            {
                MessageBox.Show("Digite a data da coleta ou realize uma pesquisa por data.");
                return;

            }
        }
        
        //Pesquisa por data da coleta tecla Entra
        private void txt_DigitedaColeta_KeyPress(object sender, KeyPressEventArgs e)
        {
            {
                if ((!char.IsNumber(e.KeyChar)) && !char.IsControl(e.KeyChar) && e.KeyChar != '/' && !char.IsWhiteSpace(e.KeyChar))
                {
                    e.Handled = true;
                }

                if (txt_DigitedaColeta.Text != "" && Convert.ToInt32(e.KeyChar) == 13)
                {
                    e.Handled = true;
                    string format = "dd/MM/yyyy";
                    string DatadeColeta = txt_DigitedaColeta.Text;
                    string Nomedoservidor = lbl_NomedoServidor.Text;
                    DateTime dateTime;
                    if (DateTime.TryParseExact(DatadeColeta, format, CultureInfo.InvariantCulture, DateTimeStyles.AllowWhiteSpaces, out dateTime))
                    {
                        string DatadaColeta = txt_DigitedaColeta.Text;
                        string Catador = txt_NomeCatadorNome.Text;
                        string vqueryColetas = @"SELECT [ID_Coleta], [Data da Coleta], [Catador], [Massa], [TipodeResiduo], [TipodeMaterial], [Cidade], [Reciclavel], [Bairro] FROM COLETAS WHERE [Data da Coleta] ='" + DatadaColeta + "' and [Catador] ='" + Catador + "' ORDER BY ID_Coleta ASC ;";
                        dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryColetas, Nomedoservidor);
                    }
                    else
                    {
                        MessageBox.Show("Digite um formato válido de data.");
                        return;
                    }
                }
            }
        }
        
        //Carregamento do formulário de editar coleta e transferência de dados.
        public void btn_EditarColetasGestao_Click(object sender, EventArgs e)
        {
            if (dgv_ColetasGestao.Rows.Count == 0)
            {
                MessageBox.Show("Sem coletas a mostrar.");
                return;

            }
            else
            {
                btn_EditarColetasGestao.Enabled = false;
                ColetasEditar EditarColetas = new ColetasEditar(this);
                EditarColetas.lbl_NomedoServidor.Text = lbl_NomedoServidor.Text;
                EditarColetas.txt_DatadaColetaColetasEditar.Text = this.dgv_ColetasGestao.CurrentRow.Cells[0].Value.ToString();
                EditarColetas.txt_CatadorRegistro.Text = this.dgv_ColetasGestao.CurrentRow.Cells[1].Value.ToString();
                EditarColetas.txt_MassaColetasEditar.Text = this.dgv_ColetasGestao.CurrentRow.Cells[2].Value.ToString();
                EditarColetas.cbb_TipodeResiduoColetasEditar.Text = this.dgv_ColetasGestao.CurrentRow.Cells[3].Value.ToString();
                EditarColetas.cbb_TipodeMaterialColetasEditar.Text = this.dgv_ColetasGestao.CurrentRow.Cells[4].Value.ToString();
                EditarColetas.txt_RegistrodaColetaColetasEditar.Text = this.dgv_ColetasGestao.CurrentRow.Cells[5].Value.ToString();
                EditarColetas.lbl_UnidadeUsuarioColeta.Text = this.dgv_ColetasGestao.CurrentRow.Cells[6].Value.ToString();
                EditarColetas.lbl_UsuarioColeta.Text = this.dgv_ColetasGestao.CurrentRow.Cells[7].Value.ToString();
                EditarColetas.Show();
            }
        }       
       
        //Eventos de marcação do radiobutton por Catador
        private void rdb_PorCatadorColetasGestao_CheckedChanged(object sender, EventArgs e)
        {
            button5.Visible = false;
            btn_PesquisarporDataColetasGestao.Visible = false;
            btn_PesquisarPorCatadorColetasGestao.Visible = true;
            btn_PesquisarporAssociacao.Visible = false;
            btn_PesquisarporAssociacaoeData.Visible = false;
            txt_Ano.Visible = false;
            nud_Mes.Visible = false;
            btn_PesquisarMeseAno.Visible = false;
            btn_PesquisarporAno.Visible = false;
            btn_PesquisarporAssociacao.Visible = false;
            btn_PesquisarporAssociacaoeData.Visible = false;
            btn_PesquisarporDataColetasGestao.Visible = false;
            btn_PesquisarporDoisMeses.Visible = false;
            btn_PesquisarporUltimaQuinzena.Visible = false;
            btn_PorUltAnoPesquisar.Visible = false;
            btn_PesquisarporUltimoDia.Visible = false;
            btn_PesquisarTresMeses.Visible = false;
            btn_PesquisarUltimaSemana.Visible = false;
            btn_PesquisarUltimoMes.Visible = false;   
            txt_DigitedaColeta.ReadOnly = true;
            txt_DigitedaColeta.BackColor = Color.LightGray;
            txt_NomeCatador.Enabled = true;
            txt_NomeCatador.Visible = true;
            txt_NomeCatadorNome.Visible = true;
            lbl_CatadorColetasGestao.Visible = true;
            cbb_Pesquisa.Visible = false;
         
           
        }
        
        //Eventos de marcação do radiobutton por Data da Coleta
        private void rdb_porData_ColetasGestao_CheckedChanged(object sender, EventArgs e)
        {
            button5.Visible = false;
            txt_DigitedaColeta.ReadOnly = false;
            txt_DigitedaColeta.Enabled = true;
            txt_DigitedaColeta.BackColor = Color.White;
            btn_PesquisarporDataColetasGestao.Visible = true;
            btn_PesquisarPorCatadorColetasGestao.Visible = false;
            txt_NomeCatador.Enabled = false;
        }
        
        //Eventos de busca por Data específica da coleta
        private void btn_PesquisarporDataColetasGestao_Click(object sender, EventArgs e)
        {

            string format = "dd/MM/yyyy";
            string DatadeColeta = txt_DigitedaColeta.Text;
            DateTime dateTime;

            string Nomedoservidor = lbl_NomedoServidor.Text;
            if (DateTime.TryParseExact(DatadeColeta, format, CultureInfo.InvariantCulture, DateTimeStyles.AllowWhiteSpaces, out dateTime))
            {
                string DatadaColeta = txt_DigitedaColeta.Text;
                string vqueryColetas = @"SELECT [ID_Coleta], [Data da Coleta], [Catador], [Massa], [MassaINT], [TipodeResiduo], [TipodeMaterial], [Unidade], [Administrador] FROM COLETAS WHERE [Data da Coleta] = '"+DatadaColeta+"' ORDER BY Catador ASC ;";
                dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryColetas, Nomedoservidor);
              
                btn_EditarColetasGestao.Visible = true;
                btn_ExcluirEditarColetasGestao.Visible = true;
                this.dgv_ColetasGestao.Columns[7].Visible = false;
                lbl_MassaTotalCatador.Visible = false;
                lbl_MassaTotalCatadorData.Visible = false;
                lbl_MassaTotalData.Visible = true;

                decimal TotalData = 0;
                for (int i = 0; i < dgv_ColetasGestao.Rows.Count; ++i)
                {
                    TotalData += Convert.ToDecimal(dgv_ColetasGestao.Rows[i].Cells[9].Value);

                }
                lbl_MassaTotalData.Text = TotalData.ToString();

            }
            else
            {
                MessageBox.Show("Digite um valor válido de data. Ex: 07/02/1998.");
                return;

            }
        }
        
        //Eventos de busca por matrícula de catador
        private void btn_PesquisarPorCatadorColetasGestao_Click(object sender, EventArgs e)
        {
            string Nomedoservidor = lbl_NomedoServidor.Text;
            string Catador = txt_NomeCatador.Text;
            string vqueryColetas = @"SELECT [ID_Coleta], [Data da Coleta], [Catador], [Massa], [MassaINT], [TipodeResiduo], [TipodeMaterial], [Unidade], [Administrador] FROM COLETAS WHERE  [Catador] ='" + Catador + "' ORDER BY [Data da Coleta] DESC ;";
            dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryColetas, Nomedoservidor);

            btn_ExcluirEditarColetasGestao.Visible = true;
            btn_EditarColetasGestao.Visible = true;
            this.dgv_ColetasGestao.Columns[7].Visible = false;
            lbl_MassaTotalCatador.Visible = true;
            lbl_MassaTotalCatadorData.Visible = false;
            lbl_MassaTotalData.Visible = false;

            decimal TotalCatador = 0;
            for (int i = 0; i < dgv_ColetasGestao.Rows.Count; ++i)
            {
                TotalCatador += Convert.ToDecimal(dgv_ColetasGestao.Rows[i].Cells[2].Value);

            }
            lbl_MassaTotalCatador.Text = TotalCatador.ToString();

        }
        
        //Todos os eventos de impressão (Maioria)
        private void btn_ImprimirColetasDGestao_Click(object sender, EventArgs e)
        {
          try
           { 
            if (dgv_ColetasGestao.Rows.Count == 0)
            {
                MessageBox.Show("Sem coletas a mostrar.");
                return;

            }
            else
            {

                if (btn_PesquisarporDataColetasGestao.Visible == true)
                {

                    string data = txt_DigitedaColeta.Text;
                    string dataout = string.Concat(data.Where(Char.IsDigit));


                    string nomeArquivo = Globais.caminhoImpressao + @"\COLETA'" + dataout + "'.pdf";
                    FileStream arquivoPDF = new FileStream(nomeArquivo, FileMode.Create);
                    Document doc = new Document(PageSize.A4.Rotate(), 30f, 0f, 120f, 150f);
                    doc.SetPageSize(doc.PageSize);
                    PdfWriter escritorPDF = PdfWriter.GetInstance(doc, arquivoPDF);
                    escritorPDF.PageEvent = new HeaderFooter(data);
                    doc.Open();

                    PdfPTable tabela = new PdfPTable(6);
                    tabela.TotalWidth = 500;
                    tabela.SpacingBefore = 50;

                    tabela.AddCell("Data da Coleta");
                    tabela.AddCell("Catador");
                    tabela.AddCell("Massa(Kg)");
                    tabela.AddCell("TipodeResiduo");
                    tabela.AddCell("TipodeMaterial");
                    tabela.AddCell("Associação");

                    string Data = txt_DigitedaColeta.Text;
                    string vqueryColetaImpressao = @"SELECT [ID_Coleta], [Data da Coleta], [Catador], [Massa], [TipodeResiduo], [TipodeMaterial], [Unidade] FROM COLETAS WHERE [Data da Coleta] ='" + Data + "' AND [Unidade] = '" + lbl_Unidade.Text + "' ORDER BY Catador ASC ;";
                    string Nomedoservidor = lbl_NomedoServidor.Text;

                    DataTable dtColetaImpressao = ConexaoBancodeDados.dql(vqueryColetaImpressao, Nomedoservidor);
                    for (int i = 0; i < dtColetaImpressao.Rows.Count; i++)
                    {
                        tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Data da Coleta"));
                        tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Catador"));
                        tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Massa"));
                        tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("TipodeResiduo"));
                        tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("TipodeMaterial"));
                        tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Unidade"));


                        if (dtColetaImpressao.Rows.Count == 0)
                        {
                            MessageBox.Show("Data sem coletas!");
                            return;

                        }
                    }

                    doc.Add(tabela);
                    doc.Close();

                    System.Diagnostics.Process.Start(Globais.caminhoImpressao + @"\COLETA'" + dataout + "'.pdf");


                }

                if (dgv_ColetasGestao.Rows.Count == 0)
                {
                    MessageBox.Show("Sem coletas a mostrar.");
                    return;

                }
                else
                {

                    if (btn_PesquisarPorCatadorColetasGestao.Visible == true)

                    {

                        string catnome = txt_NomeCatadorNome.Text;
                        string assnome = lbl_Unidade.Text;
                        string servnomev = lbl_NomedoServidor.Text;
                        string nomeArquivo = Globais.caminhoImpressao + @"\HISTÓRICO_" + catnome + ".pdf";
                        FileStream arquivoPDF = new FileStream(nomeArquivo, FileMode.Create);
                        Document doc = new Document(PageSize.A4.Rotate(), 30f, 0f, 120f, 150f);
                        doc.SetPageSize(doc.PageSize);
                        PdfWriter escritorPDF = PdfWriter.GetInstance(doc, arquivoPDF);
                        escritorPDF.PageEvent = new Cabecalho(catnome, assnome, servnomev);

                        doc.Open();
                        string dados = "";

                        PdfPTable tabela = new PdfPTable(6);
                        tabela.TotalWidth = 500;
                        tabela.SpacingBefore = 50;

                        tabela.AddCell("Data da Coleta");
                        tabela.AddCell("Catador");
                        tabela.AddCell("Massa(Kg)");
                        tabela.AddCell("TipodeResiduo");
                        tabela.AddCell("TipodeMaterial");
                        tabela.AddCell("Associação");

                        string Catador = txt_NomeCatador.Text;
                        string vqueryColetaImpressao = @"SELECT [ID_Coleta], [Data da Coleta], [Catador], [Massa], [TipodeResiduo], [TipodeMaterial], [Unidade] FROM COLETAS WHERE [Catador] ='" + Catador + "' and [Unidade] = '" + lbl_Unidade.Text + "' ORDER BY Catador ASC ;";
                        string Nomedoservidor = lbl_NomedoServidor.Text;

                        DataTable dtColetaImpressao = ConexaoBancodeDados.dql(vqueryColetaImpressao, Nomedoservidor);
                        for (int i = 0; i < dtColetaImpressao.Rows.Count; i++)
                        {
                            tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Data da Coleta"));
                            tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Catador"));
                            tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Massa"));
                            tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("TipodeResiduo"));
                            tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("TipodeMaterial"));
                            tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Unidade"));

                            if (dtColetaImpressao.Rows.Count == 0)
                            {
                                MessageBox.Show("Catador não possui coleta!");
                                return;

                            }
                        }
                        doc.Add(tabela);
                        doc.Close();

                        System.Diagnostics.Process.Start(Globais.caminhoImpressao + @"\HISTÓRICO_" + catnome + ".pdf");

                    }
                }


                if (button5.Visible == true)
                {
                    if (dgv_ColetasGestao.Rows.Count == 0)
                    {
                        MessageBox.Show("Sem coletas a mostrar.");
                        return;
                    }
                    else
                    {
                        string catadordata = txt_DigitedaColeta.Text;
                        string output = string.Concat(catadordata.Where(Char.IsDigit));
                        string nomeassociacao = lbl_Unidade.Text;
                        string nomedocatador = txt_NomeCatadorNome.Text;
                        string sernome = lbl_NomedoServidor.Text;
                        string nomeArquivo = Globais.caminhoImpressao + @"\COLETAS_" + nomedocatador + "_EM_" + output + ".pdf";
                        FileStream arquivoPDF = new FileStream(nomeArquivo, FileMode.Create);
                        Document doc = new Document(PageSize.A4.Rotate(), 30f, 0f, 120f, 150f);
                        doc.SetPageSize(doc.PageSize);
                        PdfWriter escritorPDF = PdfWriter.GetInstance(doc, arquivoPDF);
                        escritorPDF.PageEvent = new CabecalhoCatadoreData(catadordata, sernome, nomedocatador, nomeassociacao);


                        doc.Open();
                        string dados = "";



                        PdfPTable tabela = new PdfPTable(6);
                        tabela.TotalWidth = 500;


                        tabela.AddCell("Data da Coleta");
                        tabela.AddCell("Catador");
                        tabela.AddCell("Massa(Kg)");
                        tabela.AddCell("TipodeResiduo");
                        tabela.AddCell("TipodeMaterial");
                        tabela.AddCell("Associação");

                        string data = txt_DigitedaColeta.Text;
                        string Catador = txt_NomeCatador.Text;
                        string vqueryColetaImpressao = @"SELECT [ID_Coleta], [Data da Coleta], [Catador], [Massa], [TipodeResiduo], [TipodeMaterial], [Unidade], [Administrador] FROM COLETAS WHERE [Data da Coleta] = '" + data + "' and [Catador] = '" + Catador + "' and [Unidade] = '" + lbl_Unidade.Text + "' ORDER BY ID_Coleta ASC ;";
                        string Nomedoservidor = lbl_NomedoServidor.Text;

                        DataTable dtColetaImpressao = ConexaoBancodeDados.dql(vqueryColetaImpressao, Nomedoservidor);
                        for (int i = 0; i < dtColetaImpressao.Rows.Count; i++)
                        {
                            tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Data da Coleta"));
                            tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Catador"));
                            tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Massa"));
                            tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("TipodeResiduo"));
                            tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("TipodeMaterial"));
                            tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Unidade"));
                        }


                        doc.Add(tabela);

                        doc.Close();

                        System.Diagnostics.Process.Start(Globais.caminhoImpressao + @"\COLETAS_" + nomedocatador + "_EM_" + output + ".pdf");

                    }

                }


                if (btn_PesquisarMeseAno.Visible == true)
                {
                    {
                        if (dgv_ColetasGestao.Rows.Count == 0)
                        {
                            MessageBox.Show("Sem coletas a mostrar.");
                            return;
                        }
                        else
                        {
                            string associacao = lbl_Unidade.Text;
                            string Meseano = nud_Mes.Value.ToString() + txt_Ano.Text;
                            string output = string.Concat(Meseano.Where(Char.IsDigit));
                            string nomeassociacao = lbl_Unidade.Text;
                            string nomedocatador = txt_NomeCatadorNome.Text;
                            string sernome = lbl_NomedoServidor.Text;
                            string nomecabecalhodata = nud_Mes.Value.ToString() + "/" + txt_Ano.Text;
                            string nomeArquivo = Globais.caminhoImpressao + @"\COLETAS_" + associacao + "_DE_" +output + ".pdf";
                            FileStream arquivoPDF = new FileStream(nomeArquivo, FileMode.Create);
                            Document doc = new Document(PageSize.A4.Rotate(), 30f, 0f, 120f, 150f);
                            doc.SetPageSize(doc.PageSize);
                            PdfWriter escritorPDF = PdfWriter.GetInstance(doc, arquivoPDF);
                            escritorPDF.PageEvent = new CabecalhoCatadoreData(nomecabecalhodata, sernome, nomedocatador, nomeassociacao);


                            doc.Open();
                            string dados = "";



                            PdfPTable tabela = new PdfPTable(6);
                            tabela.TotalWidth = 500;


                            tabela.AddCell("Data da Coleta");
                            tabela.AddCell("Catador");
                            tabela.AddCell("Massa(Kg)");
                            tabela.AddCell("TipodeResiduo");
                            tabela.AddCell("TipodeMaterial");
                            tabela.AddCell("Associação");

                            string data = nud_Mes.Value.ToString();
                            string Catador = txt_Ano.Text;
                            string vqueryColetaImpressao = @"SELECT [ID_Coleta], [Data da Coleta], [Catador], [Massa], [TipodeResiduo], [TipodeMaterial], [Unidade], [Administrador] FROM COLETAS WHERE [Mês] = '" + data + "' and [Ano] = '" + Catador + "' AND [Unidade] = '" + lbl_Unidade.Text + "' ORDER BY ID_Coleta ASC ;";
                            string Nomedoservidor = lbl_NomedoServidor.Text;

                            DataTable dtColetaImpressao = ConexaoBancodeDados.dql(vqueryColetaImpressao, Nomedoservidor);
                            for (int i = 0; i < dtColetaImpressao.Rows.Count; i++)
                            {
                                tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Data da Coleta"));
                                tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Catador"));
                                tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Massa"));
                                tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("TipodeResiduo"));
                                tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("TipodeMaterial"));
                                tabela.AddCell(dtColetaImpressao.Rows[i].Field<string>("Unidade"));
                            }


