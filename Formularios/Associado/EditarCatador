        //Salvar alterações 
        private void btn_SalvarAlteracoesEditarCatador_Click(object sender, EventArgs e)
        {
            string format = "dd/MM/yyyy";
            string DatadeNascimento = txt_DatadeNascimentoEditarCatador.Text;
            DateTime dateTime;
            if (DateTime.TryParseExact(DatadeNascimento, format, CultureInfo.InvariantCulture, DateTimeStyles.AllowWhiteSpaces, out dateTime))
            {
                string DatadeMatricula = txt_DatadeMatriculaEditarCatador.Text;
                if (DateTime.TryParseExact(DatadeNascimento, format, CultureInfo.InvariantCulture, DateTimeStyles.AllowWhiteSpaces, out dateTime))
                {
                    if (txt_NomeCompletoEditarCatador.Text != "")
                    {
                        if (txt_DatadeNascimentoEditarCatador.Text != "")
                        {

                            if (txt_DatadeMatriculaEditarCatador.Text != "")
                            {
                                if (cbb_GeneroEditarCatador.SelectedIndex != -1)
                                {
                                    if (txt_RGEditarCatador.Text != "")
                                    {
                                        if (txt_CPFEditarCatador.Text != "")
                                        {
                                            if (txt_CEPEditarCatador.Text != "")
                                            {
                                                if (cbb_CidadeEditarCatador.SelectedIndex != -1)
                                                {
                                                    if (txt_TelefoneEditarCatador.Text != "")
                                                    {
                                                        if (txt_BairroEditarCatador.Text != "")
                                                        {
                                                            if (txt_RuaEditarCatador.Text != "")
                                                            {
                                                                if (txt_NISEditarCatador.Text != "")
                                                                {

                                                                    if (txt_Funcao.Text != "")
                                                                    {
                                                                        Criptografia cryptography = new Criptografia(System.Configuration.ConfigurationManager.AppSettings[""]);
                                                                        string BairroCrip = cryptography.Encriptar(txt_BairroEditarCatador.Text);
                                                                        string RuaCrip = cryptography.Encriptar(txt_RuaEditarCatador.Text);
                                                                        string DatadeNascimentoCrip = cryptography.Encriptar(txt_DatadeNascimentoEditarCatador.Text);
                                                                        string DatadeMatriculaCrip = cryptography.Encriptar(txt_DatadeMatriculaEditarCatador.Text);
                                                                        string RGCrip = cryptography.Encriptar(txt_RGEditarCatador.Text);
                                                                        string CPFCrip = cryptography.Encriptar(txt_CPFEditarCatador.Text);
                                                                        string NISCrip = cryptography.Encriptar(txt_NISEditarCatador.Text);
                                                                        string GeneroCrip = cryptography.Encriptar(cbb_GeneroEditarCatador.Text);
                                                                        string TelefoneCrip = cryptography.Encriptar(txt_TelefoneEditarCatador.Text);
                                                                        string CEPCrip = cryptography.Encriptar(txt_CEPEditarCatador.Text);
                                                                        string AdministradorCrip = cryptography.Encriptar(lbl_Usuario.Text);
                                                                        string FuncaoCrip = (txt_Funcao.Text);

                                                                        EditarCatadorClasse CatadorAlterado = new EditarCatadorClasse();
                                                                        CatadorAlterado.NomeCompletoCatadorEditar = txt_NomeCompletoEditarCatador.Text;
                                                                        CatadorAlterado.CidadeCadastroCatadorEditar = cbb_CidadeEditarCatador.Text;
                                                                        CatadorAlterado.BairroCadastroCatadorEditar = BairroCrip;
                                                                        CatadorAlterado.RuaCadastroCatadorEditar = RuaCrip;
                                                                        CatadorAlterado.DatadeNascimentoCatadorEditar = DatadeNascimentoCrip;
                                                                        CatadorAlterado.DatadeMatriculaoCatadorEditar = DatadeMatriculaCrip;
                                                                        CatadorAlterado.RGCadastroCatadorEditar = RGCrip;
                                                                        CatadorAlterado.CPFCadastroCatadorEditar = CPFCrip;
                                                                        CatadorAlterado.NISCadastroCatadorEditar = NISCrip;
                                                                        CatadorAlterado.GeneroCadastroCatadorEditar = GeneroCrip;
                                                                        CatadorAlterado.TelefoneCadastroCatadorEditar = TelefoneCrip;
                                                                        CatadorAlterado.CEPCadastroCatadorEditar = CEPCrip;
                                                                        CatadorAlterado.UnidadeCatadorEditar = lbl_Unidade.Text;
                                                                        CatadorAlterado.UsuarioCatadorEditar = AdministradorCrip;
                                                                        CatadorAlterado.AuxilioCatadorEditar = txt_RecebeAuxilioEditarCadastroCatador.Text;
                                                                        CatadorAlterado.DataCatadorEditar = DateTime.Parse(DateTime.Now.ToString());
                                                                        CatadorAlterado.FuncaoCatadorEditar = FuncaoCrip;
                                                                        CatadorAlterado.NomedoServidor = lbl_NomedoServidor.Text;
                                                                        CatadorAlterado.MatriculaCatadorEditar = txt_MatriculaCatadorCatadorEditar.Text;
                                                                        ConexaoBancodeDados.CatadorEdite(CatadorAlterado);
                                                                        this.Close();
                                                                    }
                                                                    else
                                                                    {
                                                                        MessageBox.Show("Escolha uma função");
                                                                        return;

                                                                    }
                                                                }

                                                                else
                                                                {

                                                                    txt_NISEditarCatador.Text = "0000000000";

                                                                }
                                                            }
                                                            else
                                                            {

                                                                MessageBox.Show("O campo Rua e Número não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                                                                return;

                                                            }
                                                        }
                                                        else
                                                        {

                                                            MessageBox.Show("O campo Bairro não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                                                            return;

                                                        }
                                                    }
                                                    else
                                                    {

                                                        MessageBox.Show("O campo Telefone não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                                                        return;

                                                    }
                                                }
                                                else
                                                {

                                                    MessageBox.Show("O campo Cidade não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                                                    return;

                                                }
                                            }
                                            else
                                            {

                                                MessageBox.Show("O campo CEP não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                                                return;

                                            }


                                        }
                                        else
                                        {

                                            MessageBox.Show("O campo CPF não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                                            return;

                                        }
                                    }
                                    else
                                    {

                                        MessageBox.Show("O campo RG não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                                        return;

                                    }
                                }

                                else
                                {

                                    MessageBox.Show("O campo Gênero não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                                    return;

                                }
                            }

                            else
                            {

                                MessageBox.Show("O campo Data de Matrícula não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                                return;

                            }
                        }
                        else
                        {

                            MessageBox.Show("O campo Data de Nascimento não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                            return;

                        }
                    }
                    else
                    {

                        MessageBox.Show("O campo Nome Completo não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                        return;

                    }
                }
                else
                {
                    MessageBox.Show("Digite um formato válido de data de matrícula.");
                    return;

                }
            }
            else
            {
                MessageBox.Show("Digite um formato válido de data de nascimento.");
                return;

            }
        }

        //Carregamento das cidades; Marcação da checkbox Auxílio Catador;
        //Verificar se ocorreu alteração no banco de dados desde a inserção.   
        private void EditarCatador_Load(object sender, EventArgs e)
         {
            string NomedoServidor = lbl_NomedoServidor.Text;
            string vqueryCidade = @"SELECT * FROM CIDADES ORDER BY ID_Cidade ASC ;";
            cbb_CidadeEditarCatador.Items.Clear();
            cbb_CidadeEditarCatador.DataSource = ConexaoBancodeDados.dql(vqueryCidade, NomedoServidor);
            cbb_CidadeEditarCatador.DisplayMember = "Cidade";
            cbb_CidadeEditarCatador.ValueMember = "ID_Cidade";
           
            if(txt_RecebeAuxilioEditarCadastroCatador.Text == "Recebe")
            {
                chb_RecebeAuxílioCatadorCadastroCatador.Checked = true;

            }

            SqlConnection Conectar = new SqlConnection("Data Source=DESKTOP-" + NomedoServidor + "\\SQLEXPRESS;Initial Catalog=SGRRC1;Integrated Security=True");
            Conectar.Open();


            string Matricula = txt_MatriculaCatadorCatadorEditar.Text;
            SqlCommand cmd2 = new SqlCommand("select [UsuarioEditadoCatador] from CATADORES WHERE [Matrícula] = '" + Matricula + "'", Conectar);
            var Matricula2 = cmd2.ExecuteScalar();

            SqlCommand cmd3 = new SqlCommand("select [DataEditadoCatador] from CATADORES WHERE [Matrícula] = '" + Matricula + "'", Conectar);
            var DataEditado = cmd3.ExecuteScalar();

            Criptografia cryptography = new Criptografia(System.Configuration.ConfigurationManager.AppSettings[""]);


            if (Matricula2 != null)
            {

                string Matricula3 = Matricula2.ToString();

                if (Matricula3 != "")
                {
                    lbl_EditadoporUltimo.Visible = true;
                    label3.Visible = true;
                    lbl_UsuarioEditado.Visible = true;
                    string Matricula3Decrip = cryptography.Decriptar(Matricula3);
                    lbl_UsuarioEditado.Text = Matricula3Decrip;
                    lbl_DataEditada.Visible = true;
                    string DataEditado2 = DataEditado.ToString();
                    lbl_DataEditada.Text = DataEditado2;
                }
      
            }
            Conectar.Close();
        }

        //Somente números e '/' são aceitos.
        private void txt_DatadeNascimentoEditarCatador_KeyPress(object sender, KeyPressEventArgs e)
        {
            if ((!char.IsNumber(e.KeyChar)) && !char.IsControl(e.KeyChar) && e.KeyChar != '/')
            {
                e.Handled = true;
            }
        }

        //Somente números, '(' e ')' são aceitos
        private void txt_TelefoneEditarCatador_KeyPress(object sender, KeyPressEventArgs e)
        {
            if ((!char.IsNumber(e.KeyChar)) && !char.IsControl(e.KeyChar) && e.KeyChar != '(' && e.KeyChar != ')' && e.KeyChar != '-')
            {
                e.Handled = true;
            }
        }

       //Somente números e '/' são aceitos.
        private void txt_DatadeMatriculaEditarCatador_KeyPress(object sender, KeyPressEventArgs e)
        {

            if ((!char.IsNumber(e.KeyChar)) && !char.IsControl(e.KeyChar) && e.KeyChar != '/')
            {
                e.Handled = true;
            }

        }

        
        //Alteração no checkbox de Auxílio Catador
        private void chb_RecebeAuxílioCatadorCadastroCatador_CheckedChanged(object sender, EventArgs e)
        {
            if(chb_RecebeAuxílioCatadorCadastroCatador.Checked == false)
            {

                txt_RecebeAuxilioEditarCadastroCatador.Text = "Não Recebe";
            }
            else if(chb_RecebeAuxílioCatadorCadastroCatador.Checked == true)
            {
                txt_RecebeAuxilioEditarCadastroCatador.Text = "Recebe";

            }
        }

        //Fechar o formulário
        private void btn_CancelarCadastrarCatador_Click(object sender, EventArgs e)
        {
            this.Close();
        }

        //Somente letras são aceitas
        private void txt_NomeCompletoEditarCatador_KeyPress(object sender, KeyPressEventArgs e)
        {
            if ((!char.IsLetter(e.KeyChar)) && !char.IsControl(e.KeyChar) && Convert.ToInt32(e.KeyChar) != 8 && Convert.ToInt32(e.KeyChar) != 32)
            {
                e.Handled = true;
            }
        }

        //Carregamento das funções.
        private void cbb_Funcao_Click(object sender, EventArgs e)
        {
            string NomedoServidor = lbl_NomedoServidor.Text;
            string vqueryFuncao = @"SELECT * FROM FUNCOES where [Unidade] = '" + lbl_Unidade.Text + "' ORDER BY Funcao ASC ; ";
            cbb_Funcao.DataSource = ConexaoBancodeDados.dql(vqueryFuncao, NomedoServidor);
            cbb_Funcao.DisplayMember = "Funcao";
        }

        //Alterações da textbox de funções
        private void cbb_Funcao_SelectedIndexChanged(object sender, EventArgs e)
        {
            txt_Funcao.Text = cbb_Funcao.Text;
        }

       
    }
}

