            this.Close();
        }

        private void CadastroFilho_KeyDown(object sender, KeyEventArgs e)
        {

        }

        private void txt_DatadeNascimentoFilhoCadastro_KeyPress(object sender, KeyPressEventArgs e)
        {
            if ((!char.IsNumber(e.KeyChar)) && !char.IsControl(e.KeyChar) && e.KeyChar != '/')
            {
                e.Handled = true;

            }
        }
    }
