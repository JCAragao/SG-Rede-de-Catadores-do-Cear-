        public CadastroFilho()
        {
            InitializeComponent();
        }


        private void button2_Click(object sender, EventArgs e)
        {
            CadastroFilho FilhoCadastro = new CadastroFilho();
            FilhoCadastro.Show();
        }

        private void button1_Click(object sender, EventArgs e)
        {
            string format = "dd/MM/yyyy";
            string DatadeNascimento = txt_DatadeNascimentoFilhoCadastro.Text;
            DateTime dateTime;
            if (DateTime.TryParseExact(DatadeNascimento, format, CultureInfo.InvariantCulture, DateTimeStyles.AllowWhiteSpaces, out dateTime))
            {

                if (txt_DatadeNascimentoFilhoCadastro.Text != "")
                {
                    if (cbb_GeneroFilhoCadastro.SelectedIndex != -1)
                    {
                        Criptografia cryptography = new Criptografia(System.Configuration.ConfigurationManager.AppSettings[""]);
                        string GeneroCrip = cbb_GeneroFilhoCadastro.Text;
                        string DataCrip = cryptography.Encriptar(txt_DatadeNascimentoFilhoCadastro.Text);

                        CadastroFilhoClasse filhoCadastroClasse = new CadastroFilhoClasse();
                        filhoCadastroClasse.PaiMae = txt_MaePaiCadastroFilho.Text;
                        filhoCadastroClasse.GeneroCadastroFilho = GeneroCrip;
                        filhoCadastroClasse.DatadeNascimentoCadastroFilho = DataCrip;
                        filhoCadastroClasse.CPFPaiMae = lbl_CPF.Text;
                        filhoCadastroClasse.Nomedoservidor = lbl_NomedoServidor.Text; ;
                        ConexaoBancodeDados.NovoFilho(filhoCadastroClasse);
                        this.Close();
                    }
                    else
                    {
                        MessageBox.Show("O campo Gênero não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                        return;
                    }
                }
                else
                {

                    MessageBox.Show("O campo Data de Nascimento não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                    return;

                }
            }
            else
            {
                MessageBox.Show("Digite um formato válido de data de nascimento. Ex: 07/02/1998");
                return;
            }

        }

        private void CadastroFilho_Load(object sender, EventArgs e)
        {


        }

