        private void btn_EditarCliente_Click(object sender, EventArgs e)
        {
            string format = "dd/MM/yyyy";
            string DatadeCadastro= txt_DatadeCadastroEditarCliente.Text;
            DateTime dateTime;
            if (DateTime.TryParseExact(DatadeCadastro, format, CultureInfo.InvariantCulture, DateTimeStyles.AllowWhiteSpaces, out dateTime))
            {
                if (txt_NomeFantasiaEditarCliente.Text != "")
                {
                    if (txt_DatadeCadastroEditarCliente.Text != "")
                    {

                        if (cbb_RegimedeTributacaoEditarCliente.SelectedIndex != -1)
                        {
                            if (txt_CNPJEditarCliente.Text != "")
                            {
                                if (txt_CEPEditarCliente.Text != "")
                                {
                                    if (cbb_CidadeEditarCliente.SelectedIndex != -1)
                                    {
                                        if (txt_TelefoneEditarCliente.Text != "")
                                        {
                                            if (txt_BairroEditarCliente.Text != "")
                                            {
                                                if (txt_RuaEditarCliente.Text != "")
                                                {

                                                    
                                                   {

                                                        Criptografia cryptography = new Criptografia(System.Configuration.ConfigurationManager.AppSettings[""]);
                                                        string NomeClienteCrip = txt_NomeFantasiaEditarCliente.Text;
                                                        string CidadeCrip = cryptography.Encriptar(cbb_CidadeEditarCliente.Text);
                                                        string BairroCrip = cryptography.Encriptar(txt_BairroEditarCliente.Text);
                                                        string RuaCrip = cryptography.Encriptar(txt_RuaEditarCliente.Text);
                                                        string DataCadastroCrip = cryptography.Encriptar(txt_DatadeCadastroEditarCliente.Text);
                                                        string CNPJCrip = cryptography.Encriptar(txt_CNPJEditarCliente.Text);
                                                        string TelefoneCrip = cryptography.Encriptar(txt_TelefoneEditarCliente.Text);
                                                        string CEPCrip = cryptography.Encriptar(txt_CEPEditarCliente.Text);
                                                        string emailCrip = cryptography.Encriptar(txt_emailEditarCliente.Text);




                                                        ClienteEditadoClasse ClienteEditado = new ClienteEditadoClasse();
                                                        ClienteEditado.NomeFantasiaEditarCliente = NomeClienteCrip;
                                                        ClienteEditado.CidadeEditarCliente = CidadeCrip;
                                                        ClienteEditado.BairroEditarCliente = BairroCrip;
                                                        ClienteEditado.RuaEditarCliente = RuaCrip;
                                                        ClienteEditado.DatadeCadastroEditarCliente = DateTime.Parse(DateTime.Now.ToString());
                                                        ClienteEditado.CNPJEditarCliente = CNPJCrip;
                                                        ClienteEditado.RegimedeTributacaoEditarCliente = cbb_RegimedeTributacaoEditarCliente.Text;
                                                        ClienteEditado.TelefoneEditarCliente = TelefoneCrip;
                                                        ClienteEditado.CEPEditarCliente = CEPCrip;
                                                        ClienteEditado.RazaoSocialEditarCliente = txt_RazaoSocialEditarCliente.Text;
                                                        ClienteEditado.emailEditarCliente = emailCrip;
                                                        ClienteEditado.IDCliente = Convert.ToInt32(txt_MatriculaCliente.Text);
                                                        ClienteEditado.UsuarioEditarCliente = lbl_Unidade.Text;
                                                        ClienteEditado.NomedoServidor = lbl_NomedoServidor.Text;

                                                        ConexaoBancodeDados.ClienteEditar(ClienteEditado);
                                                        this.Close();

                                                    }

                                                }
                                                else
                                                {

                                                    MessageBox.Show("O campo Rua e Número não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                                                    return;

                                                }
                                            }
                                            else
                                            {

                                                MessageBox.Show("O campo Bairro não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                                                return;

                                            }
                                        }
                                        else
                                        {

                                            MessageBox.Show("O campo Telefone não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                                            return;

                                        }
                                    }
                                    else
                                    {

                                        MessageBox.Show("O campo Cidade não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                                        return;

                                    }
                                }
                                else
                                {

                                    MessageBox.Show("O campo CEP não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                                    return;

                                }


                            }
                            else
                            {

                                MessageBox.Show("O campo CNPJ não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                                return;

                            }

                        }


                        else
                        {

                            MessageBox.Show("O campo Regime de Tributação não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                            return;

                        }



                    }
                    else
                    {

                        MessageBox.Show("O campo Data de Cadastro não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                        return;

                    }
                }
                else
                {

                    MessageBox.Show("O campo Nome  Fantasia  não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                    return;

                }
            }
            else
            {
                MessageBox.Show("Digite um formado de data de cadastro válido.");
                return;

            }

        }

        //Verificar se ocorreu alteração nas informações do banco de dados.
        private void ClienteEditar_Load(object sender, EventArgs e)
        {

            string NomedoServidor = lbl_NomedoServidor.Text;
            SqlConnection Conectar = new SqlConnection(@"Data Source=DESKTOP-" + NomedoServidor + "\\SQLEXPRESS;Initial Catalog=SGRRC1;Integrated Security=True");
            Conectar.Open();


            string Matricula = lbl_IDCliente.Text;
            SqlCommand cmd2 = new SqlCommand("select [UsuarioEditado] from CLIENTES WHERE [ID_Cliente] = '" + Matricula + "'", Conectar);
            var Matricula2 = cmd2.ExecuteScalar();

            SqlCommand cmd3 = new SqlCommand("select [DataEditado] from CLIENTES WHERE [ID_Cliente] = '" + Matricula + "'", Conectar);
            var DataEditado = cmd3.ExecuteScalar();

            if (Matricula2 != null)
            {

                lbl_EditadoporUltimo.Visible = true;
                label3.Visible = true;
                lbl_UsuarioEditado.Visible = true;
                string Matricula3 = Matricula2.ToString();
                lbl_UsuarioEditado.Text = Matricula3;


                lbl_DataEditada.Visible = true;
                string DataEditado2 = DataEditado.ToString();
                lbl_DataEditada.Text = DataEditado2;

            }

            Conectar.Close();
        }

        //Somente números são permitidos.
        private void txt_CNPJEditarCliente_KeyPress(object sender, KeyPressEventArgs e)
        {
            if ((!char.IsNumber(e.KeyChar)) && !char.IsControl(e.KeyChar) && e.KeyChar != '/')
            {
                e.Handled = true;
            }
        }

        //Somente números e '/' são permitidos.
        private void txt_DatadeCadastroEditarCliente_KeyPress(object sender, KeyPressEventArgs e)
        {
            if ((!char.IsNumber(e.KeyChar) && !char.IsControl(e.KeyChar) && e.KeyChar == '/'))
            {
                e.Handled = true;
            }

        }

        //Somente números e '(' ')' são permitidos.
        private void txt_TelefoneEditarCliente_KeyPress(object sender, KeyPressEventArgs e)
        {
            if ((!char.IsNumber(e.KeyChar)) && !char.IsControl(e.KeyChar) && e.KeyChar != '(' && e.KeyChar != ')' && e.KeyChar != '-')
            {
                e.Handled = true;
            }
        }

        //Somente letras são permitidas.
        private void txt_RuaEditarCliente_KeyPress(object sender, KeyPressEventArgs e)
        {
            if ((!char.IsNumber(e.KeyChar)) && (!char.IsLetter(e.KeyChar)) && !char.IsControl(e.KeyChar) && Convert.ToInt32(e.KeyChar) != 8 && Convert.ToInt32(e.KeyChar) != 32 && e.KeyChar == ',')
            {
                e.Handled = true;
            }
        }

        //Somente letras são permitidas.
        private void txt_BairroEditarCliente_KeyPress(object sender, KeyPressEventArgs e)
        {
            if ((!char.IsNumber(e.KeyChar)) && (!char.IsLetter(e.KeyChar)) && !char.IsControl(e.KeyChar))
            {
                e.Handled = true;
            }
        }

        //Somente números são aceitos.
        private void txt_CEPEditarCliente_KeyPress(object sender, KeyPressEventArgs e)
        {
            if ((!char.IsNumber(e.KeyChar)) && !char.IsControl(e.KeyChar))
            {
                e.Handled = true;
            }
        }

        //Carregamento das cidades
        private void cbb_CidadeEditarCliente_Click(object sender, EventArgs e)
        {
            string NomedoServidor = lbl_NomedoServidor.Text;
            string vqueryCidade = @"SELECT * FROM CIDADES ORDER BY ID_Cidade ASC ;";
            cbb_CidadeEditarCliente.DataSource = ConexaoBancodeDados.dql(vqueryCidade, NomedoServidor);
            cbb_CidadeEditarCliente.DisplayMember = "Cidade";
            cbb_CidadeEditarCliente.ValueMember = "ID_Cidade";
        }
        
        //Fechar o formulário
        private void btn_CancelarCadastrarCliente_Click(object sender, EventArgs e)
        {
            this.Close();
        }
    }
}
