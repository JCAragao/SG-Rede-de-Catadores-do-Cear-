        private System.Windows.Forms.ComboBox cbb_Papel;
        private System.Windows.Forms.ComboBox cbb_Vidro;
        private System.Windows.Forms.RadioButton rdb_Metal;
        private System.Windows.Forms.Label label3;
        private System.Windows.Forms.TextBox txt_GrupoTipodeMaterial;
        public System.Windows.Forms.ComboBox cbb_OrganicoTabeladePrecos;
        private System.Windows.Forms.TextBox txt_TipodeMaterial;
        public System.Windows.Forms.Label lbl_NomedoServidor;
    }
}

using Org.BouncyCastle.Asn1.X509;
using System;
using System.Collections.Generic;
using System.ComponentModel;
using System.Data;
using System.Data.SqlClient;
using System.Drawing;
using System.Linq;
