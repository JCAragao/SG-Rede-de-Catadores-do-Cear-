namespace Sistema_de_Gerenciamento_da_Rede_de_Catadores_do_Ceará__Versão_Teste_
{
    public partial class TabeladePrecos : Form
    {
        public TabeladePrecos()
        {
            InitializeComponent();
        }

        //Carregamento das comboboxes de tipo de material
        private void TabeladePrecos_Load(object sender, EventArgs e)
        {
            string Nomedoservidor = lbl_NomedoServidor.Text;
            Criptografia cryptography = new Criptografia(System.Configuration.ConfigurationManager.AppSettings[""]);
            string Associacao = cryptography.Encriptar(txt_AssociacaoTabelaPReco.Text);


            string Metal = cryptography.Encriptar(rdb_Metal.Text);

            string vqueryMetal = @"select Material from METERIAIS where Grupo = '" + Metal + "' AND Unidade = '" + Associacao + "' ";

            cbb_Metal.DataSource = ConexaoBancodeDados.dql(vqueryMetal, Nomedoservidor);
            cbb_Metal.DisplayMember = "Material";

            string Papel = cryptography.Encriptar(rdb_Papel.Text);

            string vqueryPapel = @"select Material from METERIAIS where Grupo = '" + Papel + "' AND Unidade = '" + Associacao + "' ";

            cbb_Papel.DataSource = ConexaoBancodeDados.dql(vqueryPapel, Nomedoservidor);
            cbb_Papel.DisplayMember = "Material";

            string Plastico = cryptography.Encriptar(rdb_Plastico.Text);

            string vqueryPlastico = @"select Material from METERIAIS where Grupo = '" + Plastico + "' AND Unidade = '" + Associacao + "' ";

            cbb_Plastico.DataSource = ConexaoBancodeDados.dql(vqueryPlastico, Nomedoservidor);
            cbb_Plastico.DisplayMember = "Material";

            string Organico = cryptography.Encriptar(rdb_OrganicoTabeladePrecos.Text);

            string vqueryOrganico = @"select Material from METERIAIS where Grupo = '" + Organico + "' AND Unidade = '" + Associacao + "' ";

            cbb_OrganicoTabeladePrecos.DataSource = ConexaoBancodeDados.dql(vqueryOrganico, Nomedoservidor);
            cbb_OrganicoTabeladePrecos.DisplayMember = "Material";

            string Vidro = cryptography.Encriptar(rdb_Vidro.Text);

            string vqueryVidro = @"select Material from METERIAIS where Grupo = '" + Vidro + "' AND Unidade = '" + Associacao + "' ";

            cbb_Vidro.DataSource = ConexaoBancodeDados.dql(vqueryVidro, Nomedoservidor);
            cbb_Vidro.DisplayMember = "Material";

        }

        //Salvar alterações no preço
        private void btn_SalvarAlteracoesTabeladePrecos_Click(object sender, EventArgs e)
        {
            TabeladePrecosClasse PrecoAlterado = new TabeladePrecosClasse();
            Criptografia cryptography = new Criptografia(System.Configuration.ConfigurationManager.AppSettings[""]);


            PrecoAlterado.Associacao = cryptography.Encriptar(txt_AssociacaoTabelaPReco.Text);
            PrecoAlterado.Preco =cryptography.Encriptar(txt_PrecoMaterial.Text);
            PrecoAlterado.TipodeMaterial = cbb_MaterialEditar.Text;
            PrecoAlterado.Grupo = txt_GrupoTipodeMaterial.Text;
            PrecoAlterado.NomedoServidor = lbl_NomedoServidor.Text;
            ConexaoBancodeDados.EditarPreco(PrecoAlterado);
            this.Close();
        }

     
        //Ativar a edição de texto
        private void txt_PrecoMaterial_DoubleClick(object sender, EventArgs e)
        {
            txt_PrecoMaterial.ReadOnly = false;
        }

        //Adicionar novo material e preço à tabela de materiais
        private void btn_AdicionarNovoMaterial_Click(object sender, EventArgs e)
        {
            if (txt_GrupoTipodeMaterial.Text != "")
            {
                if (txt_NovoMaterial.Text != "")
                {
                    if (txt_PrecoNovoMaterial.Text != "")
                    {
                        string NomedoServidor = lbl_NomedoServidor.Text;
                        DataTable dt = new DataTable();
                        string Associacao = txt_AssociacaoTabelaPReco.Text;
                        string Material = txt_NovoMaterial.Text;
                        string sql = "select*from METERIAIS where [Material] = '" + Material + "' and [Unidade] ='" + Associacao + "'";
                        dt = ConexaoBancodeDados.consulta(sql, NomedoServidor);

                        if (dt.Rows.Count == 0)
                        {
                            Criptografia cryptography = new Criptografia(System.Configuration.ConfigurationManager.AppSettings[""]);
                            string AssociacaoCrip = cryptography.Encriptar(txt_AssociacaoTabelaPReco.Text);
                            string PrecoCrip = cryptography.Encriptar(txt_PrecoNovoMaterial.Text);
                            string GrupoCrip = cryptography.Encriptar(txt_GrupoTipodeMaterial.Text);

                            TabeladePrecosAdicionar Adicionarpreco = new TabeladePrecosAdicionar();
                            Adicionarpreco.AssociacaoAdicionar = AssociacaoCrip;
                            Adicionarpreco.PrecoAdicionar = PrecoCrip;
                            Adicionarpreco.TipodeMaterialAdicionar = txt_NovoMaterial.Text;
                            Adicionarpreco.Grupo = GrupoCrip;
                            Adicionarpreco.NomedoServidor = lbl_NomedoServidor.Text;
                            ConexaoBancodeDados.AdicionarMaterial(Adicionarpreco);
                            this.Close();
                        }
                        else
                        {
                            MessageBox.Show("Material já cadastrado.");
                            return;

                        }
                    }
                    else
                    {
                        MessageBox.Show("Digite um preço.");
                        return;

                    }
                }
                else
                {
                    MessageBox.Show("Digite um material.");
                    return;

                }
            }
            else
            {
                MessageBox.Show("escolha um grupo de recicláveis.");
                return;

            }
        }

     
        //Excluir item
        private void btn_Excluir_Click(object sender, EventArgs e)
        {
            if (lbl_IDMaterial.Text == "Material")
            {
                MessageBox.Show("Para escolher um material pressione a tecla ENTRA");
                return;

            }
            else
            {

                string NomedoServidor = lbl_NomedoServidor.Text;
                SqlConnection Conectar = new SqlConnection("Data Source=DESKTOP-" + NomedoServidor + "\\SQLEXPRESS;Initial Catalog=SGRRC1;Integrated Security=True");
                Conectar.Open();
                SqlCommand cmd = new SqlCommand(@"DELETE FROM METERIAIS WHERE [ID_Material] ='" + lbl_IDMaterial.Text + "';", Conectar);
                cmd.ExecuteScalar();
                Conectar.Close();
                MessageBox.Show("Material excluído com sucesso!");
                this.Close();
            }
        }

        //Fechar o formulário
        private void button1_Click(object sender, EventArgs e)
        {
            this.Close();
        }


        //Eventos de marcação do radiobutton Metal
        private void rdb_Metal_CheckedChanged(object sender, EventArgs e)
        {
            txt_GrupoTipodeMaterial.Text = rdb_Metal.Text;

            cbb_Metal.Visible = true;
            cbb_Papel.Text = "";
            cbb_Plastico.Text = "";
            cbb_Vidro.Text = "";

            cbb_Papel.Visible = false;
            cbb_Plastico.Visible = false;
            cbb_Vidro.Visible = false;

            cbb_OrganicoTabeladePrecos.Text = "";
            cbb_OrganicoTabeladePrecos.Visible = false;
        }

        //Eventos de marcação do radiobutton papel
        private void rdb_Papel_CheckedChanged(object sender, EventArgs e)
        {
            txt_GrupoTipodeMaterial.Text = rdb_Papel.Text;

            cbb_Papel.Visible = true;

            cbb_Metal.Text = "";
            cbb_Plastico.Text = "";
            cbb_Vidro.Text = "";
            cbb_OrganicoTabeladePrecos.Text = "";


            cbb_Metal.Visible = false;
            cbb_Vidro.Visible = false;
            cbb_Plastico.Visible = false;
            cbb_OrganicoTabeladePrecos.Visible = false;
        }

        //Eventos de marcação do radiobutton plástico
        private void rdbPlastico__CheckedChanged(object sender, EventArgs e)
        {
            txt_GrupoTipodeMaterial.Text = rdb_Plastico.Text;

            cbb_Metal.Text = "";
            cbb_Papel.Text = "";
            cbb_Vidro.Text = "";
            cbb_OrganicoTabeladePrecos.Text = "";

            cbb_Plastico.Visible = true;
            cbb_Metal.Visible = false;
            cbb_Papel.Visible = false;
            cbb_Vidro.Visible = false;
            cbb_OrganicoTabeladePrecos.Visible = false;

        }

        //Eventos de marcação do radiobutton vidro
        private void rdb_Vidro_CheckedChanged(object sender, EventArgs e)
        {
            txt_GrupoTipodeMaterial.Text = rdb_Vidro.Text;

            cbb_OrganicoTabeladePrecos.Text = "";
            cbb_OrganicoTabeladePrecos.Visible = false;
            cbb_Vidro.Visible = true;
            cbb_Metal.Text = "";
            cbb_Papel.Text = "";
            cbb_Plastico.Text = "";

            cbb_Metal.Visible = false;
            cbb_Papel.Visible = false;
            cbb_Plastico.Visible = false;
        }

        //eventos de marcação do radiobutton orgânico
        private void rdb_OrganicoTabeladePrecos_CheckedChanged(object sender, EventArgs e)
        {
            txt_GrupoTipodeMaterial.Text = rdb_OrganicoTabeladePrecos.Text;

            cbb_Metal.Text = "";
            cbb_Papel.Text = "";
            cbb_Vidro.Text = "";
            cbb_Plastico.Text = "";

            cbb_Plastico.Visible = false;
            cbb_Metal.Visible = false;
            cbb_Papel.Visible = false;
            cbb_Vidro.Visible = false;
            cbb_OrganicoTabeladePrecos.Visible = true;

        }

        //Eventos de seleção da combobox Orgânico
        private void cbb_OrganicoTabeladePrecos_SelectedIndexChanged(object sender, EventArgs e)
        {
            cbb_MaterialEditar.Items.Clear();
            cbb_MaterialEditar.Items.Add(cbb_OrganicoTabeladePrecos.Text);
            cbb_MaterialEditar.SelectedIndex = 0;
        }

        //Eventos de seleção da combobox Metal
        private void cbb_Metal_SelectedIndexChanged(object sender, EventArgs e)
        {
            cbb_MaterialEditar.Items.Clear();
            cbb_MaterialEditar.Items.Add(cbb_Metal.Text);
            cbb_MaterialEditar.SelectedIndex = 0;
        }

        //Eventos de seleção da combobox Plástico
        private void cbb_Plastico_SelectedIndexChanged(object sender, EventArgs e)
        {
            cbb_MaterialEditar.Items.Clear();
            cbb_MaterialEditar.Items.Add(cbb_Plastico.Text);
            cbb_MaterialEditar.SelectedIndex = 0;

        }

        //Eventos de seleção da combobox Papel
        private void cbb_Papel_SelectedIndexChanged(object sender, EventArgs e)
        {
            cbb_MaterialEditar.Items.Clear();
            cbb_MaterialEditar.Items.Add(cbb_Papel.Text);
            cbb_MaterialEditar.SelectedIndex = 0;

        }

        //Eventos de seleção da comobobox Vidro
        private void cbb_Vidro_SelectedIndexChanged(object sender, EventArgs e)
        {
            cbb_MaterialEditar.Items.Clear();
            cbb_MaterialEditar.Items.Add(cbb_Vidro.Text);
            cbb_MaterialEditar.SelectedIndex = 0;

        }

        //Consulta de preço e código do material escolhido
        private void txt_TipodeMaterial_KeyPress(object sender, KeyPressEventArgs e)
        {
            txt_TipodeMaterial.Text = cbb_MaterialEditar.Text;

           
                if (e.KeyChar == 13)
            {
                if (cbb_MaterialEditar.Text != "System.Data.DataRowView")
                {
                    string NomedoServidor = lbl_NomedoServidor.Text;
                    SqlConnection Conectar = new SqlConnection("Data Source=DESKTOP-" + NomedoServidor + "\\SQLEXPRESS;Initial Catalog=SGRRC1;Integrated Security=True");
                    Conectar.Open();
                    Criptografia cryptography = new Criptografia(System.Configuration.ConfigurationManager.AppSettings[""]);
                    string Associacao = cryptography.Encriptar(txt_AssociacaoTabelaPReco.Text);

                    string Material = cbb_MaterialEditar.Text;
                    SqlCommand cmd = new SqlCommand("SELECT Preco FROM METERIAIS WHERE [Material] = '" + Material + "' AND [Unidade] = '" + Associacao + "'", Conectar);
                    SqlCommand cmd2 = new SqlCommand(@" SELECT [ID_Material] FROM METERIAIS WHERE [Unidade] = '" + Associacao + "' AND [Material] = '" + Material + "'", Conectar);
                    var Preco = cmd.ExecuteScalar();
                    var IDMaterial = cmd2.ExecuteScalar();
                    txt_PrecoMaterial.Text = cryptography.Decriptar(Preco.ToString());
                    lbl_IDMaterial.Text = IDMaterial.ToString();
                    Conectar.Close();

                }
                else
                {
                    txt_TipodeMaterial.Text = "";
                    MessageBox.Show("Clique no item escolhido.");
                    return;

                }
            }
        }

       
    }
    
}
