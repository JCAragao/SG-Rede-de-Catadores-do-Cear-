        private void cbb_Pesquisa_SelectedIndexChanged(object sender, EventArgs e)
        {

            if (cbb_Pesquisa.SelectedIndex == 0)
            {
                txt_DatadaVenda.Visible = true;
                txt_Mes.Visible = false;
                txt_Ano.Visible = false;
                cbb_Pesquisa.Visible = false;
                btn_PesquisarporDataVendasGestao.Visible = true;
                button1.Visible = false;
                btn_PesquisarQuinzena.Visible = false;
                btn_PesquisarPorUltimoDiaVendasGestao.Visible = false;
                btn_PesquisarporUltimaSemanaVendasGestao.Visible = false;
                btn_PesquisarUltimoMes.Visible = false;
                btn_PesquisarAnoeMes.Visible = false;
                btn_PesquisarporAno.Visible = false;
            }

            if (cbb_Pesquisa.SelectedIndex == 1)
            {
                txt_DatadaVenda.Visible = false;
                txt_Mes.Visible = true;
                txt_Ano.Visible = true;
                cbb_Pesquisa.Visible = false;
                btn_PesquisarporDataVendasGestao.Visible = false;
                button1.Visible = false;
                btn_PesquisarQuinzena.Visible = false;
                btn_PesquisarPorUltimoDiaVendasGestao.Visible = false;
                btn_PesquisarporUltimaSemanaVendasGestao.Visible = false;
                btn_PesquisarUltimoMes.Visible = false;
                btn_PesquisarAnoeMes.Visible = true;
                btn_PesquisarporAno.Visible = false;

            }


  
            if (cbb_Pesquisa.SelectedIndex == 2)
            {
                txt_DatadaVenda.Visible = false;
                txt_Mes.Visible = false;
                txt_Ano.Visible = true;
                cbb_Pesquisa.Visible = false;
                btn_PesquisarporDataVendasGestao.Visible = false;
                button1.Visible = false;
                btn_PesquisarQuinzena.Visible = false;
                btn_PesquisarPorUltimoDiaVendasGestao.Visible = false;
                btn_PesquisarporUltimaSemanaVendasGestao.Visible = false;
                btn_PesquisarUltimoMes.Visible = false;
                btn_PesquisarAnoeMes.Visible = false;
                btn_PesquisarporAno.Visible = true;
            }


           


            if (cbb_Pesquisa.SelectedIndex == 3)
            {
                txt_DatadaVenda.Visible = false;
                txt_Mes.Visible = false;
                txt_Ano.Visible = false;
                cbb_Pesquisa.Visible = false;
                btn_PesquisarporDataVendasGestao.Visible = false;
                button1.Visible = false;
                btn_PesquisarQuinzena.Visible = false;
                btn_PesquisarPorUltimoDiaVendasGestao.Visible = true;
                btn_PesquisarporUltimaSemanaVendasGestao.Visible = false;
                btn_PesquisarUltimoMes.Visible = false;
                btn_PesquisarAnoeMes.Visible = false;
                btn_PesquisarporAno.Visible = false;


            }


            if (cbb_Pesquisa.SelectedIndex == 4)
            {
                txt_DatadaVenda.Visible = false;
                txt_Mes.Visible = false;
                txt_Ano.Visible = false;
                cbb_Pesquisa.Visible = false;
                btn_PesquisarporDataVendasGestao.Visible = false;
                button1.Visible = false;
                btn_PesquisarQuinzena.Visible = false;
                btn_PesquisarPorUltimoDiaVendasGestao.Visible = false;
                btn_PesquisarporUltimaSemanaVendasGestao.Visible = true;
                btn_PesquisarUltimoMes.Visible = false;
                btn_PesquisarAnoeMes.Visible = false;
                btn_PesquisarporAno.Visible = false;
            }

            if (cbb_Pesquisa.SelectedIndex == 5)
            {
                txt_DatadaVenda.Visible = false;
                txt_Mes.Visible = false;
                txt_Ano.Visible = false;
                cbb_Pesquisa.Visible = false;
                btn_PesquisarporDataVendasGestao.Visible = false;
                button1.Visible = false;
                btn_PesquisarQuinzena.Visible = true;
                btn_PesquisarPorUltimoDiaVendasGestao.Visible = false;
                btn_PesquisarporUltimaSemanaVendasGestao.Visible = false;
                btn_PesquisarUltimoMes.Visible = false;
                btn_PesquisarAnoeMes.Visible = false;
                btn_PesquisarporAno.Visible = false;


            }

            if (cbb_Pesquisa.SelectedIndex == 6)
            {
                txt_DatadaVenda.Visible = false;
                txt_Mes.Visible = false;
                txt_Ano.Visible = false;
                cbb_Pesquisa.Visible = false;
                btn_PesquisarporDataVendasGestao.Visible = false;
                button1.Visible = false;
                btn_PesquisarQuinzena.Visible = false;
                btn_PesquisarPorUltimoDiaVendasGestao.Visible = false;
                btn_PesquisarporUltimaSemanaVendasGestao.Visible = false;
                btn_PesquisarUltimoMes.Visible = true;
                btn_PesquisarAnoeMes.Visible = false;
                btn_PesquisarporAno.Visible = false;
                btn_PesquisarUltimoAno.Visible = false;

            }

            if (cbb_Pesquisa.SelectedIndex == 7)
            {
                txt_DatadaVenda.Visible = false;
                txt_Mes.Visible = false;
                txt_Ano.Visible = false;
                cbb_Pesquisa.Visible = false;
                btn_PesquisarporDataVendasGestao.Visible = false;
                button1.Visible = false;
                btn_PesquisarQuinzena.Visible = false;
                btn_PesquisarPorUltimoDiaVendasGestao.Visible = false;
                btn_PesquisarporUltimaSemanaVendasGestao.Visible = false;
                btn_PesquisarUltimoMes.Visible = false;
                btn_PesquisarAnoeMes.Visible = false;
                btn_PesquisarporAno.Visible = false;
                btn_PesquisarUltimoAno.Visible = false;
                btn_DoisMeses.Visible = true;
                btn_TresMeses.Visible = false;

            }

            if (cbb_Pesquisa.SelectedIndex == 8)
            {
                txt_DatadaVenda.Visible = false;
                txt_Mes.Visible = false;
                txt_Ano.Visible = false;
                cbb_Pesquisa.Visible = false;
                btn_PesquisarporDataVendasGestao.Visible = false;
                button1.Visible = false;
                btn_PesquisarQuinzena.Visible = false;
                btn_PesquisarPorUltimoDiaVendasGestao.Visible = false;
                btn_PesquisarporUltimaSemanaVendasGestao.Visible = false;
                btn_PesquisarUltimoMes.Visible = false;
                btn_PesquisarAnoeMes.Visible = false;
                btn_PesquisarporAno.Visible = false;
                btn_PesquisarUltimoAno.Visible = false;
                btn_DoisMeses.Visible = false;
                btn_TresMeses.Visible = true;

            }

            if (cbb_Pesquisa.SelectedIndex == 9)
            {
                txt_DatadaVenda.Visible = false;
                txt_Mes.Visible = false;
                txt_Ano.Visible = false;
                cbb_Pesquisa.Visible = false;
                btn_PesquisarporDataVendasGestao.Visible = false;
                button1.Visible = false;
                btn_PesquisarQuinzena.Visible = false;
                btn_PesquisarPorUltimoDiaVendasGestao.Visible = false;
                btn_PesquisarporUltimaSemanaVendasGestao.Visible = false;
                btn_PesquisarUltimoMes.Visible = false;
                btn_PesquisarAnoeMes.Visible = false;
                btn_PesquisarporAno.Visible = false;
                btn_PesquisarUltimoAno.Visible = true;
                btn_DoisMeses.Visible = false;
                btn_TresMeses.Visible = false;

            }

        }

        private void btn_PesquisarPorUltimoDiaVendasGestao_Click(object sender, EventArgs e)
        {
            Criptografia cryptography = new Criptografia(System.Configuration.ConfigurationManager.AppSettings[""]);
            string Associacao = cryptography.Encriptar(lbl_AssociacaoGestaoVendas.Text);
            string Nomedoservidor = lbl_NomedoServidor.Text;

            string vqueryVendaDataOntem = @"select*from VENDAS where [Unidade] = '" + Associacao + "' and [Data] between (select dateadd(day, -2, getdate())) and (select dateadd(day, +0, getdate()));";
            dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryVendaDataOntem, Nomedoservidor);

            string vqueryDataVendaOntem = "SELECT [NomedoProdutoPrecoQuilo] as [Material], COUNT([NomedoProdutoPrecoQuilo]) AS [Vendidos Ontem] FROM VENDAS where [Unidade] = '" + Associacao + "' and [Data] between (select dateadd(day, -2, getdate())) and (select dateadd(day, +0, getdate()))  GROUP BY NomedoProdutoPrecoQuilo";
            dgv_MasVendidos.DataSource = ConexaoBancodeDados.dql(vqueryDataVendaOntem, Nomedoservidor);


        }

        private void btn_PesquisarQuinzena_Click(object sender, EventArgs e)
        {
            string Nomedoservidor = lbl_NomedoServidor.Text;
            Criptografia cryptography = new Criptografia(System.Configuration.ConfigurationManager.AppSettings[""]);
            string Associacao = cryptography.Encriptar(lbl_AssociacaoGestaoVendas.Text);

            string vqueryColetaData15 = @"select*from VENDAS where [Unidade] = '" + Associacao + "' and [Data] between (select dateadd(day, -15, getdate())) and (select dateadd(day, +0, getdate()));";
            dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryColetaData15, Nomedoservidor);

            string vqueryDataVenda15 = "SELECT [NomedoProdutoPrecoQuilo] as [Material], COUNT([NomedoProdutoPrecoQuilo]) AS [Vendidos Última Quinzena] FROM VENDAS where [Unidade] = '" + Associacao + "' and [Data] between (select dateadd(day, -15, getdate())) and (select dateadd(day, +0, getdate()))  GROUP BY NomedoProdutoPrecoQuilo";
            dgv_MasVendidos.DataSource = ConexaoBancodeDados.dql(vqueryDataVenda15, Nomedoservidor);

        }

        private void btn_PesquisarUltimoMes_Click(object sender, EventArgs e)
        {
            string Nomedoservidor = lbl_NomedoServidor.Text;
            Criptografia cryptography = new Criptografia(System.Configuration.ConfigurationManager.AppSettings[""]);
            string Associacao = cryptography.Encriptar(lbl_AssociacaoGestaoVendas.Text);

            string vqueryColetaDataMes = @"select*from VENDAS where [Unidade] = '" + Associacao + "' and [Data] between (select dateadd(month, -1, getdate())) and (select dateadd(month, +1, getdate()));";
            dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryColetaDataMes, Nomedoservidor);

            string vqueryDataVendaMes = "SELECT [NomedoProdutoPrecoQuilo] as [Material], COUNT([NomedoProdutoPrecoQuilo]) AS [Vendidos Último Mês] FROM VENDAS where [Unidade] = '" + Associacao + "' and [Data] between (select dateadd(month, -1, getdate())) and (select dateadd(month, +1, getdate()))  GROUP BY NomedoProdutoPrecoQuilo";
            dgv_MasVendidos.DataSource = ConexaoBancodeDados.dql(vqueryDataVendaMes, Nomedoservidor);

        }

        private void btn_PesquisarporUltimaSemanaVendasGestao_Click(object sender, EventArgs e)
        {
            string Nomedoservidor = lbl_NomedoServidor.Text;
            Criptografia cryptography = new Criptografia(System.Configuration.ConfigurationManager.AppSettings[""]);
            string Associacao = cryptography.Encriptar(lbl_AssociacaoGestaoVendas.Text);

            string vqueryColetaData7 = @"select*from VENDAS where [Unidade] = '" + Associacao + "' and [Data] between (select dateadd(week, -1, getdate())) and (select dateadd(week, +0, getdate()));";
            dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryColetaData7, Nomedoservidor);

            string vqueryDataVenda7 = "SELECT [NomedoProdutoPrecoQuilo] as [Material], COUNT([NomedoProdutoPrecoQuilo]) AS [Vendidos Última Semana] FROM VENDAS where [Unidade] = '" + Associacao + "' and [Data] between (select dateadd(week, -1, getdate())) and (select dateadd(week, +1, getdate()))  GROUP BY NomedoProdutoPrecoQuilo";
            dgv_MasVendidos.DataSource = ConexaoBancodeDados.dql(vqueryDataVenda7, Nomedoservidor);
        }

        private void btn_PesquisarUltimoAno_Click(object sender, EventArgs e)
        {
            string Nomedoservidor = lbl_NomedoServidor.Text;
            Criptografia cryptography = new Criptografia(System.Configuration.ConfigurationManager.AppSettings[""]);
            string Associacao = cryptography.Encriptar(lbl_AssociacaoGestaoVendas.Text);

            string vqueryColetaDataAno = @"select*from VENDAS where [Unidade] = '" + Associacao + "' and [Data] between (select dateadd(year, -1, getdate())) and (select dateadd(year, +1, getdate()));";
            dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryColetaDataAno, Nomedoservidor);

            string vqueryDataVendaAno = "SELECT [NomedoProdutoPrecoQuilo] as [Material], COUNT([NomedoProdutoPrecoQuilo]) AS [Vendidos Último Ano] FROM VENDAS where [Unidade] = '" + Associacao + "' and [Data] between (select dateadd(year, -1, getdate())) and (select dateadd(year, +1, getdate()))  GROUP BY NomedoProdutoPrecoQuilo";
            dgv_MasVendidos.DataSource = ConexaoBancodeDados.dql(vqueryDataVendaAno, Nomedoservidor);
        }

        private void btn_PesquisarAnoeMes_Click(object sender, EventArgs e)
        {
            string Nomedoservidor = lbl_NomedoServidor.Text;
            Criptografia cryptography = new Criptografia(System.Configuration.ConfigurationManager.AppSettings[""]);
            string Associacao = cryptography.Encriptar(lbl_AssociacaoGestaoVendas.Text);

            string vqueryColetaDataAnoeMes = @"select*from VENDAS where [Mes] = '" + txt_Mes.Text + "' and [Ano] = '" + txt_Ano.Text + "' and [Unidade] = '" + Associacao + "' order by ID_Venda ASC";
            dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryColetaDataAnoeMes, Nomedoservidor);

            string vqueryDataVendaAnoeMes = "SELECT [NomedoProdutoPrecoQuilo] as [Material], COUNT([NomedoProdutoPrecoQuilo]) AS [Vendidos Último Ano] FROM VENDAS where [Mes] = '" + txt_Mes.Text + "' and [Ano] = '" + txt_Ano.Text + "' and [Unidade] = '" + Associacao + "' GROUP BY NomedoProdutoPrecoQuilo";
            dgv_MasVendidos.DataSource = ConexaoBancodeDados.dql(vqueryDataVendaAnoeMes, Nomedoservidor);
        }

        private void btn_PesquisarporAno_Click(object sender, EventArgs e)
        {
            string Nomedoservidor = lbl_NomedoServidor.Text;
            Criptografia cryptography = new Criptografia(System.Configuration.ConfigurationManager.AppSettings[""]);
            string Associacao = cryptography.Encriptar(lbl_AssociacaoGestaoVendas.Text);

            string vqueryColetaDataAno = @"Select*from VENDAS where [Ano] = '" + txt_Ano.Text + "' and [Unidade] = '" + Associacao + "' order by ID_Venda ASC";
            dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryColetaDataAno, Nomedoservidor);

            string vqueryDataVendaAno = @"SELECT [NomedoProdutoPrecoQuilo] as [Material], COUNT([NomedoProdutoPrecoQuilo]) AS [Vendidos Ano] FROM VENDAS where [Ano] = '" + txt_Ano.Text + "' and [Unidade] = '" + Associacao + "' GROUP BY NomedoProdutoPrecoQuilo";
            dgv_MasVendidos.DataSource = ConexaoBancodeDados.dql(vqueryDataVendaAno, Nomedoservidor);
        }

        private void chb_Pesquisa_CheckedChanged(object sender, EventArgs e)
        {
            if (chb_Pesquisa.Checked == true
    )
            {
                cbb_Pesquisa.Visible = true;
                txt_Ano.Visible = false;
                txt_Mes.Visible = false;
                txt_DatadaVenda.Visible = false;
            }
        }

        private void button1_Click(object sender, EventArgs e)
        {
            MessageBox.Show("Escolha uma forma de pesquisa.");
            return;
        }

        private void btn_DoisMeses_Click(object sender, EventArgs e)
        {
            string Nomedoservidor = lbl_NomedoServidor.Text;
            Criptografia cryptography = new Criptografia(System.Configuration.ConfigurationManager.AppSettings[""]);
            string Associacao = cryptography.Encriptar(lbl_AssociacaoGestaoVendas.Text);

            string vqueryColetaDataMes = @"select*from VENDAS where [Unidade] = '" + Associacao + "' and [Data] between (select dateadd(month, -2, getdate())) and (select dateadd(month, +0, getdate()));";
            dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryColetaDataMes, Nomedoservidor);

            string vqueryDataVendaMes = "SELECT [NomedoProdutoPrecoQuilo] as [Material], COUNT([NomedoProdutoPrecoQuilo]) AS [Vendidos Último Bimestre] FROM VENDAS where [Unidade] = '" + Associacao + "' and [Data] between (select dateadd(month, -2, getdate())) and (select dateadd(month, +0, getdate()))  GROUP BY NomedoProdutoPrecoQuilo";
            dgv_MasVendidos.DataSource = ConexaoBancodeDados.dql(vqueryDataVendaMes, Nomedoservidor);

        }

        private void btn_TresMeses_Click(object sender, EventArgs e)
        {
            string Nomedoservidor = lbl_NomedoServidor.Text;
            Criptografia cryptography = new Criptografia(System.Configuration.ConfigurationManager.AppSettings[""]);
            string Associacao = cryptography.Encriptar(lbl_AssociacaoGestaoVendas.Text);

            string vqueryColetaDataMes = @"select*from VENDAS where [Unidade] = '" + Associacao + "' and [Data] between (select dateadd(month, -3, getdate())) and (select dateadd(month, +0, getdate()));";
            dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryColetaDataMes, Nomedoservidor);

            string vqueryDataVendaMes = "SELECT [NomedoProdutoPrecoQuilo] as [Material], COUNT([NomedoProdutoPrecoQuilo]) AS [Vendidos Último Trimestre] FROM VENDAS where [Unidade] = '" + Associacao + "' and [Data] between (select dateadd(month, -3, getdate())) and (select dateadd(month, +0, getdate()))  GROUP BY NomedoProdutoPrecoQuilo";
            dgv_MasVendidos.DataSource = ConexaoBancodeDados.dql(vqueryDataVendaMes, Nomedoservidor);

        }

