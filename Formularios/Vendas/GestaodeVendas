            txt_DatadaVenda.BackColor = Color.White;


        }


        private void btn_PesquisarporVendedoreDataVendasGestao_Click(object sender, EventArgs e)
        {

        }

        private void btn_PesquisarporDataVendasGestao_Click(object sender, EventArgs e)
        {

            if (txt_DatadaVenda.Text == "")
            {
                MessageBox.Show("Digite uma data de venda.");
                return;

            }
            else
            {
                string format = "dd/MM/yyyy";
                string data = txt_DatadaVenda.Text;
                DateTime dateTime;
                if (DateTime.TryParseExact(data, format, CultureInfo.InvariantCulture, DateTimeStyles.AllowWhiteSpaces, out dateTime))
                {
                    string Nomedoservidor = lbl_NomedoServidor.Text;
                    string DatadaVenda = txt_DatadaVenda.Text;
                    string vqueryDatadaVenda = @"SELECT [ID_Venda], [CNPJ], [NomeFantasia], [Massa], [ValordaCompra], 
                   [TipodePagamento], [NomedoProdutoPrecoQuilo], [DataHora], [ValorRecebido], [Usuario], [Unidade], [ValorProduto] 
                   FROM [VENDAS] WHERE  [Data] = '" + DatadaVenda + "' ORDER BY [NomeFantasia] ASC ;";
                    dgv_ColetasGestao.DataSource = ConexaoBancodeDados.dql(vqueryDatadaVenda, Nomedoservidor);


                    string vqueryColetaData = "SELECT [NomedoProdutoPrecoQuilo], COUNT([NomedoProdutoPrecoQuilo]) AS [Quantidade de vendas] FROM [VENDAS] WHERE [Data] = '" + DatadaVenda + "'  GROUP BY NomedoProdutoPrecoQuilo";
                    dgv_MasVendidos.DataSource = ConexaoBancodeDados.dql(vqueryColetaData, Nomedoservidor);

                }
                else
                {

                    MessageBox.Show("Digite um formato de data válido.");
                    return;
                }

            }
        }


        private void btn_PesquisarPorVendedorVendasGestao_Click(object sender, EventArgs e)
        {


        }



        private void txt_DatadaVenda_KeyPress(object sender, KeyPressEventArgs e)
        {
            if ((!char.IsNumber(e.KeyChar)) && !char.IsControl(e.KeyChar) && e.KeyChar != '/')
            {
                e.Handled = true;

            }
        }

        private void label1_Click(object sender, EventArgs e)
        {

        }

        private void btn_DetalhesGestaoVendas_Click(object sender, EventArgs e)
        {
            if (dgv_ColetasGestao.Rows.Count == 0)
            {
                MessageBox.Show("Nenhuma venda selecionada.");
                return;

            }
            else
            {
                DetalhesVenda VendaDetalhe = new DetalhesVenda();
                VendaDetalhe.ShowDialog();

            }
        }

        private void dgv_ColetasGestao_CellMouseEnter(object sender, DataGridViewCellEventArgs e)
        {



        }

        private void dgv_ColetasGestao_CellMouseClick(object sender, DataGridViewCellMouseEventArgs e)
        {
            if (dgv_ColetasGestao.Rows.Count != 0)
            {
                DetalhesVenda VendaDetalhe = new DetalhesVenda();

                Criptografia cryptography = new Criptografia(System.Configuration.ConfigurationManager.AppSettings[""]);
                string CNPJDecrip = cryptography.Decriptar(this.dgv_ColetasGestao.CurrentRow.Cells[0].Value.ToString());
                string MaterialPrecoDecrip = (this.dgv_ColetasGestao.CurrentRow.Cells[5].Value.ToString());
                string MassaDecrip = cryptography.Decriptar(this.dgv_ColetasGestao.CurrentRow.Cells[2].Value.ToString());
                string ValordoItemDecrip = this.dgv_ColetasGestao.CurrentRow.Cells[11].Value.ToString();
                string ValordaCompraDecrip = this.dgv_ColetasGestao.CurrentRow.Cells[3].Value.ToString();
                string TipodePagamentoDecrip = cryptography.Decriptar(this.dgv_ColetasGestao.CurrentRow.Cells[4].Value.ToString());
                string ValorRecebidoDecrip = cryptography.Decriptar(this.dgv_ColetasGestao.CurrentRow.Cells[7].Value.ToString());

                VendaDetalhe.txt_CNPJ.Text = CNPJDecrip;
                VendaDetalhe.txt_NomeFantasia.Text = this.dgv_ColetasGestao.CurrentRow.Cells[1].Value.ToString();
                VendaDetalhe.txt_Massa.Text = MassaDecrip;
                VendaDetalhe.txt_NomedoProduto.Text = MaterialPrecoDecrip;
                VendaDetalhe.txt_Precoitemselecionado.Text = ValordoItemDecrip;
                VendaDetalhe.txt_ValordaCompra.Text = ValordaCompraDecrip;
                VendaDetalhe.txt_TipodePagamento.Text = TipodePagamentoDecrip;
                VendaDetalhe.txt_ValorRecebido.Text = ValorRecebidoDecrip;
                VendaDetalhe.txt_Troco.Text = (Decimal.Parse(ValordaCompraDecrip) - Decimal.Parse(ValorRecebidoDecrip)).ToString();
                VendaDetalhe.ShowDialog();
            }
            else
            {
                MessageBox.Show("Não há vendas a mostrar.");
                return;

            }
        }

