        private void GestaodeVendas_Load(object sender, EventArgs e)
        {
            // TODO: esta linha de código carrega dados na tabela 'sGRRC1DataSetALFA.VENDAS'. Você pode movê-la ou removê-la conforme necessário.
            // TODO: esta linha de código carrega dados na tabela 'sGRRC1DataSetALFA.VENDAS'. Você pode movê-la ou removê-la conforme necessário.



            btn_PesquisarPorUltimoDiaVendasGestao.Visible = false;
            btn_PesquisarporUltimaSemanaVendasGestao.Visible = false;
            btn_PesquisarporDataVendasGestao.Visible = false;

