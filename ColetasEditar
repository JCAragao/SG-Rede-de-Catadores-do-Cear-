        public void ColetasEditar_Load(object sender, EventArgs e)
        {

            Criptografia cryptography = new Criptografia(System.Configuration.ConfigurationManager.AppSettings[""]);

            string Metal = cryptography.Encriptar(rdb_MetalicoFerrosoColetasEditar.Text);
            string Associacao = cryptography.Encriptar(lbl_UnidadeUsuarioColeta.Text);
            string NomedoServidor = lbl_NomedoServidor.Text;

            string vqueryMetal = @"select Material from METERIAIS where Grupo = '" + Metal + "' AND Unidade = '" + Associacao + "' ";

            cbb_MetalicoFerrosoColetasEditar.DataSource = ConexaoBancodeDados.dql(vqueryMetal, NomedoServidor);
            cbb_MetalicoFerrosoColetasEditar.DisplayMember = "Material";

            string Papel = cryptography.Encriptar(rdb_MetalicoNaoFerrosoAdicionarColeta.Text);

            string vqueryPapel = @"select Material from METERIAIS where Grupo = '" + Papel + "' AND Unidade = '" + Associacao + "' ";

            cbb_MetalicoNaoFerrosoColetasEditar.DataSource = ConexaoBancodeDados.dql(vqueryPapel, NomedoServidor);
            cbb_MetalicoNaoFerrosoColetasEditar.DisplayMember = "Material";

            string Plastico = cryptography.Encriptar(rdb_NaoMetalicoOrganicoColetasEditar.Text);

            string vqueryPlastico = @"select Material from METERIAIS where Grupo = '" + Plastico + "' AND Unidade = '" + Associacao + "' ";

            cbb_NaoMetalicoOrganicoColetasEditar.DataSource = ConexaoBancodeDados.dql(vqueryPlastico, NomedoServidor);
            cbb_NaoMetalicoOrganicoColetasEditar.DisplayMember = "Material";

            string Organico = cryptography.Encriptar(rdb_Organico.Text);

            string vqueryOrganico = @"select Material from METERIAIS where Grupo = '" + Organico + "' AND Unidade = '" + Associacao + "' ";

            cbb_Organico.DataSource = ConexaoBancodeDados.dql(vqueryOrganico, NomedoServidor);
            cbb_Organico.DisplayMember = "Material";

            string Vidro = cryptography.Encriptar(rdb_NaoMetalicoInorganicoColetasEditar.Text);

            string vqueryVidro = @"select Material from METERIAIS where Grupo = '" + Vidro + "' AND Unidade = '" + Associacao + "' ";

            cbb_NaoMetalicoInorganicoColetasEditar.DataSource = ConexaoBancodeDados.dql(vqueryVidro, NomedoServidor);
            cbb_NaoMetalicoInorganicoColetasEditar.DisplayMember = "Material";

            cbb_TipodeMaterialColetasEditarEnviar.Text = cbb_TipodeMaterialColetasEditar.Text;

            SqlConnection Conectar = new SqlConnection("Data Source=DESKTOP-" + NomedoServidor + "\\SQLEXPRESS;Initial Catalog=SGRRC1;Integrated Security=True");
            Conectar.Open();
            string IDCatador = txt_CatadorRegistro.Text;
            SqlCommand cmd = new SqlCommand("select [Catador] from CATADORES WHERE [Matrícula] = '" + IDCatador + "'", Conectar);
            var Catador = cmd.ExecuteScalar();

            string Matricula = txt_RegistrodaColetaColetasEditar.Text;
            SqlCommand cmd2 = new SqlCommand("select [UsuarioEditado] from COLETAS WHERE [ID_Coleta] = '" + Matricula + "'", Conectar);
            var Matricula2 = cmd2.ExecuteScalar();

            SqlCommand cmd3 = new SqlCommand("select [DataEditado] from COLETAS WHERE [ID_Coleta] = '" + Matricula + "'", Conectar);
            var DataEditado = cmd3.ExecuteScalar();

            if (Matricula2 != null)
            {
                lbl_Editadopor.Visible = true;
                lbl_UsuarioEditado.Visible = true;
                string Matricula3 = Matricula2.ToString();
                lbl_UsuarioEditado.Text = Matricula3;

                lbl_DataEditada.Visible = true;
                string DataEditado2 = DataEditado.ToString();
                lbl_DataEditada.Text = DataEditado2;

            }
            Conectar.Close();
            if (Catador != null)
            {

                txt_CatadorNome.Text = Catador.ToString();
                Conectar.Close();
            }
        }

