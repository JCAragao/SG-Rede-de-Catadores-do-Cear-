        private void btn_SalvarAlteraçõesColetasEditar_Click(object sender, EventArgs e)
        {
            //string format = "dd/MM/yyyy";
            //  string DatadaColeta = txt_DatadaColetaColetasEditar.Text;
            // DateTime dateTime;
            //  if (DateTime.TryParseExact(DatadaColeta, format, CultureInfo.InvariantCulture, DateTimeStyles.AllowWhiteSpaces, out dateTime))
            // {

            if (cbb_TipodeMaterialColetasEditarEnviar.Text != "")
            {
                if (cbb_TipodeResiduoColetasEditar.SelectedIndex != -1)
                {
                    if (txt_MassaColetasEditar.Text != "")
                    {
                        if (txt_CatadorRegistro.Text != "")
                        {
                            EditarColetaClasse coletaalterada = new EditarColetaClasse();
                            coletaalterada.CatadorEditarColeta = (txt_CatadorRegistro.Text);
                            coletaalterada.MassaEditarColeta = txt_MassaColetasEditar.Text;
                            coletaalterada.MassaINTEditarColeta = Decimal.Parse(txt_MassaColetasEditar.Text);
                            coletaalterada.TipodeResiduoEditarColeta = cbb_TipodeResiduoColetasEditar.Text;
                            coletaalterada.TipodeMaterialEditarColeta = cbb_TipodeMaterialColetasEditarEnviar.Text;
                            coletaalterada.ID_ColetaEditarColeta = txt_RegistrodaColetaColetasEditar.Text;
                            coletaalterada.UsuarioEditarColeta = lbl_UsuarioColeta.Text;
                            coletaalterada.UnidadeEditarColeta = lbl_UnidadeUsuarioColeta.Text;
                            coletaalterada.DatadaColetaEditarColeta = DateTime.Parse(DateTime.Now.ToString());
                            coletaalterada.NomedoServidor = lbl_NomedoServidor.Text;
                            ConexaoBancodeDados.ColetaEditar(coletaalterada);
                            this.Close();

                           
                        }
                        else
                        {

                            MessageBox.Show("O campo Catador não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                            return;
                        }

                    }
                    else
                    {

                        MessageBox.Show("O campo massa não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                        return;
                    }
                }
                else
                {

                    MessageBox.Show("O campo Tipo de Resíduo não foi preenchido", "campo inválido", MessageBoxButtons.OK);
                    return;
                }
            }
            else
            {
                MessageBox.Show("O campo Tipo de Material não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                return;
            }
        }
    
        

