        private void txt_DatadaColeta_KeyPress(object sender, KeyPressEventArgs e)
        {
            if ((!char.IsNumber(e.KeyChar)) && !char.IsControl(e.KeyChar) && e.KeyChar != '/')
            {
                e.Handled = true;
            }
        }

        private void txt_MassaAdicionarColeta_KeyPress(object sender, KeyPressEventArgs e)
        {
            if ((!char.IsNumber(e.KeyChar)) && !char.IsControl(e.KeyChar) && e.KeyChar != ',' && e.KeyChar != '.')
            {
                e.Handled = true;
            }


        }

        private void rdb_MetalicoFerrosoColetasEditar_CheckedChanged(object sender, EventArgs e)
        {
            cbb_MetalicoFerrosoColetasEditar.Visible = true;
            cbb_MetalicoNaoFerrosoColetasEditar.Text = "";
            cbb_NaoMetalicoOrganicoColetasEditar.Text = "";
            cbb_NaoMetalicoInorganicoColetasEditar.Text = "";
            cbb_Organico.Visible = false;
            cbb_Organico.Text = "";
            cbb_MetalicoNaoFerrosoColetasEditar.Visible = false;
            cbb_NaoMetalicoOrganicoColetasEditar.Visible = false;
            cbb_NaoMetalicoInorganicoColetasEditar.Visible = false;
            cbb_TipodeMaterialColetasEditar.Visible = false;
            cbb_TipodeMaterialColetasEditar.Text = "";
           
        }

        private void cbb_MetalicoFerrosoColetasEditar_SelectedIndexChanged(object sender, EventArgs e)
        {
            cbb_TipodeMaterialColetasEditarEnviar.Items.Clear();
