            private void txt_NomeCompletoCadastrarCatador_KeyPress(object sender, KeyPressEventArgs e)
            {
                if ((!char.IsLetter(e.KeyChar)) && !char.IsControl(e.KeyChar) && Convert.ToInt32(e.KeyChar) != 8 && Convert.ToInt32(e.KeyChar) != 32)
                {
                    e.Handled = true;
                }
            }

            private void txt_BairroCadastroCatador_KeyPress(object sender, KeyPressEventArgs e)
            {
                if ((!char.IsLetter(e.KeyChar)) && !char.IsControl(e.KeyChar) && Convert.ToInt32(e.KeyChar) != 8 && Convert.ToInt32(e.KeyChar) != 32)
                {
                    e.Handled = true;
                }
            }

            private void txt_RuaCadastroCatador_KeyPress(object sender, KeyPressEventArgs e)
            {
                if ((!char.IsNumber(e.KeyChar)) && (!char.IsLetter(e.KeyChar)) && !char.IsControl(e.KeyChar) && Convert.ToInt32(e.KeyChar) != 8 && Convert.ToInt32(e.KeyChar) != 32)
                {
                    e.Handled = true;
                }
            }

        private void btn_AdicionarCliente_Click(object sender, EventArgs e)
        {
            if (txt_RazaoSocial.Text != "")
            {
                string format = "dd/MM/yyyy";
                string DatadeCadastro = txt_DatadeCadastroCadastroCliente.Text;
                DateTime dateTime;
                if (DateTime.TryParseExact(DatadeCadastro, format, CultureInfo.InvariantCulture, DateTimeStyles.AllowWhiteSpaces, out dateTime))
                {

                    if (txt_NomeFantasiaCadastrarCliente.Text != "")
                    {
                        if (txt_DatadeCadastroCadastroCliente.Text != "")
                        {

                            if (cbb_RegimedeTributacaoCadastroCliente.SelectedIndex != -1)
                            {
                                if (txt_CNPJCadastroCliente.Text != "")
                                {
                                    if (txt_CEPCadastroCliente.Text != "")
                                    {
                                        if (cbb_CidadeCadastroCliente.SelectedIndex != -1)
                                        {
                                            if (txt_TelefoneCadastroCliente.Text != "")
                                            {
                                                if (txt_BairroCadastroCliente.Text != "")
                                                {
                                                    if (txt_RuaCadastroCliente.Text != "")
                                                    {

                                                        {

                                                            Criptografia cryptography = new Criptografia(System.Configuration.ConfigurationManager.AppSettings[""]);
                                                            string NomeClienteCrip = txt_NomeFantasiaCadastrarCliente.Text;
                                                            string CidadeCrip = cryptography.Encriptar(cbb_CidadeCadastroCliente.Text);
                                                            string BairroCrip = cryptography.Encriptar(txt_BairroCadastroCliente.Text);
                                                            string RuaCrip = cryptography.Encriptar(txt_RuaCadastroCliente.Text);
                                                            string DataCadastroCrip = cryptography.Encriptar(txt_DatadeCadastroCadastroCliente.Text);
                                                            string CNPJCrip = cryptography.Encriptar(txt_CNPJCadastroCliente.Text);
                                                            string RegimeTributacaoCrip = cbb_RegimedeTributacaoCadastroCliente.Text;
                                                            string TelefoneCrip = cryptography.Encriptar(txt_TelefoneCadastroCliente.Text);
                                                            string CEPCrip = cryptography.Encriptar(txt_CEPCadastroCliente.Text);
                                                            string RazaoSocialCrip = txt_RazaoSocial.Text;
                                                            string emailCrip = cryptography.Encriptar(txt_emailCadastroCliente.Text);



                                                            CadastrarClienteClasse ClienteCadastro = new CadastrarClienteClasse();
                                                            ClienteCadastro.NomeFantasiaCadastroCliente = NomeClienteCrip;
                                                            ClienteCadastro.CidadeCadastroCliente = CidadeCrip;
                                                            ClienteCadastro.BairroCadastroCliente = BairroCrip;
                                                            ClienteCadastro.RuaCadastroCliente = RuaCrip;
                                                            ClienteCadastro.DatadeCadastroCadastroCliente = DataCadastroCrip;
                                                            ClienteCadastro.CNPJCadastroCliente = CNPJCrip;
                                                            ClienteCadastro.RegimedeTributacaoCadastroCliente = RegimeTributacaoCrip;
                                                            ClienteCadastro.TelefoneCadastroCliente = TelefoneCrip;
                                                            ClienteCadastro.CEPCadastroCliente = CEPCrip;
                                                            ClienteCadastro.RazaoSocialCadastroCliente = RazaoSocialCrip;
                                                            ClienteCadastro.emailCadastroCliente = emailCrip;
                                                            ClienteCadastro.UnidadeCadastroCliente = lbl_Unidade.Text;
                                                            ClienteCadastro.UsuarioCadastroCliente = lbl_Usuario.Text;
                                                            ClienteCadastro.NomedoServidor = lbl_NomedoServidor.Text;
                                                            ConexaoBancodeDados.NewCliente(ClienteCadastro);
                                                            this.Close();

                                                        }

                                                    }
                                                    else
                                                    {

                                                        MessageBox.Show("O campo Rua e Número não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                                                        return;

                                                    }
                                                }
                                                else
                                                {

                                                    MessageBox.Show("O campo Bairro não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                                                    return;

                                                }
                                            }
                                            else
                                            {

                                                MessageBox.Show("O campo Telefone não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                                                return;

                                            }
                                        }
                                        else
                                        {

                                            MessageBox.Show("O campo Cidade não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                                            return;

                                        }
                                    }
                                    else
                                    {

                                        MessageBox.Show("O campo CEP não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                                        return;

                                    }


                                }
                                else
                                {

                                    MessageBox.Show("O campo CNPJ não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                                    return;

                                }

                            }


                            else
                            {

                                MessageBox.Show("O campo Regime de Tributação não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                                return;

                            }

                        }
                        else
                        {

                            MessageBox.Show("O campo Data de Cadastro não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                            return;

                        }
                    }
                    else
                    {

                        MessageBox.Show("O campo Nome  Fantasia  não foi preenchido", "Campo inválido", MessageBoxButtons.OK);
                        return;

                    }
                }
                else
                {
                    MessageBox.Show("Digite um valor válido de data de cadastro. Ex: 07/02/1998.");
                    return;

                }
            }
            else
            {

                MessageBox.Show("Digite uma razão social");
                return;
            }
        }

        private void txt_DatadeCadastroCadastroCliente_KeyPress(object sender, KeyPressEventArgs e)
        {
            if ((!char.IsNumber(e.KeyChar)) && !char.IsControl(e.KeyChar) && e.KeyChar != '/')
            {
                e.Handled = true;

            }

        }

        private void btn_CancelarCadastrarCliente_Click(object sender, EventArgs e)
        {
            this.Close();
        }

        
    }

    }

